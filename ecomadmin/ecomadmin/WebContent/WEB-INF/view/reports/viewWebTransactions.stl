<#include "../lib/layout.stl" />
<@dashboardLayout>
<style>
.form-horizontal .control-group {
width:800px;
}
input[readonly] {
width:185px; height:27px;
.formError {
position: absolute;
top: 300px;
left: 300px;
padding-bottom: 55px;
display: block;
z-index: 5000;
margin-top: 5px;
}
.formError .formErrorContent {
width: 100%;
background: #ee0101;
color: #fff;
width: 120px;
font-family: tahoma;
font-size: 11px;
border: 2px solid #ddd;
box-shadow: 0px 0px 6px #000;
-moz-box-shadow: 0px 0px 6px #000;
-webkit-box-shadow: 0px 0px 6px #000;
padding: 4px 10px 4px 10px;
border-radius: 6px;
-moz-border-radius: 6px;
-webkit-border-radius: 6px;
}


input[readonly] {
width:185px; height: 27px;
}
.daterangepicker .ranges .input-mini {
background-color: #eee;
border: 1px solid #ccc;
border-radius: 4px;
color: #555;
display: block;
font-size: 11px;
height: 30px;
line-height: 30px;
vertical-align: middle;
margin: 0 0 10px 0;
padding: 0 6px;
width: 137px;
}

.btn-search {
padding: 0 8px;
line-height: 24px;
border-width: 4px;
font-size: 13px;
}

</style>
<div class="row-fluid">
	<div class="col-xs-12">
		<div class="widget-box">
			<div class="widget-header">
				<h4>Web Transactions</h4>
				<div class="widget-toolbar">
					<a href="#" data-action="collapse">
						<i class="icon-chevron-up"></i>
					</a>
				</div><!-- end widget toolbar-->
			</div><!-- end widget header-->
			<div class="widget-body">
				<div class="widget-main">
					<div class="row-fluid">
					<div class="col-xs-3">
					</div>
					<div class="col-xs-7">
						<form id="OTCTranForm" action="searchwebtransactionsapp.admin" method="post" autocomplete="off" class="form-horizontal">
								<div class="widget-box transparent">
									<div class="widget-body">
											<div class="widget-main">
												<input type="hidden" id="pageIndex" name="pageIndex" value="0">
												<div class="row-fluid labelMarginBottom" style="margin-bottom:5px;">
												    <label class="control-label boldText k-tooltip-validation" for="dateRange" style="float: left;width: 168px;padding-top: 5px;text-align: right;font-weight:bold;">Date Range :&nbsp; </label>
												    <div class="control-group">
														<div class="input-group" >
															<span class="input-group-addon" >
																<i class="icon-calendar"></i>
															</span>
															<input type="text" style="height:28px; width:170px;" class="validate[groupRequired[payments]]"  name="dateRange" tabindex="5" id="dateRange"/>
														</div>
													</div>
												 </div>

						                        <div class="row-fluid">
						                            <label class="control-label boldText k-tooltip-validation" for="siteId" style="float: left;width: 168px;padding-top: 5px;text-align: right;font-weight:bold;margin-bottom:8px;">Site :&nbsp; </label>
						                            <div class="">
						                                <select id="siteId" name="siteId" class="k-widget k-dropdownlist" style="width:200px" tabindex="1">
													 		  <#if sites?has_content && sites?size &gt; 1>
										                         <option value="" selected>All Sites</option>
															  </#if>
													            <#if sites?has_content>
														            <#foreach site in sites>
														               <#if (selectedSiteId?has_content)>
												                                <#if (selectedSiteId == site.id)>
												                                    <option value="${site.id}" selected> ${site.description}</option>
												                                <#else>
												                                    <option value="${site.id}"> ${site.description}</option>
												                                </#if>
												                       <#else>
												                            <option value="${site.id}"> ${site.description}</option>
												                       </#if>
												                    </#foreach>
											                    </#if>
											             </select>
						                            </div>
						                        </div>

						                        <div class="row-fluid">
						                            <label class="control-label boldText k-tooltip-validation" for="transactionType" style="float: left;width: 168px;padding-top: 5px;text-align: right;font-weight:bold;margin-bottom:8px;">Transaction Type :&nbsp; </label>
						                            <div class="">
						                                <select id="transactionType" name="transactionType" class="k-widget k-dropdownlist" style="width:200px" tabindex="3">
													 		<option value="" selected>All Transactions</option>
													            <#foreach transactionType in transactionTypeList>
													               <#if (selectTranType?has_content)>
											                                <#if (selectTranType == transactionType)>
											                                    <option value="${transactionType}" selected> ${transactionType}</option>
											                                <#else>
											                                    <option value="${transactionType}"> ${transactionType}</option>
											                                </#if>
											                      <#else>
											                            <option value="${transactionType}"> ${transactionType}</option>
											                       </#if>
											                    </#foreach>
											             </select>
						                            </div>
						                        </div>
	 											<div class="row-fluid">
													<label class="control-label boldText k-tooltip-validation" for="transactionType" style="float: left;width: 168px;padding-top: 5px;text-align: right;font-weight:bold;margin-bottom:8px;">Application :&nbsp; </label>
						                            <div class="">
						                                <select id="application" name="application" class="k-widget k-dropdownlist" style="width:200px" tabindex="3">
													 		<option value="" selected>All Applications</option>
													            <#foreach application in applicationList>
													               <#if (selectedApplication?has_content)>
											                                <#if (selectedApplication == application.code)>
											                                    <option value="${application.code}" selected> ${application.description}</option>
											                             	<#else>
																				<option value="${application.code}"> ${application.description}</option>
											                             	</#if>
																	<#else>
																		<option value="${application.code}"> ${application.description}</option>
											                    	</#if>
																</#foreach>
											             </select>
						                            </div>
						                        </div>

						                        <div class="row-fluid">
						                        	<label class="control-label required" style="float: left;width: 168px;padding-top: 5px;text-align: right;font-weight:bold;"></label>
		                    						<div class="">
												        <span class="input-icon">
															<input id="GenerateReport" type="submit" class="btn btn-info btn-search" value="Generate Report"/>
															<i class="icon-table white"></i>
														</span>
													</div>
												</div><!--/span-->

											</div><!--/widget-main-->
									</div><!--/widget-body-->
								</div><!--/widget-box-->
							</form>
						</div><!--/span-->
					</div> <!--row Fluid-->
				</div> <!--end widget main-->
			</div><!--End widget body-->
		</div><!-- end widget box-->
	</div><!-- end span-->
</div><!--/row fluid-->




<div class="row-fluid">
	<div class="span12">
		<div class="widget-box transparent ">
			<div class="widget-body">
				<div class="row-fluid">
					<div class="k-content">
				        <#if displayFlag = true>
				        <table width="100%">
					        <tr>
								<td style="text-align:left;">
								</td>
					             <td style="text-align:right;">
					                Problem Viewing this Report?<a href="searchwebtransactions.admin?siteId=<#if selectedSiteId?has_content >${selectedSiteId}</#if>&dateRange=<#if fromDate?has_content >${fromDate}</#if><#if toDate?has_content > - ${toDate}</#if>&transactionType=<#if selectTranType?has_content >${selectTranType}</#if>&application=<#if selectedApplication?has_content >${selectedApplication}</#if>&pageIndex=0">View in HTML Mode<img src="resources/images/html.png" /></a>
					             </td>
					    	</tr>
				    	</table>
				        <APPLET  CODE = "com.fdt.ecomadmin.applet.ReportApplet.class" CODEBASE = "applets" ARCHIVE = "ReportApplet.jar, servlet-api.jar, commons-lang-2.6.jar, jasperreports-applet-5.5.0.jar,iText-2.1.7.jar,poi-3.6.jar,poi-ooxml-3.6-20091214.jar, commons-logging-1.1.1.jar, commons-collections-3.2.1.jar, commons-digester-1.7.jar" WIDTH = "1350" HEIGHT = "600" ></XMP>
					        <PARAM NAME="type" VALUE="application/x-java-applet;version=1.2.2">
					        <PARAM NAME="scriptable" VALUE="false">
					        <PARAM NAME = CODE VALUE = "com.fdt.ecomadmin.applet.ReportApplet.class" >
					        <PARAM NAME = CODEBASE VALUE = "applets" >
					        <PARAM NAME = ARCHIVE VALUE = "ReportApplet.jar, servlet-api.jar, commons-lang-2.6.jar, jasperreports-applet-5.5.0.jar,iText-2.1.7.jar,poi-3.6.jar,poi-ooxml-3.6-20091214.jar, commons-logging-1.1.1.jar, commons-collections-3.2.1.jar, commons-digester-1.7.jar" >
					        <PARAM NAME = "REPORT_URL" VALUE="<#if reportURL?has_content >${reportURL}</#if>viewwebtransactionsreport.admin" />
					        <PARAM NAME = "REQUEST_URL" VALUE="<#if reportURL?has_content >${reportURL}</#if>" />
					        <PARAM NAME = "JSESSIONID" VALUE="<#if sessionId?has_content >${sessionId}</#if>">
					        <PARAM NAME = "SITE_ID" VALUE="<#if selectedSiteId?has_content >${selectedSiteId}</#if>">
					        <PARAM NAME = "LOCATION_ID" VALUE="<#if selectedLocationId?has_content >${selectedLocationId}</#if>">
					        <PARAM NAME = "IS_CERTIFIED" VALUE="<#if selectedIsCertified?has_content >${selectedIsCertified}</#if>">
					        <PARAM NAME = "FROM_DATE" VALUE="<#if fromDate?has_content >${fromDate}</#if>">
					        <PARAM NAME = "TO_DATE" VALUE="<#if toDate?has_content >${toDate}</#if>">
					        <PARAM NAME = "TX_TYPE" VALUE="<#if selectTranType?has_content >${selectTranType}</#if>">
					        <PARAM NAME = "APPLICATION" VALUE="<#if selectedApplication?has_content >${selectedApplication}</#if>">
				    	</APPLET>
				        </#if>
					</div> <!--End k-content-->
				</div> <!--end row-fluid-->

			</div><!--End widget body-->
		</div><!-- end widget box-->
	</div><!-- end span-->
</div><!-- end row-fluid-->

 <script type="text/javascript">
	$(document).ready(function(){
		$('#dateRange').daterangepicker(
		    {
		      ranges: {
		         'Today': [moment(), moment()],
		         'Yesterday': [moment().subtract('days', 1), moment().subtract('days', 1)],
		         'Last 7 Days': [moment().subtract('days', 6), moment()],
		         'Last 30 Days': [moment().subtract('days', 29), moment()],
		         'This Month': [moment().startOf('month'), moment().endOf('month')],
		         'Last Month': [moment().subtract('month', 1).startOf('month'), moment().subtract('month', 1).endOf('month')]
		      },
		      startDate: moment().subtract('days', 0),
		      endDate: moment()
		    }
		);
		<#if fromDate?has_content && fromDate != "" && toDate?has_content && toDate != "">
			$('#dateRange').val('${fromDate}' + ' - ' + '${toDate}');
		<#else>
			$('#dateRange').val(moment().subtract('days', 1).format('MM/DD/YYYY') + ' - ' + moment().format('MM/DD/YYYY'));
		</#if>

		$("#siteId").kendoDropDownList();
		$("#transactionType").kendoDropDownList();
		$("#application").kendoDropDownList();


		kendo.data.binders.widget.max = kendo.data.Binder.extend({
            init: function(widget, bindings, options) {
                //call the base constructor
                kendo.data.Binder.fn.init.call(this, widget.element[0], bindings, options);
            },
            refresh: function() {
                var that = this,
                value = that.bindings["max"].get(); //get the value from the View-Model
                $(that.element).data("kendoDatePicker").max(value); //update the widget
            }
        });

		//set max date as today's date
		var viewModel = kendo.observable({
            value: new Date(),
            maxDate: new Date()
        });
        kendo.bind($(document.body), viewModel);

       $("#GenerateReport").click(function() {
		    //Check Whether is it Empty
		    if( !$('#dateRange').val() ) {
			    bootbox.confirm("Please Enter a Date Range");
			    return false;
    		}
		});

		<#if SystemUtil.isPSOSuperAdmin(request)>
        	$( "#main_menu" ).accordion( "option", "active", 3 );
        <#else>
        	$( "#main_menu" ).accordion( "option", "active", 2 );
        </#if>

    });
</script>
</@dashboardLayout>