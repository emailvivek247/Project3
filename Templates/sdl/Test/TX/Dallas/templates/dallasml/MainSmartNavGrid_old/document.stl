<#-- *Modify columns -->
<#assign urlEncoder = new("net.javacoding.xsearch.utility.EscapeChars")>
 <tr>
    <td>
      <a target="_parent" onClick='rowSelected(this)' href="${request.getContextPath()}/search.do?indexName=dallasdetails&templateName=MLDetails_SN&lq=${urlEncoder.forURL("Instrument:\"" + doc.get("Instrument") + "\"")}">${number.format("###0.##",doc.get("Instrument"))}</a>
   </td>    
   <td>
    <#if isPhoneticSearch == "1">
   	  ${searchResult.highlight(doc, "PhoneticName")}      
   <#else>
          ${searchResult.highlight(doc, "Name")}
   </#if>  
   </td>
   <td>${searchResult.highlight(doc, "PartyRole")}</td>
   <td>${searchResult.highlight(doc, "DocType")}</td>
   <td>${searchResult.highlight(doc, "LicenseNum")}</td>
</tr>
