    <sql><![CDATA[
SELECT DISTINCT
       PT.PERSON_ALIAS_ID AS 'PERSON_ALIAS_ID'
      ,PT.PARTY_ID AS 'PARTYID'
      ,CASE WHEN LTRIM(RTRIM(REPLACE(DL.LICENSE_NUM, '-', ''))) = '' THEN 'AMCADDBNULL' ELSE ISNULL(REPLACE(DL.LICENSE_NUM, '-', ''), 'AMCADDBNULL') END AS       'DLNUMBER'
      ,CASE WHEN LTRIM(RTRIM(REPLACE((SELECT 
             ST.STATE_CD
        FROM ACM_STATE ST 
        WHERE ST.STATE_ID = DL.STATE_ID), '-', ''))) = '' THEN 'AMCADDBNULL' ELSE ISNULL(REPLACE((SELECT 
             ST.STATE_CD
        FROM ACM_STATE ST 
        WHERE ST.STATE_ID = DL.STATE_ID), '-', ''), 'AMCADDBNULL') END AS 'DLSTATE'
       ,CASE_ID AS 'CASEID'
FROM ACM_DRIVERSLICENSE DL 
INNER JOIN ACM_PARTY PT ON DL.DRIVERS_LICENSEID = PT.DRIVERS_LICENSEID
