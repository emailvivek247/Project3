<style type="text/css">
<!--
.active, .inactive {text-align: center; padding-top:0px; padding-bottom:5px; margin:5px}
.active   {font-weight:bold;}
.inactive {text-decoration: underline;}
//-->
</style>
<table border=0 cellpadding=0 cellspacing=0><tr>
<#foreach dc in dcs>
  <#if dc.isValid() && (dc.displayOrder>=0) && dc.getDefaultTemplateName()?has_content>
    <td class=<#if indexName == dc.name>"active"<#else>"inactive"</#if>>
      <a class=<#if indexName == dc.name>"active"<#else>"inactive"</#if> href="search.do?indexName=${dc.name}&q=${searchResult.getURLEncodedClearQuery()}">${dc.displayName}</a>
    </td>
  </#if>
</#foreach>
</tr></table>