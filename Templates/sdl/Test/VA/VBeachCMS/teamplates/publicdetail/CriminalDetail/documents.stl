<#if searchResult.docs?has_content>


<b>Defendant Information</b>
<#if (partyinfoCounter > 0)>
<table id="partyinfo" class="tablesorter">
 <thead><tr>
  <th>${searchResult.datasetConfiguration.getColumn("DefendantName").displayName}</th>
  <th>${searchResult.datasetConfiguration.getColumn("DOBMonth").displayName}</th>
  <th>${searchResult.datasetConfiguration.getColumn("Sex").displayName}</th>
  <th>${searchResult.datasetConfiguration.getColumn("City").displayName}</th>
  <th>${searchResult.datasetConfiguration.getColumn("State").displayName}</th>
 </tr></thead>
 <tbody>

<#foreach doc in searchResult.docs>

    <#assign DOBDay = doc.getValues("DOBDay")>
    <#assign DOBMonth = doc.getValues("DOBMonth")>
<#assign Sex = doc.getValues("Sex")>  
<#assign City = doc.getValues("City")>  
   <#assign State = doc.getValues("State")>  
   
    <#assign loopCounter = 0>

    <#foreach DefendantName in doc.getValues("DefendantName")>
	<tr>
       
<td>${DefendantName} </td>

             <td>${DOBMonth[loopCounter]?replace('|', '&nbsp;')}/${DOBDay[loopCounter]?replace('|', '&nbsp;')}</td>
 <td>${Sex[loopCounter]?replace('|', '&nbsp;')}</td> 
 <td>${City[loopCounter]?replace('|', '&nbsp;')}</td>  
 <td>${State[loopCounter]?replace('|', '&nbsp;')}</td>  
                          <#assign loopCounter = loopCounter + 1>
	</tr>
     </#foreach>

</#foreach>

 </tbody>
</table>
<#else>
<br><br>
<b><i>No information available.</i></b>
<br><br>
</#if>
</#if>



<#assign schedulingCounter = 0>

<#if searchResult.docs?has_content>
    <#foreach doc in searchResult.docs>
	<#foreach SessionDate in doc.getValues("SessionDate")>
	   <#assign schedulingCounter = 1>	
	   <#break>
	</#foreach>
    </#foreach>
</#if>


<#if searchResult.docs?has_content>

<b>Scheduling Information</b>
<#if (schedulingCounter > 0)>
<table id="scheduling" class="tablesorter {sortlist: [[0,0]]}" >
 <thead><tr>
  <th>${searchResult.datasetConfiguration.getColumn("SessionDate").displayName}</th>
  <th>${searchResult.datasetConfiguration.getColumn("ScheduledParty").displayName}</th>
  <th>${searchResult.datasetConfiguration.getColumn("ScheduledCharge").displayName}</th>
  <th>${searchResult.datasetConfiguration.getColumn("AppearanceReason").displayName}</th>
  <th>${searchResult.datasetConfiguration.getColumn("HearingResult").displayName}</th>
  <th>${searchResult.datasetConfiguration.getColumn("SchedulingComment").displayName}</th>
 </tr></thead>
 <tbody>

<#foreach doc in searchResult.docs>
    
    <#assign ScheduledParty = doc.getValues("ScheduledParty")>
    <#assign ScheduledCharge = doc.getValues("ScheduledCharge")>
    <#assign AppearanceReason = doc.getValues("AppearanceReason")>
    <#assign HearingResult = doc.getValues("HearingResult")>
    <#assign SchedulingComment = doc.getValues("SchedulingComment")>
    <#assign loopCounter = 0>

    <#foreach SessionDate in doc.getValues("SessionDate")>
	<tr>
             <td>${SessionDate?replace('01/01/1753', '&nbsp;')?replace('AM', ' AM')?replace('PM', ' PM')}</td>
             <td>${ScheduledParty[loopCounter]}</td>
             <td>${ScheduledCharge[loopCounter]}</td>
             <td>${AppearanceReason[loopCounter]}</td>
             <td>${HearingResult[loopCounter]}</td>
             <td>${SchedulingComment[loopCounter]}</td>
            
             <#assign loopCounter = loopCounter + 1>
	</tr>
     </#foreach>

</#foreach>

 </tbody>
</table>
<#else>
<br><br>
<b><i>No information available.</i></b>
<br><br>
</#if>
</#if>







<#assign chargeCounter = 0>

<#if searchResult.docs?has_content>
