  <dataquery name="WorkingQueue" batch-size="128" is-skipping-null-parameters="false">
    <sql><![CDATA[
SELECT DISTINCT
	 CASE WHEN LTRIM(RTRIM(SD.APPR_DESCRIPTION)) = '' THEN '|' ELSE ISNULL(SD.APPR_DESCRIPTION, '|') END AS 'AppDesc'
	,SD.SESSION_DATE AS 'SessionDate'
	,SD.CASE_ID AS 'CaseID'
	,SD.CASE_NUMBER AS 'CaseNumber'
	,CASE WHEN LTRIM(RTRIM(SD.HEARING_DESC)) = '' THEN '|' ELSE ISNULL(SD.HEARING_DESC, '|') END AS 'HearingDesc'
	,SD.JUDGE_CD AS 'Judge'
	,CASE WHEN LTRIM(RTRIM(AR.HEARING_DESC)) = '' THEN '|' ELSE ISNULL(AR.HEARING_DESC, '|') END AS 'HearingResult'
	,SD.COURT_SESSION_ID AS 'CourtSessionID'
FROM WN_ACC_V_SESSION_DETAIL SD
INNER JOIN SW_ACC_V_SESN_ITM_APPR_RESN AR
ON SD.COURT_SESSION_ID = AR.COURT_SESSION_ID
WHERE SEALED_CASE_FLG <> 'Y'
AND IS_RESTRICTED_FLG <> 'Y'
AND EXPUNGE_FLG <> 'Y'
    ]]></sql>
