<script type="text/javascript" src="<@com.currentUrlPrefix/>/resource/jquery.suggest.js"></script>
<link rel="stylesheet" href="<@com.currentUrlPrefix/>/resource/jquery.suggest.css" type="text/css"/>


<script type="text/javascript">

$(document).ready(function(){
  $("#enable").click(function(){
        $("#WebName").attr("autocomplete", "off");
        
		$("#WebName").suggest("suggest.do?indexName=${indexName}&length=10000",
			 {
			  minchars:1,
			  preventDefaultReturn: true,
			  preventDefaultTab: true,
			  autoFill: true,
			  multiple: true,
			  multipleSeparator: " "
			 });
  });
  $("#disable").click(function(){
        $("#WebName").attr("autocomplete", "on").off();
        $("#WebName").unbind("ui.combobox");

  });
});


</script>