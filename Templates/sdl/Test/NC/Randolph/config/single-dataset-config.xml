  <dataquery name="WorkingQueue" batch-size="128" is-skipping-null-parameters="false">
    <sql><![CDATA[
Select distinct
cast(a.id as varchar)+ '-' + cast(isnull(a.seq, 0) as varchar) as 'ROAMPKKEY'
,a.ID AS 'PK_KEY'
,'' AS 'ADD'
,a.VERIFIED as VER
,a.STATUS
,CAST(a.SEQ_KEY AS VARCHAR) AS 'INSTRUMENT'
,CONVERT(VARCHAR(10), a.RECDATE1, 112) AS DATEREC
,RECDATE1 AS RECDATEFILTER
,CONVERT(VARCHAR(30), a.RECDATE1, 120) AS DATERECDISPLAY
,CONVERT(VARCHAR(20), a.RECDATE2, 112) AS DATEVENT
,CONVERT(VARCHAR(20), a.RECDATE2, 120) AS DATEVENTDISPLAY
,RECDATE2 AS DATEVENTFILTER
,LNAME+' '+ISNULL(FNAME,'')+' '+ ISNULL(MNAME,'') as NAME
,LNAME+' '+ISNULL(FNAME,'')+' '+ ISNULL(MNAME,'') as LIKENAME
,LNAME+' '+ISNULL(FNAME,'')+' '+ ISNULL(MNAME,'') as PHONETICNAME
,LNAME+' '+ISNULL(FNAME,'')+' '+ ISNULL(MNAME,'') as NAMESORT
,LNAME+' '+ISNULL(FNAME,'')+' '+ ISNULL(MNAME,'') as SORTNAME
,LNAME+' '+ISNULL(FNAME,'')+' '+ ISNULL(MNAME,'') as NAMEDISPLAY
,a.ROLENAME AS PARTYROLE
,DESIGSTATUS AS 'DESIGSTATUS'
,a.ROLE AS 'ROLE'
,OTHERNAME as 'OTHERNAME'
,OTHERDESIGSTATUS
,OTHERROLE
,OTHERROLENAME
,a.INDEXTYPE
,a.code as TYPE
,Book AS 'BOOK'
,Page AS 'PAGE'
,a.FIRSTREFERENCE as MARGINALS
,offlinerecord
, a.DOCTYPE AS 'DOCTYPE'
,a.FIRSTLEGAL as REMARKS
,a.AMOUNT2 as CONSIDERATION
,(isnull (a.bookshort,'')+ ' ' + isnull(a.BOOK,'') + ' '+ isnull(a.PAGE,'')) AS 'BOOKPAGE'
,a.bookshort AS BOOKSHORT
,a.OFFLINEIMAGE
,a.BACKFILENUM
,a.DOCTYPEDESC
,a.MULTIPLE_PARTY1
,a.MULTIPLE_PARTY2
,a.MULTIPLE_LEGAL
,a.MULTIPLE_REFERENCE
,a.MULTIPLE_RETURNEE
,a.CORP
,a.date_time_mod
from PUBLICSEARCH a where OFFLINERECORD <> 'Y' AND ROLE IN (1,0) AND a.SEQ = 1
    ]]></sql>
