
<#if searchResult.docs?has_content>
<table id="searchResult" class="tablesorter">
 <thead><tr>
  <th>${searchResult.datasetConfiguration.getColumn("CASE_NUMBER").displayName}</th>
  <th>${searchResult.datasetConfiguration.getColumn("COURT_NAME").displayName}</th>
  <th>${searchResult.datasetConfiguration.getColumn("CASE_TYP_DESC").displayName}</th>
  <th>${searchResult.datasetConfiguration.getColumn("CASE_SHORT_TITLE").displayName}</th>
  <th>${searchResult.datasetConfiguration.getColumn("CASE_FILING_DT").displayName}</th>
  <th>${searchResult.datasetConfiguration.getColumn("BOOK").displayName}</th>
  <th>${searchResult.datasetConfiguration.getColumn("PAGE").displayName}</th>
  <th>${searchResult.datasetConfiguration.getColumn("JUDGEMENT_AGAINST").displayName}</th>
  <th>${searchResult.datasetConfiguration.getColumn("JUDGEMENT_FOR").displayName}</th>
  <th>${searchResult.datasetConfiguration.getColumn("EVENT_ENT_TYP_DESC").displayName}</th>
  <th>${searchResult.datasetConfiguration.getColumn("DISPOSITION_DATE").displayName}</th>
  <th>${searchResult.datasetConfiguration.getColumn("JUDGMENT_AMOUNT").displayName}</th>
  <th>${searchResult.datasetConfiguration.getColumn("JUDGMENT_SATISFIED_BY").displayName}</th>
  <th>${searchResult.datasetConfiguration.getColumn("ACCOUNT_FOLIO_NUMBER").displayName}</th>
  
  
 </tr></thead>
 <tbody>
<#assign ResultId =0>
 <#foreach doc in searchResult.docs>
  <#include "document.stl">
 </#foreach>
<#else>
 <#include "noResults.stl"> 
</#if>
 </tbody>
</table>


<script type="text/javascript">
$(document).ready(function() {
	oTable = $('#searchResult').dataTable({
		"bJQueryUI": true,
		"bPaginate": false,
		"bLengthChange": false,
		"bFilter": false,
		"bSort": true,
		"bInfo": false,
		"bAutoWidth": false,
		"aaSorting": []
	});
} );
</script>
