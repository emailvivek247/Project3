  <dataquery name="WorkingQueue" batch-size="128" is-skipping-null-parameters="false">
    <sql><![CDATA[
SELECT DISTINCT 
	 ISNULL(RTRIM(CASE_ID), '|') AS 'CaseID'
	,ISNULL(OFFENSE_REF_NUM, '|') AS 'ChargeCode'
	,ISNULL(OFFENSE_DESC, '|') AS 'ChargeDescription'
	,ISNULL(OFFENSE_CLASS_DESC, '|') AS 'ChargeClass'
	,ISNULL(OFFENSE_TYP_DESC, '|') AS 'ChargeType'
	,CONVERT(VARCHAR(20), OFFENSE_DT_FRM, 101) AS 'OffenseDateFrom'
	,ISNULL(DISPLAYNAME, '|') AS 'ChargeName'
	,ISNULL(DISPOSITION_DESC, '|') AS 'Disposition'
	,ISNULL(CONVERT(VARCHAR(20), DISPOSITION_DT, 101), '|') AS 'DispositionDate'
	,CHARGE_SEQ AS 'SeqNum'
FROM VICT_ACM_V_GCI_CHARGES
WHERE SEALED_FLAG <> 'Y'
AND SEALED_PARTY_FLG <> 'Y'
    ]]></sql>
