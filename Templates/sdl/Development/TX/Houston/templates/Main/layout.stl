<link rel="stylesheet" href="<@com.currentUrlPrefix/>/resource/jquery-ui-1.8.11.glossy-blue.css" type="text/css" media="print, projection, screen" />
<link rel="stylesheet" href="<@com.currentUrlPrefix/>/resource/theme-blue.css" type="text/css" media="print, projection, screen" />
