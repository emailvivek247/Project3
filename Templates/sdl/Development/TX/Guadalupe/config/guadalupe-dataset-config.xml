  <dataquery name="WorkingQueue" batch-size="128" is-skipping-null-parameters="false">
    <sql><![CDATA[
SELECT DISTINCT
cast(a.id as varchar)+ '-' + cast(isnull(b.seq, 0) as varchar) as 'ROAMPKKEY'
,a.ID AS 'PK_KEY'
,'' AS 'ADD'
,a.VERIFIED as VER
,a.STATUS
,CAST(a.SEQ_KEY AS VARCHAR) AS 'INSTRUMENT'
,CONVERT(VARCHAR(10), RECDATE1, 112) AS DATEREC
,RECDATE1 AS RECDATEFILTER
,CONVERT(VARCHAR(30), RECDATE1, 120) AS DATERECDISPLAY
,LTRIM(RTRIM(CONVERT(VARCHAR(20), a.RECDATE2, 112))) AS DATEVENT
,LTRIM(RTRIM(CONVERT(VARCHAR(20), a.RECDATE2, 120))) AS DATEVENTDISPLAY
,RECDATE2 AS DATEVENTFILTER
,LNAME+' '+ISNULL(FNAME,'')+' '+ ISNULL(MNAME,'') as 'WebName'
,LNAME+' '+ISNULL(FNAME,'')+' '+ ISNULL(MNAME,'') as 'WebPhoneticName'
,LTRIM(RTRIM(LNAME+' '+ISNULL(FNAME,'')+' '+ ISNULL(MNAME,''))) as NAME
,LTRIM(RTRIM(LNAME+' '+ISNULL(FNAME,'')+' '+ ISNULL(MNAME,''))) as NAMESORT
,LTRIM(RTRIM(LNAME+' '+ISNULL(FNAME,'')+' '+ ISNULL(MNAME,''))) as LIKENAME
,LTRIM(RTRIM(LNAME+' '+ISNULL(FNAME,'')+' '+ ISNULL(MNAME,''))) as PHONETICNAME
,LTRIM(RTRIM(LNAME+' '+ISNULL(FNAME,'')+' '+ ISNULL(MNAME,''))) as NAMEDISPLAY
,a.ROLENAME AS PARTYROLE
,DESIGSTATUS
,UPPER(SUBSTRING(ROLENAME,1,1)) + LOWER(SUBSTRING(ROLENAME,2,50)) AS PartyRoleType
,case when role=1 then 'Direct Party' when role=2 then 'Reverse Party' when role not in (1,2) then 'No Party' end as RoleType
,a.ROLE AS 'ROLE'
,OTHERNAME as 'OTHERNAME'
,OTHERDESIGSTATUS
,OTHERROLE
,OTHERROLENAME
,a.INDEXTYPE AS 'INDEXTYPE'
,LTRIM(RTRIM(a.code)) as TYPE
,Book AS 'BOOK'
,Book AS 'BOOKEXACT'
,Page AS 'PAGE'
,Page AS 'PAGEEXACT'
,b.LOTNUM
,b.SECTIONNUM AS 'SECTIONNUM'
,b.PHASE AS 'PHASE'
,b.BLOCKNUM
,b.SUBDIVISION AS 'SUBDIVISION'
,LDESC as 'Freeform'
,LEGAL1 as 'ProjectNum'
,LEGAL2 as 'MapInstrumentNumber'
,LDESC2 as 'Desc2'
,LDESC3 as 'Desc3'
,b.GPIN
,a.FIRSTREFERENCE as MARGINALS
,b.seq AS 'SEQ'
,offlinerecord
,'Guadalupe' as 'Location'
, a.indextype as 'IndexCode'
, a.seq as PartySeq
, a.DOCTYPE AS 'DOCTYPE'
, a.DOCTYPE AS 'DocCode'
,a.FIRSTLEGAL as REMARKS
,a.AMOUNT2 as CONSIDERATION
,(isnull (a.bookshort,'')+ ' ' + isnull(a.BOOK,'') + ' '+ isnull(a.PAGE,'')) AS 'BOOKPAGE'
,a.bookshort AS BOOKSHORT
,a.OFFLINEIMAGE
,a.BACKFILENUM AS 'BACKFILENUM'
,a.BACKFILENUM AS 'BACKFILENUMEXACT'
,a.DOCTYPEDESC
,a.MULTIPLE_PARTY1
,a.MULTIPLE_PARTY2
,a.MULTIPLE_LEGAL
,a.MULTIPLE_REFERENCE
,a.MULTIPLE_RETURNEE
,b.MAPBOOK
,b.MAPPAGE
,b.LEGAL_TYPE
,b.LDESC
,b.LDESC2
,b.LDESC3
,b.LEGAL2
,b.ACTIVE AS ACTIVE
,b.LEGALDESCRIPTION
,b.CITY
,a.CORP
,CASE 
      WHEN a.CORP = 'N' THEN 'Human'
      WHEN a.CORP = 'Y' THEN 'Non-Human'
END AS 'CorpFlag'
,a.date_time_mod
FROM PUBLICSEARCH a left outer join LEGAL b ON a.SEQ_KEY = b.SEQ_KEY 
WHERE OFFLINERECORD <> 'Y' 
ORDER BY a.date_time_mod DESC
    ]]></sql>
