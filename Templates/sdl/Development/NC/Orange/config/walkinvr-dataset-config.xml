  <dataquery name="Incremental">
    <sql><![CDATA[
Select distinct
cast(a.id as varchar)+ '-' + cast(isnull(b.seq, 0) as varchar) as 'ROAMPKKEY'
,CAST(a.SEQ_KEY AS VARCHAR) AS 'Instrument'
,CONVERT(VARCHAR(10), RECDATE1, 112) AS 'RecDate'
,RECDATE1 AS 'RecDateFiler'
,CONVERT(VARCHAR(30), RECDATE1, 120) AS 'RecDateDisplay'
,CONVERT(VARCHAR(20), a.RECDATE2, 112) AS 'EventDate'
,CONVERT(VARCHAR(20), a.RECDATE2, 120) AS 'EventDateDisplay'
,RECDATE2 AS 'EventDateFilter'
,LNAME+' '+ISNULL(FNAME,'')+' '+ ISNULL(MNAME,'') as 'WebName'
,LNAME+' '+ISNULL(FNAME,'')+' '+ ISNULL(MNAME,'') as 'BasicName'
,LNAME+' '+ISNULL(FNAME,'')+' '+ ISNULL(MNAME,'') as 'NameSort'
,LTRIM(RTRIM(LNAME+' '+ISNULL(FNAME,'')+' '+ ISNULL(MNAME,''))) as 'ExactName'
,LNAME+' '+ISNULL(FNAME,'')+' '+ ISNULL(MNAME,'') as 'WebPhoneticName'
,LNAME+' '+ISNULL(FNAME,'')+' '+ ISNULL(MNAME,'') as 'PhoneticNameSort'
,LNAME+' '+ISNULL(FNAME,'')+' '+ ISNULL(MNAME,'') as 'SortName'
,LTRIM(RTRIM(a.ROLENAME)) AS 'PartyRole'
,case when a.role is null then '0' when a.role = '' then '0' else LTRIM(RTRIM(a.role)) end as RoleFilter
,UPPER(SUBSTRING(ROLENAME,1,1)) + LOWER(SUBSTRING(ROLENAME,2,50)) AS 'PartyRoleType'
,case when role=1 then 'Direct Party' when role=2 then 'Reverse Party' when role not in (1,2) then 'No Party' end as 'RoleType'
,(SELECT LTRIM(RTRIM(descr)) FROM Doctype2lov WHERE type2 = a.indextype) as 'IndexTypeFilter'
,DESIGSTATUS AS 'DesigStatus'
,OTHERNAME as 'OtherName'
,LTRIM(RTRIM(a.code)) as 'DocType'
,LTRIM(RTRIM(c.DESCR)) AS 'DocTypeDesc'
,Book AS 'Book'
,Page AS 'Page'
,b.LOTNUM AS 'LotNum'
,b.UNIT AS 'Unit'
,b.SECTIONNUM AS 'SectionNum'
,b.PHASE AS 'Phase'
,b.BUILDING AS 'Building'
,b.ACREAGE AS 'Acreage'
,b.SUBDIVISION AS 'Subdivision'
,b.TOWNSHIP AS 'Township'
,b.GPIN
,a.FIRSTREFERENCE as 'FirstReference'--MARGINALS
,case when b.seq is null then 0 else b.seq end as 'SEQ'
,a.seq AS 'PartySeq'
,offlinerecord
, a.DOCTYPE AS 'DocTypeCode' --DOCTYPE
,a.MULTIPLE_LEGAL
,a.MULTIPLE_REFERENCE
,b.LDESC AS 'Other'
,b.LEGALDESCRIPTION
,a.CORP
,CASE 
      WHEN a.CORP = 'N' THEN 'Human'
      WHEN a.CORP = 'Y' THEN 'Non-Human'
END AS 'CorpFlag'
,RT.date_time_mod
from PUBLICSEARCH a left outer join LEGAL b on
      a.SEQ_KEY = b.SEQ_KEY 
      inner join DOCTYPE1LOV c
      on a.CODE = c.CODE
      INNER JOIN ROAM_TRACK RT with (nolock) ON RT.PK_KEY = A.ID
      AND RT.TAB_NAME = 'PUBLICSEARCH' 
      LEFT OUTER JOIN ROAM_TRACK RT1 with (nolock) ON RT1.PK_KEY = B.SEQ_KEY
      AND RT1.TAB_NAME = 'LEGAL' 
      where OFFLINERECORD ='N' 
      and a.indextype in (6,13,14)
     AND RT.DATE_TIME_MOD > ?
ORDER BY RT.date_time_mod
    ]]></sql>
  </dataquery>
