function toggle(legal) {
	var legalDiv = document.getElementById("legal");
	
	if (legalDiv.style.display == "")
		legalDiv.style.display = "none";
	else legalDiv.style.display = "";
	}
	
function toggle2(toggleLink, link) {
  var hideLegalLink = document.getElementById("toggleLink");
 
  if (document.getElementById("legal").style.display == "none") 
      {
	    link.innerHTML = "[+] Expand Legal Fields";
	    link.style.color = "green"
	  } else {
	    link.innerHTML = "[&ndash;] Collapse Legal Fields";
	    link.style.color = "red"
	  }
      }


