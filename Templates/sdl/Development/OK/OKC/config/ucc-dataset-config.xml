  <dataquery name="Incremental">
    <sql><![CDATA[
Select distinct
cast(a.id as varchar) as pk_key
,CASE WHEN LTRIM(RTRIM(CONVERT(VARCHAR(50), a.SEQ_KEY))) = '' THEN '|' ELSE ISNULL(CONVERT(VARCHAR(50), a.SEQ_KEY), '|') END AS 'Instrument'
,CONVERT(VARCHAR(10), a.RECDATE1, 112) AS RecDate
,a.RECDATE1 AS RecDateFilter
,CONVERT(VARCHAR(30), RECDATE1, 120) AS RecDateDisplay
,LTRIM(RTRIM(LNAME+' '+ISNULL(FNAME,'')+' '+ ISNULL(MNAME,''))) as 'DisplayName'
,LTRIM(RTRIM(LNAME))+' '+ISNULL(LTRIM(RTRIM(FNAME)),'')+' '+ ISNULL(LTRIM(RTRIM(MNAME)),'') as 'NameSort'
,LTRIM(RTRIM(a.LNAME)) AS 'LastName'
,LTRIM(RTRIM(a.LNAME)) AS 'LastNameStarts'
,LTRIM(RTRIM(a.LNAME)) AS 'LastNameExact'
,LTRIM(RTRIM(a.LNAME)) AS 'LastNamePhonetic'
,LTRIM(RTRIM(a.FNAME + ISNULL(LTRIM(RTRIM(a.MNAME)), ''))) AS 'FirstName'
,LTRIM(RTRIM(a.FNAME + ISNULL(LTRIM(RTRIM(a.MNAME)), ''))) AS 'FirstNameStarts'
,LTRIM(RTRIM(a.FNAME + ISNULL(LTRIM(RTRIM(a.MNAME)), ''))) AS 'FirstNameExact'
,LTRIM(RTRIM(a.FNAME + ISNULL(LTRIM(RTRIM(a.MNAME)), ''))) AS 'FirstNamePhonetic'
,a.DESIGSTATUS AS DesigStatus
,LTRIM(RTRIM(UPPER(SUBSTRING(a.ROLENAME,1,1)) + LOWER(SUBSTRING(a.ROLENAME,2,50)))) AS PartyRole
,case when a.role=1 then 'Direct Party' when a.role=2 then 'Reverse Party' when role not in (1,2) then 'No Party' end as Role
,case when a.role is null then '0' when a.role = '' then '0' else LTRIM(RTRIM(a.role)) end as RoleFilter
,a.OTHERNAME AS OtherName
,a.OTHERDESIGSTATUS AS OtherDesigStatus
,a.OTHERROLE AS OtherRole
,a.OTHERROLENAME AS OtherRoleName
,(SELECT descr FROM Doctype2lov WHERE type2 = indextype) as IndexType 
,a.code as DocType
,c.DESCR AS 'DocTypeDesc'
,a.FirstReference
,a.MULTIPLE_REFERENCE AS MultipleReferece
,a.offlinerecord
,a.offlineimage
,(Select propertyvalue from document_detail where propertyid=75 and seq_key=a.seq_key ) as Collateral 
,(Select propertyvalue from document_detail where propertyid=78 and seq_key=a.seq_key ) as BigCollateral 
,'Oklahoma' as Location
,a.indextype as IndexCode
,a.seq as PartySeq
,a.doctype as DocCode
,a.seq
,RT.date_time_mod
,a.backfilenum AS 'LegacyNumber'
    from PUBLICSEARCH a 
    inner join DOCTYPE1LOV c
    on a.CODE = c.CODE
    where OFFLINERECORD ='N' 
    AND a.indextype in (2,101)
    AND (RT.DATE_TIME_MOD > ?)
order by RT.date_time_mod desc
    ]]></sql>
