  <dataquery name="WorkingQueue" batch-size="128" is-skipping-null-parameters="false">
    <sql><![CDATA[
SELECT DISTINCT
    FC.CASE_ID AS 'CaseID'
   ,FC.CASE_NUMBER AS 'CaseNumber'
   ,EE.EVENT_ENTRY_ID AS 'EventEntryID'
   ,EET.EVENT_ENT_TYP_DESC AS 'CauseType'
   ,FC.NEXT_SESSION_DATE AS 'NextDate'
   ,CST.CASE_STATUS_DESCR AS 'CaseStatus'
   ,FC.CASE_SHORT_TITLE AS 'CaseStyle'
   ,P.PERSON_ALIAS_ID AS 'PersonAliasID'
   ,CR.COURT_ROOM_DESC AS 'CourtRoom'
   ,PERS.DISPLAYNAME AS 'AttorneyName'
   ,ATTBAR.BAR_ASSOC_NUM AS 'BarAssocNum'
FROM ACM_FACTS_CASE FC
INNER JOIN ACM_PARTY P ON P.CASE_ID = FC.CASE_ID 
INNER JOIN ACM_PERSON PERS ON PERS.PERSON_ALIAS_ID = P.PERSON_ALIAS_ID
INNER JOIN ACM_ATTORNEY_BAR_ASSOC ATTBAR ON ATTBAR.PERSON_ALIAS_ID = P.PERSON_ALIAS_ID 
LEFT JOIN ACM_EVENT_TO_CASE ETC ON ETC.CASE_ID = FC.CASE_ID
LEFT JOIN ACM_EVENT_ENTRY EE ON EE.EVENT_ENTRY_ID = ETC.EVENT_ENTRY_ID
LEFT JOIN ACM_EVENT_ENT_TYP EET ON EET.EVENT_ENTRY_TYP_ID = EE.EVENT_ENTRY_TYP_ID
LEFT JOIN ACM_CASE_STATUS CS ON CS.CASE_ID = FC.CASE_ID
LEFT JOIN ACM_CASE_STATUS_TYP CST ON CST.CASE_STATUS_TYP_ID = CS.CASE_STATUS_TYP_ID
LEFT JOIN ACC_COURT_ROOM CR ON CR.COURT_ROOM_ID = FC.COURT_ROOM_ID
WHERE FC.SEALED_CASE_FLG <> 'Y'
AND FC.IS_RESTRICTED_FLG <> 'Y'
AND EE.IS_SEALED_FLG <> 'Y'
AND EE.IS_RESTRICTED_FLG <> 'Y'
AND EE.ROA_HIDDEN <> 'Y'
AND EE.IS_DRAFT <> 'Y'
    ]]></sql>
