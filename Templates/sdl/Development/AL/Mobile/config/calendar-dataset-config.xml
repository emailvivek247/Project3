  <dataquery name="WorkingQueue" batch-size="128" is-skipping-null-parameters="false">
    <sql><![CDATA[
SELECT DISTINCT
    COALESCE(X.DT1,Y.DT2) SessionDate,
    ISNULL(X.COUNT1,0) CourtRoom1, 
    ISNULL(Y.COUNT2,0) CourtRoom2,
    DATENAME(DW,COALESCE(X.DT1,Y.DT2)) AS 'SessionWeekDay'
   ,CONVERT(VARCHAR(30), COALESCE(X.DT1,Y.DT2), 120) AS 'SessionDateDisplay'
   ,CONVERT(VARCHAR(10), COALESCE(X.DT1,Y.DT2), 112) AS 'SessionDateSearch' 
   
FROM 
(
SELECT cast(SESSION_START_DT as DATE) DT1,COUNT(DISTINCT CASE_NUMBER) Count1
FROM MCPC_ACC_V_ATTENDANCE_SEARCH 
where court_room_id = 1
group by cast(SESSION_START_DT as DATE)
)X 
FULL OUTER JOIN 
(
SELECT DISTINCT cast(SESSION_START_DT as DATE) DT2,COUNT(DISTINCT CASE_NUMBER)  Count2
FROM MCPC_ACC_V_ATTENDANCE_SEARCH 
where court_room_id = 2
group by cast(SESSION_START_DT as DATE)
)Y ON Y.DT2 = X.DT1 
WHERE (COALESCE(X.DT1,Y.DT2) >= (GETDATE() - 30) and COALESCE(X.DT1,Y.DT2) <= GETDATE()) 
or (COALESCE(X.DT1,Y.DT2) >= GETDATE() and (COALESCE(X.DT1,Y.DT2) <= (GETDATE() + 150)))
order by COALESCE(X.DT1,Y.DT2) asc
    ]]></sql>
