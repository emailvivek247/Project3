
<#if searchResult.docs?has_content>
<b>General Case Information</b>
	<table id="gci" class="tablesorter">
		<thead>
			<tr>
			  <th>${searchResult.datasetConfiguration.getColumn("CaseNumber").displayName}</th>
			  <th>${searchResult.datasetConfiguration.getColumn("Book").displayName}</th>
			  <th>${searchResult.datasetConfiguration.getColumn("Page").displayName}</th>
			</tr>
		</thead>
		<tbody>
		<#assign ResultId =0>


		<#foreach doc in searchResult.docs>

		<#assign CaseNumber = doc.getValues("CaseNumber")>
		<#assign Book = doc.getValues("Book")>
		<#assign Page = doc.getValues("Page")>
		<#assign CaseID = doc.getValues("CaseID")>

		       <tr>
			<td>${searchResult.highlight(doc, "CaseNumber")}</td>
			<td>${searchResult.highlight(doc, "Book")}</td>
			<td>${searchResult.highlight(doc, "Page")}</td>
			<#assign caseNumberKey = doc.getValues("CaseID")>
		       </tr>

	        </#foreach>
	        </tbody>
	</table>
<#else>
 <table id="gci" class="tablesorter">
 		<thead>
 			<tr>
 			  <th>${searchResult.datasetConfiguration.getColumn("CaseNumber").displayName}</th>
 			  <th>${searchResult.datasetConfiguration.getColumn("Book").displayName}</th>
 			  <th>${searchResult.datasetConfiguration.getColumn("Page").displayName}</th>
 			</tr>
 		</thead>
 		<tbody>
 		<#assign ResultId =0>
 
 
 		<#foreach doc in searchResult.docs>
 
 		<#assign CaseNumber = doc.getValues("CaseNumber")>
 		<#assign Book = doc.getValues("Book")>
 		<#assign Page = doc.getValues("Page")>
 		<#assign CaseID = doc.getValues("CaseID")>
 
 		       <tr>
 			<td></td>
 			<td></td>
 			<td></td>
 			<#assign caseNumberKey = doc.getValues("CaseID")>
 		       </tr>
 
 	        </#foreach>
 	        </tbody>
	</table>
</#if>


<div id="trustfundcontainer"></div>
<div id="bondscontainer"></div>
<div id="partycontainer"></div>
<div id="roacontainer"></div>
<div id="accountingcontainer"></div>
<div id="claimscontainer"></div>
<script type="text/javascript">
$(document).ready(function() {

var url1 = "search.do?indexName=financials&templateName=financials&lq=CaseID%3A%22<#list caseNumberKey as CaseID>${CaseID}</#list>%22";
$('#trustfundcontainer').load(url1);
var url2 = "search.do?indexName=bonds&templateName=bonds&lq=CaseID%3A%22<#list caseNumberKey as CaseID>${CaseID}</#list>%22";
$('#bondscontainer').load(url2);
var url3 = "search.do?indexName=party&templateName=party&lq=CaseID%3A%22<#list caseNumberKey as CaseID>${CaseID}</#list>%22";
$('#partycontainer').load(url3);
var url4 = "search.do?indexName=roa&templateName=roa&lq=CaseID%3A%22<#list caseNumberKey as CaseID>${CaseID}</#list>%22";
$('#roacontainer').load(url4);
var url5 = "search.do?indexName=accounting&templateName=accounting&lq=CaseID%3A%22<#list caseNumberKey as CaseID>${CaseID}</#list>%22";
$('#accountingcontainer').load(url5);
var url6 = "search.do?indexName=claims&templateName=claims&lq=CaseID%3A%22<#list caseNumberKey as CaseID>${CaseID}</#list>%22";
$('#claimscontainer').load(url6);




	oTable = $('#gci').dataTable({
		"bJQueryUI": true,
		"bPaginate": false,
		"bLengthChange": false,
		"bFilter": false,
		"bSort": true,
		"bInfo": false,
		"bAutoWidth": false,
		"aaSorting": []
	});
	
} );
</script>
