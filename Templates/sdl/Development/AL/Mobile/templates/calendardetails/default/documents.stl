
<#if searchResult.docs?has_content>
<table id="searchResult" class="tablesorter">
 <thead><tr>
  <th>${searchResult.datasetConfiguration.getColumn("CaseNumber").displayName}</th>
  <th>${searchResult.datasetConfiguration.getColumn("CourtRoom").displayName}</th>
  <th>${searchResult.datasetConfiguration.getColumn("SessionDateDisplay").displayName}</th>
  <th>${searchResult.datasetConfiguration.getColumn("StartTime").displayName}</th>
  <th>${searchResult.datasetConfiguration.getColumn("CaseStyle").displayName}</th>
  <th>${searchResult.datasetConfiguration.getColumn("AppearanceReason").displayName}</th>
  <th>${searchResult.datasetConfiguration.getColumn("Judge").displayName}</th>
  
  
 </tr></thead>
 <tbody>
<#assign ResultId =0>
 <#foreach doc in searchResult.docs>
  
   <tr>
              <td><a href="${request.getContextPath()}/search.do?indexName=casedetails&templateName=casedetails&lq=${pageStyleUtil.encodeURL("CaseNumber:\"" + doc.get("CaseNumber") + "\"")}">${searchResult.highlight(doc, "CaseNumber")}</a></td>
              <td>${searchResult.highlight(doc, "CourtRoom")}</td>
              <td>${pageStyleUtil.format(date.format("yyyy/MM/dd HH:mm:ss z",doc.getDate("SessionDateDisplay")), "yyyy/MM/dd HH:mm:ss z", "MM/dd/yyyy")}</td>
              
              <td>${searchResult.highlight(doc, "StartTime")}</td>
              <td>${searchResult.highlight(doc, "CaseStyle")}</td>
              <td>${searchResult.highlight(doc, "AppearanceReason")}</td>
              <td>${searchResult.highlight(doc, "Judge")}</td>
   </tr>

 </#foreach>
<#else>
 <#include "noResults.stl"> 
</#if>
 </tbody>
</table>


<script type="text/javascript">
$(document).ready(function() {
	oTable = $('#searchResult').dataTable({
		"bJQueryUI": true,
		"bPaginate": false,
		"bLengthChange": false,
		"bFilter": false,
		"bSort": true,
		"bInfo": false,
		"bAutoWidth": false,
		"aaSorting": []
	});
} );
</script>
