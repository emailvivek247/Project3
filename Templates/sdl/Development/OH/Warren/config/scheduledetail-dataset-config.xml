  <dataquery name="Incremental">
    <sql><![CDATA[
SELECT DISTINCT
      A.COURT_SESSION_ID AS 'CourtSessionId'
      ,A.SESSION_ITEM_ID AS 'SessionItemId'
      ,A.CASE_ID AS 'CaseID'
      ,A.CASE_NUMBER AS 'CaseNumber'
      ,CONVERT(VARCHAR, A.ACTUAL_START_DT, 120) AS 'ActualStartDate'
      ,CONVERT(VARCHAR, ACTUAL_START_DT, 120) AS 'ActualStartDateDisp'
      ,CONVERT(VARCHAR, A.ACTUAL_END_DT, 120) AS 'ActualEndDate'
      ,CONVERT(VARCHAR, ACTUAL_END_DT, 120) AS 'ActualEndDateDisp'
      ,CONVERT(VARCHAR, A.SESSION_DATE, 120) AS 'Session Date'
      ,CONVERT(VARCHAR, SESSION_DATE , 120) AS 'SessionDateDisp'
      ,A.START_TIME AS 'StartTime'
      ,A.END_TIME AS 'EndTime'
      ,A.SEQ_NUM AS 'Seq'
      ,B.CASE_SHORT_TITLE AS 'CaseShortTitle'
      ,A.TRIAL_DAYS AS 'TrialDays'
      ,A.APPR_DESCRIPTION AS 'AppearanceReason'
      ,A.HEARING_DESC AS 'HearingResult'
      ,'' AS 'InterpreterRequired'
      ,'' AS 'inCustody'
      ,'' AS 'ADA_FLG'
      ,A.COURT_CD AS 'CourtCode'
      ,A.COURT_TYP_ID AS 'CourtTypeId'
      ,B.SEALED_CASE_FLG
      ,B.IS_RESTRICTED_FLG
      FROM ACC_V_SESSION_DETAIL A
      INNER JOIN ACM_V_FACTS_CASE B
      ON A.CASE_ID = B.CASE_ID AND B.SEALED_CASE_FLG = 'N' AND IS_RESTRICTED_FLG = 'N'
      WHERE DATE_TIME_CREATED > dateadd(hh,-1,getdate()) OR DATE_TIME_MOD > dateadd(hh,-1,getdate())
    ]]></sql>
  </dataquery>
