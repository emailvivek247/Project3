  <dataquery name="WorkingQueue" batch-size="128" is-skipping-null-parameters="false">
    <sql><![CDATA[
SELECT DISTINCT 
	 ISNULL(RTRIM(PT.CASE_ID), '|') AS 'CaseID'
	,ISNULL(C.OFFENSE_REF_NUM, '|') AS 'ChargeCode'
	,ISNULL(C.OFFENSE_DESC, '|') AS 'ChargeDescription'
	,ISNULL(C.OFFENSE_CLASS_DESC, '|') AS 'ChargeClass'
	,ISNULL(C.OFFENSE_TYP_DESC, '|') AS 'ChargeType'
	,CONVERT(VARCHAR(20), C.OFFENSE_DT_FRM, 101) AS 'OffenseDateFrom'
	,ISNULL(PS.DISPLAYNAME, '|') AS 'ChargeName'
	,ISNULL(C.DISPOSITION_DESC, '|') AS 'Disposition'
	,ISNULL(CONVERT(VARCHAR(20), C.DISPOSITION_DT, 101), '|') AS 'DispositionDate'
	,C.CHARGE_SEQ AS 'SeqNum'
	,C.DATE_TIME_CREATED 
	,C.DATE_TIME_MOD 
FROM  WN_ACM_V_CHARGE AS C 
      INNER JOIN ACM_PARTY PT ON C.PARTY_ID = PT.PARTY_ID AND C.DELETED_FLG <> 'Y' AND PT.VOID <> 'Y'
      LEFT OUTER JOIN ACM_PERSON PS ON PT.PERSON_ALIAS_ID = PS.PERSON_ALIAS_ID
      LEFT OUTER JOIN ACM_PARTY_ROLE_TYP PR ON PT.PARTY_ROLE_TYP_ID = PR.PARTY_ROLE_TYP_ID
      LEFT OUTER JOIN ACM_DISPOSITION DP ON C.DISPOSITION_ID = DP.DISPOSITION_ID
WHERE PS.SEALED_FLAG <> 'Y'
AND PT.SEALED_PARTY_FLG <> 'Y'
    ]]></sql>
