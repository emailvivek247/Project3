    <sql><![CDATA[
select 
 SESSION_START_DT as 'SessionStartDateFilter'
 ,CONVERT(VARCHAR(10), SESSION_START_DT, 112) as 'SessionStartDate'
,CONVERT(VARCHAR, SESSION_START_DT, 120) as 'SessionStartDateDisp'
,datepart(MM,SESSION_START_DT) as 'SESSION_START_DT_MM' 
,datepart(DD,SESSION_START_DT) as 'SESSION_START_DT_DD'
,SESSION_END_DT as 'SessionEndDateFilter'
,CONVERT(VARCHAR(10), SESSION_END_DT, 112) as 'SessionEndDate'
,CONVERT(VARCHAR, SESSION_END_DT, 120) as 'SessionEndDateDisp'
--,datepart(MM,SESSION_END_DT) as SESSION_END_DT_MM --
--,datepart(DD,SESSION_END_DT) as SESSION_END_DT_DD --
,SCHEDULED_CASE_NUM AS 'CaseNumber'
,SCHEDULED_MAX_NUM AS 'ScheduledMaxNum'
,COMMENT_TEXT AS 'Comment'
,COURT_ROOM_ID AS 'CourtRoomId'
,(SELECT P.DISPLAYNAME FROM ACM_PERSON P INNER JOIN ACM_JUDGE J ON P.PERSON_ALIAS_ID = J.PERSON_ALIAS_ID 
WHERE J.JUDGE_CD = ACC_V_COURT_ROOM_SESSION.JUDGE_CD) AS 'JudgeName'
,(SELECT P.DISPLAYNAME FROM ACM_PERSON P INNER JOIN ACM_JUDGE J ON P.PERSON_ALIAS_ID = J.PERSON_ALIAS_ID 
WHERE J.JUDGE_CD = ACC_V_COURT_ROOM_SESSION.JUDGE_CD) AS 'JudgeNameFilter'
,UPPER(COURT_ROOM_DESC) as 'CourtRoomDesc'
,COURT_SESSION_ID AS 'PK_KEY'
,INTERPRETER_NEEDED AS 'InterpreterNeeded'
,INCUSTODY_FLG AS 'InCustodyFlag'
,PARENT_SESSION_ID AS 'ParentSessionId'
,COURT_DAY_MON AS 'Monday'
,COURT_DAY_TUE AS 'Tuesday'
,COURT_DAY_WED AS 'Wednesday'
,COURT_DAY_THU AS 'Thursday'
,COURT_DAY_FRI AS 'Friday'
,REC_START_DT AS 'RecStartDate'
,datepart(MM,REC_START_DT) as REC_START_DT_MM --
,datepart(DD,REC_START_DT) as REC_START_DT_DD --
,REC_END_DT AS 'RecEndDate'
,datepart(MM,REC_END_DT) as REC_END_DT_MM --
,datepart(DD,REC_END_DT) as REC_END_DT_DD --
,ISREOCCUR AS 'Reoccuring'
,ORPHANED_SESSION_FLG AS 'OrphanSessionFlag'
,SESSION_TYP_DESCR AS 'SessionTypeDesc'
,PERSON_ALIAS_ID AS 'PersonAliasId'
,SESSION_DETAIL AS 'SessionDetails'
,SESSION_TYP_CD AS 'SessionTypeCode'
,PRIVATE_FLG AS 'PrivateFlag'
from ACC_V_COURT_ROOM_SESSION
    ]]></sql>
