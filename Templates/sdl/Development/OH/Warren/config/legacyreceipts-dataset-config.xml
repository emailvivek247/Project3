  <dataquery name="WorkingQueue" batch-size="128" is-skipping-null-parameters="false">
    <sql><![CDATA[
SELECT
 b.dscr AS 'CaseNumber'
,a.rec_nbr AS 'ReceiptNumber'
,a.received_from AS 'ReceivedFrom'
,CONVERT(VARCHAR(10), a.rec_dt, 112) AS 'ReceiptDateSearch'
,CONVERT(VARCHAR(30), a.rec_dt, 101) AS 'ReceiptDateDisplay'
,a.rec_dt AS 'ReceiptDateSort'
,a.ins_by AS 'ReceiptedBy ' -- check w/julie
,a.payment_amt AS 'PaymentAmount'
,ltrim(rtrim(c.ref_nbr)) AS 'ReferenceNumber'
 from REC a
inner join real_case b
on a.case_id = b.case_id
left join rectend c
on c.rec_id = a.rec_id
    ]]></sql>
