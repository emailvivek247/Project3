<br /><br />

<#if searchResult.docs?has_content>

<table id="searchResult" class="tablesorter">
     <thead>
 	<tr>
	  <th>${searchResult.datasetConfiguration.getColumn("CaseNumber").displayName}</th>
	  <th>${searchResult.datasetConfiguration.getColumn("CaseShortTitle").displayName}</th>
	  <th>${searchResult.datasetConfiguration.getColumn("AppearanceReason").displayName}</th>
	  <th>${searchResult.datasetConfiguration.getColumn("Session Date").displayName}</th>
	  <th>${searchResult.datasetConfiguration.getColumn("StartTime").displayName}</th>
	  <th>${searchResult.datasetConfiguration.getColumn("EndTime").displayName}</th>
	  <th>${searchResult.datasetConfiguration.getColumn("TrialDays").displayName}</th>
	  <th>${searchResult.datasetConfiguration.getColumn("HearingResult").displayName}</th>
	  <th>${searchResult.datasetConfiguration.getColumn("InterpreterRequired").displayName}</th>
  	</tr>
     </thead>
 <tbody>
<#assign ResultId =0>
 <#foreach doc in searchResult.docs>
  <#include "document.stl">
 </#foreach>
<#else>
 <#include "noResults.stl"> 
</#if>
 </tbody>
</table>


<script type="text/javascript">
$(function() {      
    $("#searchResult").tablesorter({widgets: ['zebra']});
}); 
</script>


