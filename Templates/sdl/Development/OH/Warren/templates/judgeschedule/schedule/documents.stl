<div id='calendar' style="height:90%"></div>
<#if searchResult.docs?has_content>
<script type='text/javascript'>
	$(document).ready(function() {	
                $('#calendar').fullCalendar({
			events: [
                                <#assign iCount = 0>  
 				<#foreach doc in searchResult.docs>				
                                <#assign iCount = iCount + 1>  
				{
					title: '${doc.get("JudgeName")} (${doc.get("CaseNumber")}/${doc.get("ScheduledMaxNum")})',
					start: '${doc.get("SessionStartDateDisp")}',
					end: '${doc.get("SessionEndDateDisp")}',
                                        allDay: false,
                                        id:'${doc.get("PK_KEY")}'  
                                        //,url: 'search.do?indexName=scheduledetail&templateName=details&lq=%23CourtSessionId%3A%22${doc.get("PK_KEY")}%22'
				}<#if iCount &lt; searchResult.total>,</#if>
                                </#foreach>
			],
                        loading: function(bool) {
				if (bool) {
					$('#loading').show();
				}else{
					$('#loading').hide();
				}
			},
                        header: {
				left: 'prev,next today',
				center: 'title',
				right: 'month,agendaWeek,agendaDay'
			},
			eventRender: function(event, element) {       
				var tipContent = '<iframe width="100%" height="100%" src="search.do?indexName=scheduledetail&templateName=details&lq=%23CourtSessionId%3A%22' + event.id + '%22" />';			
				element.qtip({
					//content: $('<<span class="highlight" style="padding-left: 0px; padding-right: 0px;">iframe</span> src="index.html">'), 
					content: tipContent,
						position: {
							corner: {
								tooltip: 'bottomMiddle', 
								target: 'topMiddle' 
							}
						},
					show: {
						when: 'click', 
						solo: true 
					},
					hide: 'unfocus',
					style: {
						width: 700,
						height: 264,
						padding: 0,
						tip: true,
						name: 'dark'
					},
					effect: { 
						type: 'slide' 
					}
				});
			},
			aspectRatio: 2,
			theme: true,
			minTime: 7,
			maxTime: 21,
			buttonText: {		              
			today: 'Today',
			month: 'Month',
			week: 'Week',
			day: 'Day'
			}  
		});
	});
</script>
<#else>
 <#include "noResults.stl"> 
</#if>