<script type="text/javascript">
    function unique(arrayName) {
        var newArray=new Array();
        label:for(var i=0; i<arrayName.length;i++ )
        {  
            for(var j=0; j<newArray.length;j++ ) {
                if(newArray[j].name == arrayName[i].name) 
                 continue label;
            }
            setCookie('TEST' + arrayName[i].name, jQuery.trim(arrayName[i].value), 0);
            if (arrayName[i].type == 'text' && jQuery.trim(arrayName[i].value).length != 0) {
                newArray[newArray.length] = arrayName[i];
            }
        }
            setCookie('TESTfrmCourtRoomId', document.getElementById('frmCourtRoomId').value, 0);
            setCookie('TESTtoCourtRoomId',  document.getElementById('toCourtRoomId').value, 0);
            setCookie('TESTfrmPK_KEY', document.getElementById('frmPK_KEY').value, 0);
            setCookie('TESTtoPK_KEY',  document.getElementById('toPK_KEY').value, 0);
            setCookie('TESTfrmPersonAliasId', document.getElementById('frmPersonAliasId').value, 0);
            setCookie('TESTtoPersonAliasId',  document.getElementById('toPersonAliasId').value, 0);
            setCookie('TESTfrmSessionId', document.getElementById('frmSessionId').value, 0);
            setCookie('TESTtoSessionId',  document.getElementById('toSessionId').value, 0);
        return newArray;
    }   
    function postAdvancedSearch() {
        try {
                    	if (!doNumericCheck('frmCourtRoomId','toCourtRoomId'))return;          
                    	if (!doNumericCheck('frmPK_KEY','toPK_KEY'))return;          
                    	if (!doNumericCheck('frmPersonAliasId','toPersonAliasId'))return;          
                    	if (!doNumericCheck('frmSessionId','toSessionId'))return;          
            document.getElementById("q").value = '';
            var inputElemenets =  $('input[type="text"]');
            var elem = unique(inputElemenets);
            var searchString = '';
            var searchQuery = '';
            var andOrClause ='';
            var doubleQuotes = '"';
            var counter = 0;
            var textcounter = 0;
            var searchValue = '';
            var searchValueQuery = '';
            for(var i = 0; i < elem.length; i++) {
                if (document.getElementById(elem[i].name + '_select') != null) {
                    counter++;  
                }                   
            }
            for(var i = 0; i < elem.length; i++) {
                if (document.getElementById(elem[i].name + '_select') != null) {
                    setCookie('TEST' + elem[i].name + '_select', document.getElementById(elem[i].name + '_select').selectedIndex, 0);
                    textcounter++
                    if (textcounter == counter) {
                        andOrClause = '';
                    } else {
                        andOrClause = ' ' + document.getElementById(elem[i].name + '_select').value + '  ';
                    }
                     if (elem[i].name == 'CourtRoomId'|| elem[i].name == 'PK_KEY'|| elem[i].name == 'PersonAliasId'|| elem[i].name == 'SessionId') {
                    	searchValue = getRangeQuery(elem[i].name, "Inclusive");
                    	searchValueQuery = getRangeSearchQuery(elem[i].name, "Inclusive");
                     }
                    else {
                        searchValue = elem[i].name + ':' + doubleQuotes + jQuery.trim(elem[i].value) + doubleQuotes + '~5';
                        searchValue = '#' + searchValue + '#';
                        searchValueQuery = elem[i].name + ':' + doubleQuotes + jQuery.trim(elem[i].value) + doubleQuotes;
                    }
                    searchString = searchString + searchValue + andOrClause;
                    searchQuery = searchQuery  + searchValueQuery + andOrClause;
                }
            }
            document.getElementById("lq").value = searchString;
            document.getElementById("searchQuery").value = searchQuery;
            document.search_${searchResult.indexName}.method = "get";
            document.search_${searchResult.indexName}.action = "search.do";
            document.search_${searchResult.indexName}.searchType.value = "2";           
            document.search_${searchResult.indexName}.submit();
        } catch (err) {
            $.prompt(err.message,{buttons:{Ok:true},	prefix:'extblue'});
        }
    }   
    function postBasicSearch() {
        try {   
            setCookie('TEST' + document.getElementById("q").name, document.getElementById("q").value, 0);
            document.getElementById("lq").value = '';
            document.search_${searchResult.indexName}.method = "get";
            document.getElementById('searchQuery').value = document.getElementById('q').value
            document.search_${searchResult.indexName}.action = "search.do";
            document.search_${searchResult.indexName}.searchType.value = "1";
            document.search_${searchResult.indexName}.submit();
        } catch (err) {
            $.prompt(err.message,{buttons:{Ok:true},	prefix:'extblue'});
        }
    } 
</script>
