
<#if searchResult.docs?has_content>
<table id="searchResult" class="tablesorter">
 <thead><tr>
  <th>${searchResult.datasetConfiguration.getColumn("AppearanceReason").displayName}</th><th>${searchResult.datasetConfiguration.getColumn("AttorneyName").displayName}</th><th>${searchResult.datasetConfiguration.getColumn("CaseNumber").displayName}</th><th>${searchResult.datasetConfiguration.getColumn("CourtRoomDesc").displayName}</th><th>${searchResult.datasetConfiguration.getColumn("CourtRoomId").displayName}</th><th>${searchResult.datasetConfiguration.getColumn("HearingResult").displayName}</th><th>${searchResult.datasetConfiguration.getColumn("IsRestrictedFlag").displayName}</th><th>${searchResult.datasetConfiguration.getColumn("JudgeName").displayName}</th><th>${searchResult.datasetConfiguration.getColumn("PK_KEY").displayName}</th><th>${searchResult.datasetConfiguration.getColumn("PersonAliasId").displayName}</th><th>${searchResult.datasetConfiguration.getColumn("SealedCaseFlag").displayName}</th><th>${searchResult.datasetConfiguration.getColumn("SessionEndDate").displayName}</th><th>${searchResult.datasetConfiguration.getColumn("SessionId").displayName}</th><th>${searchResult.datasetConfiguration.getColumn("SessionStartDate").displayName}</th><th>${searchResult.datasetConfiguration.getColumn("obj_type").displayName}</th>
  
  
  
 </tr></thead>
 <tbody>
<#assign ResultId =0>
 <#foreach doc in searchResult.docs>
  <#include "document.stl">
 </#foreach>
<#else>
 <#include "noResults.stl"> 
</#if>
 </tbody>
</table>


<script type="text/javascript">
$(function() {      
    $("#searchResult").tablesorter({widgets: ['zebra']});
}); 
</script>
