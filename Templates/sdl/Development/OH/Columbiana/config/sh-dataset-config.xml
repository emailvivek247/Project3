  <dataquery name="Incremental">
    <sql><![CDATA[
select distinct
         CASE WHEN LTRIM(RTRIM(st.SESSION_TYP_DESCR)) = '' THEN '|' ELSE ISNULL(st.SESSION_TYP_DESCR, '|') END AS 'HearingDesc'
        ,CASE WHEN LTRIM(RTRIM(siar.APPR_DESCRIPTION)) = '' THEN '|' ELSE ISNULL(siar.APPR_DESCRIPTION, '|') END AS 'AppDesc'
	,CASE WHEN LTRIM(RTRIM(siar.HEARING_DESC)) = '' THEN '|' ELSE ISNULL(siar.HEARING_DESC, '|') END AS 'HearingResult'
	,j.DISPLAYNAME AS 'Judge'
	,CONVERT(VARCHAR,cs.SESSION_START_DT,120) AS 'SessionDate'
	,si.SESSION_ITEM_ID
	,FC.CASE_ID AS 'CaseID'
	,FC.CASE_NUMBER AS 'CaseNumber'
FROM ACC_COURT_SESSION cs
	inner join ACC_SESSION_ITEM si on si.COURT_SESSION_ID = cs.COURT_SESSION_ID
	inner join ACC_SESSION_TYP st on st.SESSION_TYP_ID = cs.SESSION_TYP_ID
	inner join SW_ACC_V_SESN_ITM_APPR_RESN siar on siar.SESSION_ITEM_ID =si.SESSION_ITEM_ID
	inner join WN_ACM_V_JUDGE j on j.JUDGE_CD = cs.JUDGE_CD
	INNER JOIN ACM_FACTS_CASE FC ON SI.CASE_ID = FC.CASE_ID
WHERE FC.SEALED_CASE_FLG <> 'Y'
AND FC.IS_RESTRICTED_FLG <> 'Y'
AND FC.EXPUNGE_FLG <> 'Y'
AND cs.DATE_TIME_CREATED > dateadd(hh,-1,getdate()) OR cs.DATE_TIME_MOD > dateadd(hh,-1,getdate())
