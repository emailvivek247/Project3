

<#assign display=JspTaglibs["http://displaytag.sf.net"]>


<#if searchResult.docs?has_content>
    <#assign ResultId =0>
	<@display.table name="searchResult.docs" id="doc" export=true requestURI="/search.do" excludedParams="searchQuery searchType CaseID CaseID_select ChargeClass ChargeClass_select ChargeCode ChargeCode_select ChargeDescription ChargeDescription_select ChargeName ChargeName_select ChargeType ChargeType_select Disposition Disposition_select DispositionDate DispositionDate_select OffenseDateFrom OffenseDateFrom_select OffenseDateTo OffenseDateTo_select ">
	  <#include "document.stl">
	</@display.table>
<#else>
 <#include "noResults.stl"> 
</#if>


<script type="text/javascript">
$(function() {      
    $("#doc").tablesorter({widgets: ['zebra']});
}); 
</script>
