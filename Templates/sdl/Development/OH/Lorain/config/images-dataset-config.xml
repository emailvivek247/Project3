  <dataquery name="WorkingQueue" batch-size="128" is-skipping-null-parameters="false">
    <sql><![CDATA[
SELECT 
	B.STITCHED_PATH+CAST(DI.DOCUMENT_NAME AS VARCHAR)+'.PDF' AS 'Path'
   ,DS.EVENT_ENTRY_ID AS 'EventEntryID'
   ,DS.CASE_ID AS 'CaseID'
FROM BATCH B INNER JOIN BATCHACCESS BA 
ON B.BATCH_ID = BA.BATCH_ID
INNER JOIN SPLIT_BATCH_ACCESS SBA ON SBA.BATCHACCESS_ID = BA.BATCHACCESS_ID INNER JOIN DOCUMENT_INFORMATION DI 
ON DI.SPLIT_BATCH_ACCESS_ID = SBA.ID
inner join DOCUMENT_STORAGE DS
on DI.ID = DS.EXTERNAL_DMS_ID
WHERE DS.SEALED_DOC_FLG <> 'Y'
AND DS.IS_RESTRICTED_FLG <> 'Y'
AND DS.IS_VOID <> 'Y'
AND DS.REMOVE_PUBLIC_ACCESS_FLG <> 'Y'
AND ISNULL(BA.IS_LOCKED, '') <> 'Y'
    ]]></sql>
