  <dataquery name="WorkingQueue" batch-size="128" is-skipping-null-parameters="false">
    <sql><![CDATA[
select distinct 
       b.PARTY_ID AS 'PartyID'
      ,PS.DISPLAYNAME AS 'DisplayName'
      ,PS.DISPLAYNAME AS 'PhoneticName'
      ,(select CASE_STATUS_DESCR from ACM_CASE_STATUS_TYP where CASE_STATUS_CD = PS.CASE_STATUS_CD) AS 'CaseStatusDesc'
      ,PS.CASE_STATUS_CD AS 'CaseStatusCode'
      ,PS.CASE_ID AS 'CaseID'
      ,PS.CASE_NUMBER AS 'CaseNumber'
      ,CONVERT(VARCHAR(10), PS.DATEOFBIRTH, 112) AS 'DOB'
      ,CONVERT(VARCHAR(30), PS.DATEOFBIRTH, 101) AS 'DOBDisplay'
      ,PS.DATEOFBIRTH AS 'DOBFilter'
      ,PS.PARTY_ROLE_DESC AS 'PartyRole'
      ,PS.PARTY_ROLE_DESC AS 'PartyRoleFilter'
      ,PS.CASE_TYP_CD AS 'CaseTypeCode'
      ,(select CASE_TYP_DESC from ACM_CASE_TYP where CASE_TYP_ID = a.CASE_TYP_ID)AS 'CaseType'
      ,(select CASE_TYP_DESC from ACM_CASE_TYP where CASE_TYP_ID = a.CASE_TYP_ID)AS 'CaseTypeFilter'
      ,isnull(w.ISACTIVE,'N')  AS 'ActiveWarrantFlag'
      ,(select COURT_CD from ACM_COURT_TYP where COURT_TYP_ID = a.COURT_TYP_ID)AS 'CourtCode'
      ,(select COURT_NAME from ACM_COURT_TYP where COURT_TYP_ID = a.COURT_TYP_ID)AS 'CourtType'
      ,x.OFFENSE_REF_NUM AS 'StatuteNumber'
      ,x.CHARGE_DESC AS 'StatuteDescription'
      ,x.COMPLAINT_NUM AS 'CitationNumber'
      ,x.CHARGE_SEQ AS 'SeqNum'
,CONVERT(VARCHAR(10),a.CASE_FILING_DT, 112) AS 'CaseFilingDate'
,w.WARRANT_TYP_ID
from 
ACM_FACTS_CASE a
inner join ACM_PARTY b
on a.CASE_ID = b.CASE_ID
inner join PARTYSEARCH ps
on ps.PARTY_ID = b.PARTY_ID
left join ACM_WARRANT w
on w.PARTY_ID = b.PARTY_ID
left join (select a.PARTY_ID,d.OFFENSE_REF_NUM,c.CHARGE_DESC,c.CHARGE_SEQ,e.COMPLAINT_NUM from ACM_PARTY a
inner join ACM_CHARGE_PARTY b
on a.PARTY_ID = b.PARTY_ID
inner join ACM_CHARGE c
on c.CHARGE_ID = b.CHARGE_ID
inner join ACM_STATUTE d
on d.STATUTE_ID = c.STATUTE_ID
left join ACM_CITATION e
on e.ID = c.CITATION_ID) x
on x.party_id = b.party_id
left join 
(select distinct case_id from acm_warrant a
inner join acm_party b
on a.party_id = b.party_id
where a.warrant_typ_id = 10) z
on a.case_id = z.case_id
where z.case_id is null
and ISNULL(a.SEALED_CASE_FLG, '') <> 'Y'
AND ISNULL(a.VOID, '') <> 'Y'
AND ISNULL(PS.VOID, '') <> 'Y'
AND ISNULL(a.IS_RESTRICTED_FLG, '') <> 'Y'
AND ISNULL(PS.IS_RESTRICTED_FLG, '') <> 'Y'
AND ISNULL(a.EXPUNGE_FLG, '') <> 'Y'
AND PS.CASE_TYP_CD NOT IN ('CRSLD','JRSLD','MSSLD','PB','PBSLD','TRSLD')
AND ISNULL(a.SEALED_CASE_FLG, '') <> 'Y'
AND ISNULL(PS.SEALED_FLAG, '') <> 'Y'
AND ISNULL(b.RSTRCD_PARTY_FLAG, '') <> 'Y'
AND PS.PARTY_ROLE_TYP_CD NOT IN ('VICT','VICTIM_REP','VICWIT','WIT','W','V')
AND ISNULL(b.JUVENILE_FLG, '') <> 'Y'
--AND ISNULL(w.WARRANT_TYP_ID,'') <> '10'
AND ISNULL(PS.CONFIDENTIAL_FLAG, '') <> 'Y'
    ]]></sql>
