  <dataquery name="Deletion" is-delete-only="true">
    <sql><![CDATA[
SELECT RT.TAB_PK_KEY
FROM ROAM_TRACK RT
WHERE 
RT.DATE_TIME_MOD > DATEADD(mi, -10, GETDATE()) AND 
RT.TAB_NAME = 'ACM_PARTY' AND
RT.DML_TYP IN ('D', 'U') 
UNION ALL
SELECT PT.PARTY_ID
FROM ACM_PARTY PT INNER JOIN ROAM_TRACK RT 
ON PT.CASE_ID = RT.PK_KEY
WHERE 
RT.DATE_TIME_MOD > DATEADD(mi, -10, GETDATE()) AND 
RT.DML_TYP IN ('D', 'U') AND 
(PT.VOID = 'Y')
    ]]></sql>
  </dataquery>
  <dataquery name="Incremental">
    <sql><![CDATA[
SELECT DISTINCT 
PERSON_ALIAS_ID AS 'PersonSPN',
PARTY_ID AS 'PartyID',
CASE WHEN LTRIM(RTRIM(RSTRCD_PARTY_FLAG)) = '' THEN 'N' ELSE ISNULL(RSTRCD_PARTY_FLAG, 'N') END AS 'PartyConfidential',
RT.DATE_TIME_MOD, RT.ID
FROM ACM_PARTY PT INNER JOIN ROAM_TRACK RT
ON PT.party_ID = RT.tab_pk_key
WHERE VOID <> 'Y'
AND RT.DML_TYP IN ('I', 'U')
AND (RT.DATE_TIME_MOD > ? OR RT.DATE_TIME_MOD > DATEADD(mi, -15, GETDATE()))
AND RT.TAB_NAME = 'ACM_PARTY'
ORDER BY RT.DATE_TIME_MOD DESC
    ]]></sql>
