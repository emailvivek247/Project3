  <dataquery name="WorkingQueue" batch-size="128" is-skipping-null-parameters="false">
    <sql><![CDATA[
SELECT DISTINCT
             CS.PARTY_ID AS 'PartyID',
             CS.CASE_NUMBER AS 'CaseNumber',
             CS.CASE_NUMBER AS 'CaseNumberSort',
             CS.CASE_ID AS 'CaseID',
  	     CS.CASE_STATUS AS 'CaseStatus',
  	     CS.CASE_STATUS AS 'CaseStatusFilter',
             LTRIM(RTRIM(CS.LAST_NAME)) AS 'LastName',
             LTRIM(RTRIM(CS.FIRST_NAME)) AS 'FirstName',
             LTRIM(RTRIM(CS.MIDDLE_NAME)) AS 'MiddleName',
      	     LTRIM(RTRIM(CS.DISPLAYNAME)) AS 'Party',
      	     LTRIM(RTRIM(CS.SUFFIX_NAME)) AS 'SuffixName',
             CS.PARTY_ROLE_DESC AS 'PartyType',
             CS.PARTY_ROLE_DESC AS 'PartyRoleFilter',
      	     CS.COURT_CD AS 'CourtType',
      	     CS.CASE_TYPE AS 'CaseType',
             CS.CASE_TYPE AS 'FilterByCaseType',
             CS.COURT_CD AS 'FilterByCourtType',
      	     CONVERT(varchar(10), CS.CASE_FILING_DT, 112) AS 'CaseFilingDate',
      	     CONVERT(varchar(30), CS.CASE_FILING_DT, 120) AS 'DisplayCaseFilingDate',
      	     CS.CASE_FILING_DT AS 'SortCaseFilingDate',
      	     CS.CATEGORY_TYPE AS 'CategoryType',
      	     CS.CASE_SHORT_TITLE AS 'CaseStyle',
      	     CONVERT(varchar(20), CS.NEXT_SESSION_DATE, 100) AS 'HearingDate',
             CS.CASE_SEALED AS 'CaseSealed',
             CS.PERSON_SEALED AS 'PersonSealed',
             CS.PARTY_RESTRICTED AS 'PartyRestricted',
             CS.CASE_RESTRICTED AS 'CaseRestricted',
             PS.SSN AS 'SSN',
             RIGHT(PS.SSN,4) AS 'SSNDisplay',
             CS.DATE_TIME_MOD
FROM  ACM_V_ESCASESEARCH CS
LEFT JOIN PARTYSEARCH PS ON PS.CASE_ID = CS.CASE_ID
WHERE (CASE_SEALED = 'N' AND PERSON_SEALED = 'N' AND CASE_RESTRICTED = 'N')
AND CS.PARTY_ROLE_DESC IN ('PLAINTIFF', 'DEFENDANT')
AND CS.COURT_CD IN ('CRIMINAL', 'CIVIL')
AND CS.CASE_TYPE NOT IN ('APPEAL FROM J & DR COURT')
AND ISNULL(CS.CASE_VOID,'') <> 'Y'
AND ISNULL(CS.CASE_RESTRICTED,'') <> 'Y'
AND ISNULL(CS.CASE_SEALED,'') <> 'Y'
AND ISNULL(CS.PARTY_RESTRICTED,'') <> 'Y'
AND ISNULL(CS.PARTY_VOID,'') <> 'Y'
AND ISNULL(CS.PERSON_SEALED,'') <> 'Y'
ORDER BY CS.DATE_TIME_MOD DESC
    ]]></sql>
