  <dataquery name="Deletion" is-delete-only="true">
    <sql><![CDATA[
SELECT RT.TAB_PK_KEY
FROM ROAM_TRACK RT
WHERE 
RT.DATE_TIME_MOD > DATEADD(mi, -10, GETDATE()) and 
RT.DML_TYP IN ('D', 'U') AND RT.TAB_NAME = 'ACM_PARTY'
UNION ALL
SELECT ES.PARTY_ID
FROM ACM_V_ESCASESEARCH ES INNER JOIN ROAM_TRACK RT
ON ES.CASE_ID = RT.PK_KEY
WHERE 
RT.DATE_TIME_MOD > DATEADD(mi, -10, GETDATE()) AND 
RT.DML_TYP IN ('D', 'U') AND 
((ES.CASE_SEALED = 'Y' OR ES.PERSON_SEALED = 'Y' AND COURT_CD IN ('CRIMINAL','CIVIL')) OR CASE_VOID = 'Y' OR PARTY_VOID = 'Y')
    ]]></sql>
  </dataquery>
  <dataquery name="Incremental">
    <sql><![CDATA[
SELECT DISTINCT
             PARTY_ID AS 'PartyID',
             CASE_NUMBER AS 'CaseNumber',
             CASE_ID AS 'CaseID',
  	CASE_STATUS AS 'CaseStatus',
             LAST_NAME AS 'LastName',
             FIRST_NAME AS 'FirstName',
             MIDDLE_NAME AS 'MiddleName',
      	DISPLAYNAME AS 'Party',
             SUFFIX_NAME AS 'SuffixName',
             PARTY_ROLE_DESC AS 'PartyType',
      	COURT_CD AS 'CourtType',
      	CASE_TYPE AS 'CaseType',
             CASE_TYPE AS 'FilterByCaseType',
             COURT_CD AS 'FilterByCourtType',
      	CONVERT(varchar(10), CASE_FILING_DT, 112) AS 'CaseFilingDate',
      	CATEGORY_TYPE AS 'CategoryType',
      	CASE_SHORT_TITLE AS 'CaseStyle',
      	CONVERT(varchar(20), NEXT_SESSION_DATE, 100) AS 'HearingDate',
             CASE_SEALED AS 'CaseSealed',
             PERSON_SEALED AS 'PersonSealed',
             PARTY_RESTRICTED AS 'PartyRestricted',
             CASE_RESTRICTED AS 'CaseRestricted',
             RT.DATE_TIME_MOD
FROM ACM_V_ESCASESEARCH CS INNER JOIN ROAM_TRACK RT
ON CS.CASE_ID = RT.PK_KEY
WHERE (CASE_SEALED = 'N' AND PERSON_SEALED = 'N' AND CASE_RESTRICTED = 'N')
AND CASE_VOID = 'N'
AND PARTY_VOID = 'N'
AND PARTY_ROLE_DESC IN ('PLAINTIFF', 'DEFENDANT')
AND COURT_CD IN ('CRIMINAL', 'CIVIL')
AND RT.DML_TYP IN ('I', 'U')
AND (RT.DATE_TIME_MOD > ? OR RT.DATE_TIME_MOD > DATEADD(mi, -15, GETDATE()))
ORDER BY RT.DATE_TIME_MOD DESC
    ]]></sql>
  </dataquery>
