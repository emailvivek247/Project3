  <dataquery name="WorkingQueue" batch-size="128" is-skipping-null-parameters="false">
    <sql><![CDATA[
SELECT DISTINCT
	 VAP.PARTY_DISPLAYNAME AS 'PartyName'
	,VAP.PARTY_DISPLAYNAME AS 'PartyNameSort'
	,VAP.ATTORNEY_DISPLAYNAME AS 'AttorneyName'
	,VAP.PARTY_ROLE_DESC AS 'PartyRole'
        ,VAP.PARTY_ID AS 'PartyID'
	,CS.CASE_NUMBER AS 'CaseNumber'
	,CS.CASE_NUMBER AS 'CaseNumberSort'
	,CS.CASE_ID AS 'CaseID'
	,CS.COURT_CD AS 'CourtType'
	,CS.COURT_CD AS 'CourtTypeFilter'
	,CONVERT(VARCHAR(10), VC.SESSIONDATE, 112) AS 'CourtDate'
	,VC.SESSIONDATE AS 'CourtDateFilter'
	,VC.REASON AS 'AppReason'
	,VC.COURT_ROOM_DESC AS 'CourtRoom'
FROM ACM_V_CASESEARCH CS
LEFT JOIN ACM_V_ATTORNEY_PARTY VAP ON CS.CASE_ID = VAP.CASE_ID
LEFT JOIN ACM_V_COURTSESSIONS VC ON CS.CASE_ID = VC.CASE_ID
WHERE CS.CASE_TYP_CD NOT IN ('JAPL')
AND ISNULL(CS.SEALED_CASE_FLAG,'') <> 'Y'
AND ISNULL(CS.SEALED_FLAG, '') <> 'Y'
AND ISNULL(CS.VOID,'') <> 'Y'
AND ISNULL(CS.IS_RESTRICTED_CASE_FLG, '') <> 'Y'
AND ISNULL(CS.IS_RESTRICTED_FLG, '') <> 'Y'
    ]]></sql>
