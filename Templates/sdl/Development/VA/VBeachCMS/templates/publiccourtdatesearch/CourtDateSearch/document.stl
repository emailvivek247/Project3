<@display.column media="html" title="${searchResult.datasetConfiguration.getColumn(\"CaseNumber\").displayName}">
    <#if doc.get("CourtType") = "CIVIL">
       <a href="${request.getContextPath()}/search.do?indexName=publicdetail&templateName=CivilDetail&lq=${pageStyleUtil.encodeURL("CaseID:\"" + doc.get("CaseID") + "\"")}">${searchResult.highlight(doc, "CaseNumber")}</a>             
    <#else>
       <a href="${request.getContextPath()}/search.do?indexName=publicdetail&templateName=CriminalDetail&lq=${pageStyleUtil.encodeURL("CaseID:\"" + doc.get("CaseID") + "\"")}">${searchResult.highlight(doc, "CaseNumber")}</a> 
     </#if>
</@display.column>

<@display.column  media="rtf excel pdf"  title="${searchResult.datasetConfiguration.getColumn(\"CaseNumber\").displayName}">
                ${doc.get("CaseNumber")}
</@display.column>



