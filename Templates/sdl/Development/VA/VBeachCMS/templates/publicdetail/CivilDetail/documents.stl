<#assign caseCounter = 0>

<#if searchResult.docs?has_content>
    <#foreach doc in searchResult.docs>
	<#foreach CaseID in doc.getValues("CaseID")>
	   <#assign caseCounter = 1>	
	   <#break>
	</#foreach>
    </#foreach>
</#if>


<#if searchResult.docs?has_content>


<b>General Case Information</b>
<#if (caseCounter > 0)>
<table id="case" class="tablesorter">
 <thead><tr>
  <th class="ui-state-default">${searchResult.datasetConfiguration.getColumn("CaseNumber").displayName}</th>
  <th class="ui-state-default">${searchResult.datasetConfiguration.getColumn("CaseFilingDate").displayName}</th>
 </tr></thead>
 <tbody>

<#foreach doc in searchResult.docs>
    
    <#assign CaseFilingDate = doc.getValues("CaseFilingDate")>
    <#assign loopCounter = 0>

    <#foreach CaseNumber in doc.getValues("CaseNumber")>
	<tr>
       
	<td>${CaseNumber}</td>
	
	
	
	<td>${CaseFilingDate[loopCounter]}</td> 
                         <#assign loopCounter = loopCounter + 1>
	</tr>
     </#foreach>

</#foreach>

 </tbody>
</table>
<#else>
<br>
<b><i >No information available.</i></b>
<br><br>
</#if>
</#if>






