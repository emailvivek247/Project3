<#if searchResult.docs?has_content>


<b>Parties</b>
<#if (partiesCounter > 0)>
<table id="parties" class="tablesorter">
 <thead><tr>
  <th class="ui-state-default">${searchResult.datasetConfiguration.getColumn("DetailDisplayName").displayName}</th>
  <th class="ui-state-default">${searchResult.datasetConfiguration.getColumn("DetailPartyRole").displayName}</th>
  <th class="ui-state-default">${searchResult.datasetConfiguration.getColumn("Attorney").displayName}</th>
  <th class="ui-state-default">${searchResult.datasetConfiguration.getColumn("AttorneyRole").displayName}</th>
   </tr></thead>
 <tbody>

<#foreach doc in searchResult.docs>
    
    <#assign DetailPartyRole = doc.getValues("DetailPartyRole")>
 <#assign Attorney = doc.getValues("Attorney")>
 <#assign AttorneyRole = doc.getValues("AttorneyRole")>
       <#assign PartyID = doc.getValues("PartyID")>  
   
    <#assign loopCounter = 0>

    <#foreach DetailDisplayName in doc.getValues("DetailDisplayName")>
	<tr>
       
	<td>${DetailDisplayName} </td>
	<td>${DetailPartyRole[loopCounter]}</td> 
	<td>${Attorney[loopCounter]}</td>
	<td>${AttorneyRole[loopCounter]}</td>
                                   
             <#assign loopCounter = loopCounter + 1>
	</tr>
     </#foreach>

</#foreach>

 </tbody>
</table>
<#else>
<br>
<b><i >No information available.</i></b>
<br><br>
</#if>
</#if>


