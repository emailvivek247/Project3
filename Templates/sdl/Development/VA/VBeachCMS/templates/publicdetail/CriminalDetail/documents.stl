<#if searchResult.docs?has_content>


<b>Charge Information</b>
<#if (chargeCounter > 0)>
<table id="charges" class="tablesorter">
 <thead><tr>
  <th class="ui-state-default">${searchResult.datasetConfiguration.getColumn("ChargeCount").displayName}</th>
  <th class="ui-state-default">${searchResult.datasetConfiguration.getColumn("ChargeDescription").displayName}</th>
  <th class="ui-state-default">${searchResult.datasetConfiguration.getColumn("OffenseType").displayName}</th>	
  <th class="ui-state-default">${searchResult.datasetConfiguration.getColumn("OffenseDate").displayName}</th>
  <th class="ui-state-default">${searchResult.datasetConfiguration.getColumn("Disposition").displayName}</th>
  <th class="ui-state-default">${searchResult.datasetConfiguration.getColumn("DispositionDate").displayName}</th>

   </tr></thead>
 <tbody>

<#foreach doc in searchResult.docs>
    
	<#assign chargeDescription = doc.getValues("ChargeDescription")>	
	<#assign OffenseType = doc.getValues("OffenseType")>
	<#assign OffenseDate = doc.getValues("OffenseDate")>
	<#assign Disposition = doc.getValues("Disposition")>
	<#assign DispositionDate = doc.getValues("DispositionDate")>

    <#assign loopCounter = 0>

    <#foreach chargeCount in doc.getValues("ChargeCount")>
	<tr>
	<td>${chargeCount}</td>
	<td>${chargeDescription[loopCounter]}</td>
	<td>${OffenseType[loopCounter]}</td>
	<td>${OffenseDate[loopCounter]}</td>
	<td>${Disposition[loopCounter]}</td>
	<td>${DispositionDate[loopCounter]?replace('01/01/1753', '&nbsp;')}</td>

                          <#assign loopCounter = loopCounter + 1>
	</tr>
     </#foreach>

</#foreach>

 </tbody>
</table>
<#else>
<br>
<b><i >No information available.</i></b>
<br><br>
</#if>
</#if>

