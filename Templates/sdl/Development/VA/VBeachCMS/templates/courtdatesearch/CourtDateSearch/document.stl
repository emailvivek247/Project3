<#assign urlEncoder = new("net.javacoding.xsearch.utility.EscapeChars")>
<#assign searchable = request.getParameter("searchable")>
<#assign search = "ScheduledParty,CaseNumber,SessionDate,PartyRole,AppearanceReason,HearingResult,CourtType">



<@display.column media="html" title="${searchResult.datasetConfiguration.getColumn(\"CaseNumber\").displayName}">
                  <#if doc.get("CourtType") = "CIVIL">
                  <a href="${request.getContextPath()}/search.do?indexName=detail&templateName=CivilDetail&lq=${pageStyleUtil.encodeURL("CaseID:\"" + doc.get("CaseID") + "\"")}">${searchResult.highlight(doc, "CaseNumber")}</a>             
                  <#else>
                  <a href="${request.getContextPath()}/search.do?indexName=detail&templateName=CriminalDetail&lq=${pageStyleUtil.encodeURL("CaseID:\"" + doc.get("CaseID") + "\"")}">${searchResult.highlight(doc, "CaseNumber")}</a> 
                  </#if>
                  </@display.column>


<@display.column  media="rtf excel pdf"  title="${searchResult.datasetConfiguration.getColumn(\"CaseNumber\").displayName}">
                ${doc.get("CaseNumber")}
</@display.column>




