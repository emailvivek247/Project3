<table id="searchResult" class="tablesorter">
 <thead><tr>
  <th>${searchResult.datasetConfiguration.getColumn("CaseNumber").displayName}</th><th>${searchResult.datasetConfiguration.getColumn("PartyID").displayName}</th>
  
  
 </tr></thead>
 <tbody>
<#if searchResult.docs?has_content>
<#assign ResultId =0>
 <#foreach doc in searchResult.docs>
  <#include "document.stl">
 </#foreach>
</#if>
 </tbody>
</table>


<script type="text/javascript">
$(function() {      
    $("#searchResult").tablesorter({widgets: ['zebra']});
}); 
</script>
