<#if (searchResult.docs?has_content && searchResult.docs.size() > 0) >
 <#foreach doc in searchResult.docs>
   <p style="border-bottom:0px solid #DDDDDD; margin:0 0 0 0; text-align:right">
      <font size="-1" style="font-weight:bold;color:#ff4500;" >Date:</font><font size="-1" color="#ff4500">${pageStyleUtil.format(date.format("yyyy/MM/dd HH:mm:ss z",doc.getDate("CurrentDateTime")), "yyyy/MM/dd HH:mm:ss z", "MM/dd/yyyy HH:mm:ss a")}</font>
      <#break>
 </#foreach> 
</#if>