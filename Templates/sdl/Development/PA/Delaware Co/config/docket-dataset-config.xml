  <dataquery name="WorkingQueue" batch-size="128" is-skipping-null-parameters="false">
    <sql><![CDATA[
SELECT DISTINCT
    FC.CASE_ID AS 'CaseID'
   ,FC.CASE_NUMBER
   ,CASE WHEN LTRIM(RTRIM(EE.EVENT_ENTRY_DESC)) = '' THEN ' ' ELSE ISNULL(EE.EVENT_ENTRY_DESC, ' ') END AS 'Description'
   ,CASE WHEN LTRIM(RTRIM(EEN.ADDITIONAL_COMMENTS)) = '' THEN ' ' ELSE ISNULL(EEN.ADDITIONAL_COMMENTS, ' ') END AS 'Comments'
   ,CONVERT(VARCHAR(10), EE.FILING_DT, 101) AS 'DateStamped' 
   ,CONVERT(VARCHAR(10), EE.FILING_DT, 108) AS 'TimeStamped'
   ,ISNULL(ATP.DISPLAYNAME,'') AS 'FilingAttorney'
   ,ATT.ATTORNEY_CD 
   ,EE.FILING_DT
   ,EE.EVENT_ENTRY_ID AS 'EventEntryID'
   ,case when DS.EVENT_ENTRY_ID IS null then 'N' ELSE 'Y' END AS 'IDFlag'
FROM ACM_FACTS_CASE FC
JOIN ACM_EVENT_TO_CASE ETC on ETC.CASE_ID = FC.CASE_ID
JOIN ACM_EVENT_ENTRY EE on EE.EVENT_ENTRY_ID = ETC.EVENT_ENTRY_ID
INNER JOIN ACM_EVENT_TO_PARTY EP ON EE.EVENT_ENTRY_ID = EP.EVENT_ENTRY_ID
INNER JOIN ACM_PARTY P ON EP.PARTY_ID = P.PARTY_ID
INNER JOIN ACM_PERSON PP ON P.PERSON_ALIAS_ID = PP.PERSON_ALIAS_ID
LEFT JOIN ACM_ATTORNEY ATT ON PP.PERSON_ALIAS_ID = ATT.PERSON_ALIAS_ID
LEFT JOIN ACM_PERSON ATP ON ATP.PERSON_ALIAS_ID = ATT.PERSON_ALIAS_ID
LEFT JOIN ACM_EVENT_ENTRY_NOTES EEN on EEN.EVENT_ENTRY_ID = EE.EVENT_ENTRY_ID
LEFT JOIN ACM_ADDRESS ADDD on ADDD.PERSON_ALIAS_ID = ATT.PERSON_ALIAS_ID
LEFT JOIN DOCUMENT_STORAGE DS ON DS.EVENT_ENTRY_ID = EE.EVENT_ENTRY_ID
WHERE FC.SEALED_CASE_FLG <> 'Y'
AND FC.IS_RESTRICTED_FLG <> 'Y'
AND EE.IS_SEALED_FLG <> 'Y'
AND EE.IS_RESTRICTED_FLG <> 'Y'
AND EE.ROA_HIDDEN <> 'Y'
AND EE.IS_DRAFT <> 'Y'
AND EE.VOID <> 'y'
AND EE.REMOVE_PUBLIC_ACCESS_FLG <> 'y'
AND FC.CASE_NUMBER <> '2012-005040'
ORDER BY EE.FILING_DT
    ]]></sql>
