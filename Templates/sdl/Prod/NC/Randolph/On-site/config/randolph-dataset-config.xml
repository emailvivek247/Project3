  <dataquery name="WorkingQueue" batch-size="128" is-skipping-null-parameters="false">
    <sql><![CDATA[
Select distinct
cast(a.id as varchar)+ '-' + cast(isnull(b.seq, 0) as varchar) as 'ROAMPKKEY'
,a.ID AS 'PK_KEY'
,'' AS 'ADD'
,a.VERIFIED as VER
,a.STATUS
,CAST(a.SEQ_KEY AS VARCHAR) AS 'INSTRUMENT'
,CONVERT(VARCHAR(10), RECDATE1, 112) AS DATEREC
,RECDATE1 AS RECDATEFILTER
,CONVERT(VARCHAR(30), RECDATE1, 120) AS DATERECDISPLAY
,LTRIM(RTRIM(CONVERT(VARCHAR(20), a.RECDATE2, 112))) AS DATEVENT
,LTRIM(RTRIM(CONVERT(VARCHAR(20), a.RECDATE2, 120))) AS DATEVENTDISPLAY
,RECDATE2 AS DATEVENTFILTER
,LTRIM(RTRIM(a.DISPLAYNAME)) as NAME
,dbo.fnReplaceNonAlphaNumericCharacters (LTRIM(RTRIM(a.DISPLAYNAME))) as LIKENAME
,LTRIM(RTRIM(a.DISPLAYNAME)) as PHONETICNAME
,LTRIM(RTRIM(a.DISPLAYNAME)) as NAMESORT
,LTRIM(RTRIM(a.DISPLAYNAME)) as SORTNAME
,LTRIM(RTRIM(a.DISPLAYNAME)) as NAMEDISPLAY
,a.ROLENAME AS PARTYROLE
,DESIGSTATUS
,a.ROLE AS 'ROLE'
,OTHERNAME as 'OTHERNAME'
,OTHERDESIGSTATUS
,OTHERROLE
,OTHERROLENAME
,a.INDEXTYPE
,LTRIM(RTRIM(a.code)) as TYPE
,Book AS 'BOOK'
,Page AS 'PAGE'
,b.LOTNUM
,b.SECTIONNUM AS 'SECTIONNUM'
,b.BLOCKNUM
,b.SUBDIVISION AS 'SUBDIVISION'
,b.TOWNSHIP AS 'TOWNSHIP'
,b.GPIN
,a.FIRSTREFERENCE as MARGINALS
,b.seq AS 'SEQ'
,offlinerecord
, a.DOCTYPE AS 'DOCTYPE'
,a.FIRSTLEGAL as REMARKS
,a.AMOUNT2 as CONSIDERATION
,(isnull (a.bookshort,'')+ ' ' + isnull(a.BOOK,'') + ' '+ isnull(a.PAGE,'')) AS 'BOOKPAGE'
,a.bookshort AS BOOKSHORT
,a.OFFLINEIMAGE
,a.BACKFILENUM
,a.DOCTYPEDESC
,a.MULTIPLE_PARTY1
,a.MULTIPLE_PARTY2
,a.MULTIPLE_LEGAL
,a.MULTIPLE_REFERENCE
,a.MULTIPLE_RETURNEE
,b.RANGENUM
,b.LEGAL_TYPE
,b.LDESC
,b.LDESC2
,b.LDESC3
,b.LEGAL1
,b.LEGAL2
,b.ACTIVE AS ACTIVE
,b.LEGALDESCRIPTION
,b.PARCEL
,a.CORP
,a.date_time_mod
FROM PUBLICSEARCH a left outer join LEGAL b ON a.SEQ_KEY = b.SEQ_KEY 
WHERE OFFLINERECORD <> 'Y' 
ORDER BY a.date_time_mod DESC
    ]]></sql>
