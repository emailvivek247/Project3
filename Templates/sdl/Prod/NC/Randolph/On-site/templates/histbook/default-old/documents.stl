<#if searchResult.docs?has_content>
<table id="searchResult" class="tablesorter">
 <thead><tr>
<th>${searchResult.datasetConfiguration.getColumn("Book").displayName}</th>
<th>${searchResult.datasetConfiguration.getColumn("Page").displayName}</th>
<th>Quick View</th>
<th>${searchResult.datasetConfiguration.getColumn("Image").displayName}</th>
<#--<th>Add to Briefcase</th>-->
  
  
  
 </tr></thead>
 <tbody>
<#assign ResultId =0>
 <#foreach doc in searchResult.docs>
  <#include "document.stl">
 </#foreach>
<#else>
 <#include "noResults.stl"> 
</#if>
 </tbody>
</table>


<script type="text/javascript">
$(function() {      
    $("#searchResult").tablesorter({widgets: ['zebra'],sortList: [[0,0],[1,0]]});  
    $("#searchResult").bind("sortEnd",function() { 
         $(".gallery a[rel^='prettyPhoto']").prettyPhoto({theme:'light_rounded',animationSpeed:'fast',slideshow:false,autoplay_slideshow: false,overlay_gallery: false,keyboard_shortcuts: true,opacity:0.3});
    });   
   
}); 
</script>