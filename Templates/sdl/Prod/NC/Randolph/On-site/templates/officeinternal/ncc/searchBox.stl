            searchValue = '#' + searchValue + '#';
                        searchValueQuery = elem[i].name + ':' + doubleQuotes + jQuery.trim(elem[i].value) + doubleQuotes;
                    } else if (elem[i].name == 'FirstName') {
            searchValue = elem[i].name + ':' + doubleQuotes + jQuery.trim(removeNonAlphaNumCharEndWildCard(elem[i].value)) + doubleQuotes + '~5';
