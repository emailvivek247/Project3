  <dataquery name="Content" is-batch-needed="true">
    <sql><![CDATA[
SELECT SEQKEY AS 'Instrument',
       SEQ AS 'CustomerInfoSeq',
       CASE WHEN LTRIM(RTRIM(NAME)) = '' THEN '|' ELSE ISNULL(NAME, '|') END AS 'CustomerInfoName',
       CASE WHEN LTRIM(RTRIM(CONVERT(VARCHAR(10), DATERETURNED, 101))) = '' THEN '|' ELSE ISNULL(CONVERT(VARCHAR(10), DATERETURNED, 101), '|') END AS 'DateReturned',
       CASE WHEN LTRIM(RTRIM(ADDRESSLINE1)) = '' THEN '|' ELSE ISNULL(ADDRESSLINE1, '|') END AS 'CustomerInfoAddress1',
       CASE WHEN LTRIM(RTRIM(ADDRESSLINE2)) = '' THEN '|' ELSE ISNULL(ADDRESSLINE2, '|') END AS 'CustomerInfoAddress2',
       CASE WHEN LTRIM(RTRIM(CITY)) = '' THEN '|' ELSE ISNULL(CITY, '|') END AS 'CustomerInfoCity',
       CASE WHEN LTRIM(RTRIM(STATE)) = '' THEN '|' ELSE ISNULL(STATE, '|') END AS 'CustomerInfoState',
       CASE WHEN LTRIM(RTRIM(ZIP)) = '' THEN '|' ELSE ISNULL(ZIP, '|') END AS 'CustomerInfoZip'
FROM CUSTOMERINFO
WHERE TYPE = 'RETURNEE'
AND SEQKEY IN (?)
    ]]></sql>
