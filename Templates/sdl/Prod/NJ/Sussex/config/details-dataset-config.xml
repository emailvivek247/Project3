  <dataquery name="Content" is-batch-needed="true">
    <sql><![CDATA[
SELECT D.SEQ_KEY AS 'Instrument',
       CASE WHEN LTRIM(RTRIM(D.DOCREFTYPE)) = '' THEN '|' ELSE ISNULL(D.DOCREFTYPE, '|') END AS 'DocRefType',
        ISNULL(D.SEQ, '-1') AS 'DocRefSeq',
       CASE WHEN LTRIM(RTRIM(D.INSTNUM)) = '' THEN '|' ELSE ISNULL(D.INSTNUM, '|') END AS 'RefInstrument',
       CASE WHEN LTRIM(RTRIM(DOC.USERID3)) = '' THEN '|' ELSE ISNULL(DOC.USERID3, '|') END AS 'DocRefBookType',
       CASE WHEN LTRIM(RTRIM(DOC.BOOK)) = '' THEN '|' ELSE ISNULL(DOC.BOOK, '|') END AS 'DocRefBook',
       CASE WHEN LTRIM(RTRIM(DOC.PAGE)) = '' THEN '|' ELSE ISNULL(DOC.PAGE, '|') END AS 'DocRefPage',
       CASE WHEN LTRIM(RTRIM(DOC.USERID2)) = '' THEN '|' ELSE ISNULL(DOC.USERID2, '|') END AS 'DocRefLegacy',
       CASE WHEN LTRIM(RTRIM(DOC.DOC_TYPE)) = '' THEN '|' ELSE ISNULL(DOC.DOC_TYPE, '|') END AS 'DocTypeRef',
       CASE WHEN LTRIM(RTRIM(CONVERT(VARCHAR(10), DOC.RECDATE1, 101))) = '' THEN '|' ELSE ISNULL(CONVERT(VARCHAR(10), DOC.RECDATE1, 101), '|') END AS 'RefDate'
FROM DOCREF D
INNER JOIN DOCUMENT DOC ON D.INSTNUM = DOC.SEQ_KEY
WHERE D.SEQ_KEY IN (?)
    ]]></sql>
