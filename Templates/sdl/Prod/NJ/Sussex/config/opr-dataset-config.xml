  <dataquery name="WorkingQueue" batch-size="128" is-skipping-null-parameters="false">
    <sql><![CDATA[
Select 
cast(a.id as varchar)+ '-' + cast(isnull(b.seq, 0) as varchar) as pk_key
,CASE WHEN LTRIM(RTRIM(CONVERT(VARCHAR(50), a.SEQ_KEY))) = '' THEN '|' ELSE ISNULL(CONVERT(VARCHAR(50), a.SEQ_KEY), '|') END AS 'Instrument'
,CONVERT(VARCHAR(MAX), a.SEQ_KEY) "InstrumentFilter"
,CONVERT(VARCHAR(10), RECDATE1, 112) AS 'RecDate'
,RECDATE1 AS 'RecDateFilter'
,CONVERT(VARCHAR(30), RECDATE1, 120) AS 'RecDateDisplay'
,LTRIM(RTRIM(ISNULL(SUFFIX,'')+ ' ' + ISNULL(LNAME,'') +' '+ISNULL(FNAME,'')+' '+ ISNULL(MNAME,''))) as 'DisplayName'
,LTRIM(RTRIM(LTRIM(RTRIM(LNAME))+' '+ISNULL(LTRIM(RTRIM(FNAME)),'')+' '+ ISNULL(LTRIM(RTRIM(MNAME)),''))) as 'NameSort'
,LTRIM(RTRIM(a.LNAME)) AS 'LastName'
,LTRIM(RTRIM(a.LNAME)) AS 'LastNameStarts'
,LTRIM(RTRIM(a.LNAME)) AS 'LastNameExact'
,LTRIM(RTRIM(a.LNAME)) AS 'LastNamePhonetic'
,LTRIM(RTRIM(a.FNAME + ISNULL(LTRIM(RTRIM(a.MNAME)), ''))) AS 'FirstName'
,LTRIM(RTRIM(a.FNAME + ISNULL(LTRIM(RTRIM(a.MNAME)), ''))) AS 'FirstNameStarts'
,LTRIM(RTRIM(a.FNAME + ISNULL(LTRIM(RTRIM(a.MNAME)), ''))) AS 'FirstNameExact'
,LTRIM(RTRIM(a.FNAME + ISNULL(LTRIM(RTRIM(a.MNAME)), ''))) AS 'FirstNamePhonetic'
,DESIGSTATUS AS 'DesigStatus'
,LTRIM(RTRIM(UPPER(SUBSTRING(ROLENAME,1,1)) + LOWER(SUBSTRING(ROLENAME,2,50)))) AS PartyRole
,case when role=1 then 'Direct Party' when role=2 then 'Reverse Party' when role not in (1,2) then 'No Party' end as Role
,case when a.role is null then '0' when a.role = '' then '0' else LTRIM(RTRIM(a.role)) end as RoleFilter
,OTHERNAME AS 'OtherName'
,OTHERDESIGSTATUS AS 'OtherDesigStatus'
,OTHERROLE AS 'OtherRole'
,OTHERROLENAME AS 'OtherRoleName'
,(SELECT descr FROM Doctype2lov WHERE type2 = a.indextype) as IndexType 
,LTRIM(RTRIM(a.code)) as DocType
,LTRIM(RTRIM(c.DESCR)) AS 'DocTypeDesc'
,b.legal_type as LegalType
,b.lotnum as Lot
,b.blocknum as Block
,a.book
,a.page
,b.township
,LegalDescription
,FirstReference
,MULTIPLE_REFERENCE AS MultipleReference
,case when b.seq is null then '0' else b.seq end as 'seq'
,offlinerecord
,offlineimage
, a.indextype as IndexCode
, a.seq as PartySeq
, a.doctype as DocCode
,b.LEGALDESCRIPTION AS 'Freeform'
,b.LDESC AS 'Other'
,a.date_time_mod
    from PUBLICSEARCH a left outer join LEGAL b on
    a.SEQ_KEY = b.SEQ_KEY 
    inner join DOCTYPE1LOV c
    on a.CODE = c.CODE
    where OFFLINERECORD ='N' 
    AND a.indextype not in (8,12,46)
    AND a.code not in ('DHPL','INST','HPL')
    ]]></sql>
