  <dataquery name="Incremental">
    <sql><![CDATA[
Select distinct
cast(a.id as varchar)+ '-' + cast(isnull(b.seq, 0) as varchar) as pk_key
,CASE WHEN LTRIM(RTRIM(CONVERT(VARCHAR(50), a.SEQ_KEY))) = '' THEN '|' ELSE ISNULL(CONVERT(VARCHAR(50), a.SEQ_KEY), '|') END AS 'Instrument'
,CONVERT(VARCHAR(10), RECDATE1, 112) AS 'RecDate'
,RECDATE1 AS 'RecDateFilter'
,CONVERT(VARCHAR(30), RECDATE1, 120) AS 'RecDateDisplay'
,LTRIM(RTRIM(LTRIM(RTRIM(LNAME))+' '+ISNULL(LTRIM(RTRIM(FNAME)),'')+' '+ ISNULL(LTRIM(RTRIM(MNAME)),''))) as 'DisplayName'
,LTRIM(RTRIM(LTRIM(RTRIM(LNAME))+' '+ISNULL(LTRIM(RTRIM(FNAME)),'')+' '+ ISNULL(LTRIM(RTRIM(MNAME)),''))) as 'DisplayNameStarts'
,LTRIM(RTRIM(LTRIM(RTRIM(LNAME))+' '+ISNULL(LTRIM(RTRIM(FNAME)),'')+' '+ ISNULL(LTRIM(RTRIM(MNAME)),''))) as 'DisplayNameExact'
,LTRIM(RTRIM(LTRIM(RTRIM(LNAME))+' '+ISNULL(LTRIM(RTRIM(FNAME)),'')+' '+ ISNULL(LTRIM(RTRIM(MNAME)),''))) as 'DisplayNamePhonetic'
,LTRIM(RTRIM(LNAME))+' '+ISNULL(LTRIM(RTRIM(FNAME)),'')+' '+ ISNULL(LTRIM(RTRIM(MNAME)),'') as 'NameSort'
,DESIGSTATUS AS 'DesigStatus'
,LTRIM(RTRIM(UPPER(SUBSTRING(ROLENAME,1,1)) + LOWER(SUBSTRING(ROLENAME,2,50)))) AS PartyRole
,case when role=1 then 'Direct Party' when role=2 then 'Reverse Party' when role not in (1,2) then 'No Party' end as Role
,case when a.role is null then '0' when a.role = '' then '0' else LTRIM(RTRIM(a.role)) end as RoleFilter
,OTHERNAME AS 'OtherName'
,OTHERDESIGSTATUS AS 'OtherDesigStatus'
,OTHERROLE AS 'OtherRole'
,OTHERROLENAME AS 'OtherRoleName'
,(SELECT descr FROM Doctype2lov WHERE type2 = a.indextype) as IndexType 
,LTRIM(RTRIM(a.code)) as DocType
,LTRIM(RTRIM(c.DESCR)) AS 'DocTypeDesc'
,b.legal_type as LegalType
,b.lotnum as Lot
,b.blocknum as Block
,b.acreage
,b.unit
,b.gpin as PID
,b.rangenum
,a.book
,a.page
,LTRIM(RTRIM(b.subdivision)) AS 'Subdivision'
,LTRIM(RTRIM(b.subdivision)) AS 'SubdivisionStarts'
,LTRIM(RTRIM(b.subdivision)) AS 'SubdivisionExact'
,LTRIM(RTRIM(b.subdivision)) AS 'SubdivisionPhonetic'
,b.ldesc as Detail
,b.sectionnum as Section
,b.township
,b.legal1 as Call1
,b.legal2 as Call2
,b.legal3 as Call3
,b.ldesc2 as Call4
,LegalDescription
,MULTIPLE_LEGAL AS MultipleLegal
,FirstReference
,MULTIPLE_REFERENCE AS MultipleReference
,case when b.seq is null then '0' else b.seq end as 'seq'
,offlinerecord
,offlineimage
,'Oklahoma' as Location
, a.indextype as IndexCode
, a.seq as PartySeq
, a.doctype as DocCode
,a.backfilenum AS 'LegacyNumber'
,RT.date_time_mod
    from PUBLICSEARCH a with (nolock) left outer join LEGAL b with (nolock) on
    a.SEQ_KEY = b.SEQ_KEY
    inner join DOCTYPE1LOV c with (nolock) on a.CODE = c.CODE
    INNER JOIN ROAM_TRACK RT with (nolock) ON RT.PK_KEY = a.ID 
    AND RT.TAB_NAME = 'PUBLICSEARCH' 
    LEFT OUTER JOIN ROAM_TRACK RT1 with (nolock) ON RT1.PK_KEY = B.SEQ_KEY
    AND RT1.TAB_NAME = 'LEGAL'
    where OFFLINERECORD ='N' 
    AND a.indextype in (61,11,62)
    AND RT.DATE_TIME_MOD >?
    ]]></sql>
  </dataquery>
