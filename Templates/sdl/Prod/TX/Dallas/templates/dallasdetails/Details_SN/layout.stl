</script>

<#-- *Loading -->
<style type="text/css">
#loading {
padding:1ex;
position:absolute;
top:50%;
left:70%;
display:none;
border:0px;
background-color:#ffffff;
color:#000000;
}

.basket table {	
	font-size: 13px;
}
</style>

<script type="text/javascript">
var loadit=function(){
var f=document.getElementById('image'), l=document.getElementById('loading').style;
l.display='block';
if(f.onload==null){
	f.onload=function(){l.display='none'};
	if(window.attachEvent)
	f.attachEvent('onload', f.onload);
}
return true;
}
</script>
