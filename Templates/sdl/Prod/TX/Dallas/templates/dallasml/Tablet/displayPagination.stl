<#assign num_pages = (searchResult.total/searchResult.length)?int>
<#if num_pages*searchResult.length < searchResult.total>
 <#assign num_pages = num_pages + 1>
</#if>
<#assign current_page_index = (searchResult.start/searchResult.length)?int + 1>
<link rel="stylesheet" href="<@com.currentUrlPrefix/>/resource/pagination-base.css" type="text/css" media="print, projection, screen" />
 <link rel="stylesheet" href="<@com.currentUrlPrefix/>/resource/pagination-glossy-blue.css" type="text/css" media="print, projection, screen" />
 <#assign pagination_already_defined = true >
<div class="paginationHolder">
 <center>
  <#if (num_pages>1)>
    <#--determin pagination rage-->
    <#if current_page_index <= num_pages-4>
    	<#assign start_page = current_page_index-5>
    <#else>
    	<#assign start_page = current_page_index-9+(num_pages-current_page_index)>
    </#if>
    <#if start_page <= 0><#assign start_page = 1></#if>
    <#assign end_page = start_page + 9>
    <#if (end_page>num_pages)><#assign end_page = num_pages></#if>
    <#--Render Previous-->

    <#if (current_page_index>=2)>
      <a class="prev" href="?${noStartQueryString}&start=${length*(current_page_index - 2)}&searchable=${searchable}&isPhoneticSearch=${isPhoneticSearch}">Previous</a>
    </#if>
    <#foreach page_index in start_page..end_page>
      <#if page_index == current_page_index>
        <span class="current">${page_index}</span>
      <#else>
        <a class="otherPage" href="?${noStartQueryString}&start=${length*(page_index-1)}&searchable=${searchable}&isPhoneticSearch=${isPhoneticSearch}">${page_index}</a>
      </#if>
    </#foreach>
    <#if num_pages &gt; end_page > 
	     <span class="text">of</span>
	     <#if current_page_index == num_pages>        
	       <span class="current">${page_index}</span>
	     <#else>
	       <a class="otherPage" href="?${noStartQueryString}&searchable=${searchable}&isPhoneticSearch=${isPhoneticSearch}&start=${length*(num_pages-1)}">${num_pages}</a>
		 </#if>
	</#if>


    <#--Render Next -->

<#-- *Check type of search -->

    <#if current_page_index < num_pages>
      <a class="next" href="?${noStartQueryString}&start=${length*current_page_index}&searchable=${searchable}&isPhoneticSearch=${isPhoneticSearch}">Next</a>
    </#if>
    <input type="text" id="pageNumber" name="pageNumber" size="2"/>
    <a onclick="gotoPage('${num_pages}', '${length}', '?${noStartQueryString}&searchable=${searchable}&isPhoneticSearch=${isPhoneticSearch}&start=')" class="goPaginate">Go Â»</a>

  </#if>
 </center>
 </div>
 <div style="clear: both;"></div>
