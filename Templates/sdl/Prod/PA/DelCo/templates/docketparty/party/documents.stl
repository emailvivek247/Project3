<#if searchResult.docs?has_content>

<b>Litigant(s) Information</b>
<table  id="partyInfo" class="tablesorter">
 <thead>
	 <tr>
	    <th>${searchResult.datasetConfiguration.getColumn("PartyName").displayName}</th>
	    <th>${searchResult.datasetConfiguration.getColumn("AttorneyName").displayName}</th>
	    <th>${searchResult.datasetConfiguration.getColumn("PartyRole").displayName}</th>
	    <th>${searchResult.datasetConfiguration.getColumn("Address").displayName}</th>
  	    <th>${searchResult.datasetConfiguration.getColumn("CaseFilingDate").displayName}</th>
	</tr>
</thead>


 <tbody>

<#foreach doc in searchResult.docs>
    <#assign PartyName = doc.getValues("PartyName")>
    <#assign AttorneyName = doc.getValues("AttorneyName")>
    <#assign PartyRole = doc.getValues("PartyRole")>
    <#assign Address = doc.getValues("Address")>
    <#assign CaseFilingDate = doc.getValues("CaseFilingDate")>
    <#assign loopCounter = 0>

    <tr>
         <td>${doc.get("PartyName")}</td>
         <td>${doc.get("AttorneyName")}</td>
         <td>${doc.get("PartyRole")}</td>
	 <td>${doc.get("Address")}</td>
	 <td>${pageStyleUtil.format(date.format("yyyy/MM/dd HH:mm:ss z",doc.getDate("CaseFilingDate")), "yyyy/MM/dd HH:mm:ss z", "MM/dd/yyyy")}</td>
             

    <#assign loopCounter = loopCounter + 1>
   </tr>
</#foreach>


 </tbody>
</table>

<#else>
<center><b>No Party Information Available</b></center>

</#if>
<script type="text/javascript">
$(document).ready(function() {
	oTable = $('#partyInfo').dataTable({
		"bJQueryUI": true,
		"bPaginate": false,
		"bLengthChange": false,
		"bFilter": false,
		"bSort": true,
		"bInfo": false,
		"bAutoWidth": false,
		"aaSorting": [[2,"desc"],[0, "asc"]]
	});
} );
</script>