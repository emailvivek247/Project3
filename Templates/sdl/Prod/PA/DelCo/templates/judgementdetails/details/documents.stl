<#if searchResult.docs?has_content>

<table  id="searchResult" class="tablesorter">
 <thead>
	 <tr>
	    <th>${searchResult.datasetConfiguration.getColumn("CASE_NUMBER").displayName}</th>
	    <th>${searchResult.datasetConfiguration.getColumn("COURT_NAME").displayName}</th>
	    <th>${searchResult.datasetConfiguration.getColumn("CASE_TYP_DESC").displayName}</th>
	    <th>${searchResult.datasetConfiguration.getColumn("CASE_SHORT_TITLE").displayName}</th>
	    <th>${searchResult.datasetConfiguration.getColumn("CASE_FILING_DT").displayName}</th>
	    <th>${searchResult.datasetConfiguration.getColumn("JUDGE").displayName}</th>
	    <th>${searchResult.datasetConfiguration.getColumn("BOOK").displayName}</th>
	    <th>${searchResult.datasetConfiguration.getColumn("PAGE").displayName}</th>
	    <th>${searchResult.datasetConfiguration.getColumn("JUDGEMENT_AGAINST").displayName}</th>
	    <th>${searchResult.datasetConfiguration.getColumn("JUDGEMENT_FOR").displayName}</th>
	</tr>
</thead>


 <tbody>

<#foreach doc in searchResult.docs>
    
    <#assign CASE_NUMBER = doc.getValues("CASE_NUMBER")>
    <#assign COURT_NAME = doc.getValues("COURT_NAME")>
    <#assign CASE_TYP_DESC = doc.getValues("CASE_TYP_DESC")>
    <#assign CASE_SHORT_TITLE = doc.getValues("CASE_SHORT_TITLE")>
    <#assign CASE_FILING_DT = doc.getValues("CASE_FILING_DT")>
    <#assign JUDGE = doc.getValues("JUDGE")>
    <#assign BOOK = doc.getValues("BOOK")>
    <#assign PAGE = doc.getValues("PAGE")>
    <#assign JUDGEMENT_AGAINST = doc.getValues("JUDGEMENT_AGAINST")>
    <#assign JUDGEMENT_FOR = doc.getValues("JUDGEMENT_FOR")>
    <#assign loopCounter = 0>

    <tr>
         <td>${doc.get("CASE_NUMBER")}</td>
	 <td>${searchResult.highlight(doc, "COURT_NAME")}</td>
	 <td>${searchResult.highlight(doc, "CASE_TYP_DESC")}</td>
	 <td>${searchResult.highlight(doc, "CASE_SHORT_TITLE")}</td>
         <td>${pageStyleUtil.format(date.format("yyyy/MM/dd HH:mm:ss z",doc.getDate("CASE_FILING_DT")), "yyyy/MM/dd HH:mm:ss z", "MM/dd/yyyy")}</td>
	 <td>${searchResult.highlight(doc, "JUDGE")}</td>
	 <td>${searchResult.highlight(doc, "BOOK")}</td>
	 <td>${searchResult.highlight(doc, "PAGE")}</td>
	 <td>${searchResult.highlight(doc, "JUDGEMENT_AGAINST")}</td>
         <td>${searchResult.highlight(doc, "JUDGEMENT_FOR")}</td>
             

    <#assign loopCounter = loopCounter + 1>
   </tr>
</#foreach>


 </tbody>
</table>



<table id="searchResult2" class="tablesorter">
 <thead>
	 <tr>
	    <th>${searchResult.datasetConfiguration.getColumn("JUDGEMENT_ISSUED_TYP").displayName}</th>
	    <th>${searchResult.datasetConfiguration.getColumn("DISPOSITION_DATE").displayName}</th>
	    <th>${searchResult.datasetConfiguration.getColumn("DISPOSITION_DESC").displayName}</th>
	    <th>${searchResult.datasetConfiguration.getColumn("JUDGMENT_AMOUNT").displayName}</th>
	    <th>${searchResult.datasetConfiguration.getColumn("JUDGMENT_SATISFIED_BY").displayName}</th>
	    <th>${searchResult.datasetConfiguration.getColumn("ACCOUNT_FOLIO_NUMBER").displayName}</th>
	    <th>${searchResult.datasetConfiguration.getColumn("WRIT_TYPE").displayName}</th>
	    <th>${searchResult.datasetConfiguration.getColumn("SATISFACTION_COST").displayName}</th>
            <th>${searchResult.datasetConfiguration.getColumn("CLAIM_TEXT").displayName}</th>
	    
	 </tr>
 </thead>

 <tbody>

<#foreach doc in searchResult.docs>
    
    <#assign JUDGEMENT_ISSUED_TYP = doc.getValues("JUDGEMENT_ISSUED_TYP")>
    <#assign DISPOSITION_DATE = doc.getValues("DISPOSITION_DATE")>
    <#assign DISPOSITION_DESC = doc.getValues("DISPOSITION_DESC")>
    <#assign JUDGMENT_AMOUNT = doc.getValues("JUDGMENT_AMOUNT")>    
    <#assign JUDGMENT_SATISFIED_BY = doc.getValues("JUDGMENT_SATISFIED_BY")>
    <#assign ACCOUNT_FOLIO_NUMBER = doc.getValues("ACCOUNT_FOLIO_NUMBER")>
    <#assign WRIT_TYPE = doc.getValues("WRIT_TYPE")>
    <#assign SATISFACTION_COST = doc.getValues("SATISFACTION_COST")>
    <#assign CLAIM_TEXT = doc.getValues("CLAIM_TEXT")>
    
    <#assign loopCounter = 0>


    
    <tr>
             
         <td>${searchResult.highlight(doc, "JUDGEMENT_ISSUED_TYP")}</td>
	 <td>${pageStyleUtil.format(date.format("yyyy/MM/dd HH:mm:ss z",doc.getDate("DISPOSITION_DATE")), "yyyy/MM/dd HH:mm:ss z", "MM/dd/yyyy")}</td>
	 <td>${searchResult.highlight(doc, "DISPOSITION_DESC")}</td>
	 <td>${pageStyleUtil.formatCurrency(doc.get("JUDGMENT_AMOUNT"))}</td>
	 <td>${searchResult.highlight(doc, "JUDGMENT_SATISFIED_BY")}</td>
         <td>${searchResult.highlight(doc, "ACCOUNT_FOLIO_NUMBER")}</td>
         <td>${searchResult.highlight(doc, "WRIT_TYPE")}</td>
         <td>${searchResult.highlight(doc, "SATISFACTION_COST")}</td>
         <td>${searchResult.highlight(doc, "CLAIM_TEXT")}</td>
         
         <#assign loopCounter = loopCounter + 1>
         <#assign caseNumberKey = doc.getValues("CASE_NUMBER")>
    </tr>
     

</#foreach>

	    <table align="center">
		<tr>
		   <td>
		     <a href="#" onClick="open_docket();" style="text-decoration:underline;color:#2e7ee4;font-weight:bold;">Click to see Civil Case Docket information</a>
		   </td>
		</tr>
	   </table>
 </tbody>
</table>
 
 <br />
 <br />
 
 <#else>
 <center><b>No Judgment Information Available</b></center>

</#if>

<script type="text/javascript">
function open_docket()
{
window.open('${request.getContextPath()}/search.do?indexName=docketcase&templateName=case&lq=CASE_NUMBER%3A%22<#list caseNumberKey as CaseNumber>${CaseNumber}</#list>%22','_self')
}

$(document).ready(function() {
	oTable = $('#searchResult').dataTable({
		"bJQueryUI": true,
		"bPaginate": false,
		"bLengthChange": false,
		"bFilter": false,
		"bSort": true,
		"bInfo": false,
		"bAutoWidth": false,
		"aaSorting": []
	});
	oTable = $('#searchResult2').dataTable({
		"bJQueryUI": true,
		"bPaginate": false,
		"bLengthChange": false,
		"bFilter": false,
		"bSort": true,
		"bInfo": false,
		"bAutoWidth": false,
		"aaSorting": []
	});
} );
</script>