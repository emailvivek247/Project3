  <dataquery name="WorkingQueue" batch-size="128" is-skipping-null-parameters="false">
    <sql><![CDATA[
SELECT DISTINCT
    FC.CASE_NUMBER AS 'CASE_NUMBER'
   ,ACT.CASE_TYP_DESC AS 'CaseType'
   ,FC.CASE_FILING_DT AS 'CaseFilingDate'
   FROM ACM_FACTS_CASE FC 
LEFT JOIN ACM_CASE_TYP ACT ON FC.CASE_TYP_ID = ACT.CASE_TYP_ID
WHERE FC.SEALED_CASE_FLG <> 'Y'
AND FC.IS_RESTRICTED_FLG <> 'Y'
AND FC.EXPUNGE_FLG <> 'Y'
AND FC.CASE_NUMBER <> '2012-005040'
    ]]></sql>
