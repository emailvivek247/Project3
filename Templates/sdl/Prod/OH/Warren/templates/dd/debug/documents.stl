
<#if searchResult.docs?has_content>
<table id="searchResult" class="tablesorter">
 <thead><tr>
  <th>${searchResult.datasetConfiguration.getColumn("CaseID").displayName}</th><th>${searchResult.datasetConfiguration.getColumn("DocketDate").displayName}</th><th>${searchResult.datasetConfiguration.getColumn("DocketText").displayName}</th><th>${searchResult.datasetConfiguration.getColumn("IS_RESTRICTED_FLG").displayName}</th><th>${searchResult.datasetConfiguration.getColumn("IS_DRAFT").displayName}</th><th>${searchResult.datasetConfiguration.getColumn("IS_SEALED_FLG").displayName}</th><th>${searchResult.datasetConfiguration.getColumn("ROA_HIDDEN").displayName}</th><th>${searchResult.datasetConfiguration.getColumn("SEALED_CASE_FLG").displayName}</th>
  
  
  
 </tr></thead>
 <tbody>
<#assign ResultId =0>
 <#foreach doc in searchResult.docs>
  <#include "document.stl">
 </#foreach>
<#else>
 <#include "noResults.stl"> 
</#if>
 </tbody>
</table>


<script type="text/javascript">
$(function() {      
    $("#searchResult").tablesorter({widgets: ['zebra']});
}); 
</script>
