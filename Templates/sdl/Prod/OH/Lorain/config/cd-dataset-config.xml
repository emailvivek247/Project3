  <dataquery name="WorkingQueue" batch-size="128" is-skipping-null-parameters="false">
    <sql><![CDATA[
SELECT DISTINCT
         ISNULL(RTRIM(FC.CASE_ID), '|') AS 'CaseID'
	,CASE WHEN FC.CASE_NUMBER = '' THEN '|' ELSE ISNULL(FC.CASE_NUMBER, '|') END AS 'CaseNumber'
	,ISNULL(CST.CASE_STATUS_DESCR, '|') AS 'CaseStatus'
	,ISNULL(VC.FILED_JURISDICTION_DESC, '|') AS 'Jurisdiction'
FROM acm_facts_case FC
LEFT JOIN LN_ACM_V_CHARGE VC ON VC.CASE_ID = FC.CASE_ID 
LEFT JOIN ACM_CASE_STATUS_TYP CST ON CST.CASE_STATUS_TYP_ID = FC.CASE_STATUS_TYP_ID
WHERE FC.SEALED_CASE_FLG <> 'Y'
    ]]></sql>
