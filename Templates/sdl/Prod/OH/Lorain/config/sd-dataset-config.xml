  <dataquery name="WorkingQueue" batch-size="128" is-skipping-null-parameters="false">
    <sql><![CDATA[
SELECT
          ISNULL(CONVERT(VARCHAR(10),S.ISSUE_DT, 101), '|') AS 'IssueDate'
         ,ISNULL(CONVERT(VARCHAR(10),S.SERVED_DT, 101), '|') AS 'ServiceDate'
           ,CASE WHEN LTRIM(RTRIM(EEN.ADDITIONAL_COMMENTS)) = '' THEN '|' ELSE ISNULL(EEN.ADDITIONAL_COMMENTS, '|') END AS 'Comments'
         ,CASE WHEN LTRIM(RTRIM(HOW_SERVED_DESC)) = '' THEN '|' ELSE ISNULL(HOW_SERVED_DESC, '|') END AS 'ServiceDesc'
         ,CASE WHEN ETP.MULTI_PARTIES = 'Y' 
                   THEN PS.DISPLAYNAME + '  ET AL.' 
                   ELSE PS.DISPLAYNAME END AS 'PARTY_NAME'
         ,P.CASE_ID AS 'CaseID'
         ,FC.CASE_NUMBER AS 'CaseNumber'
         ,S.EVENT_ENTRY_ID AS 'EventEntryID'
         ,ET.EVENT_ENTRY_TYP_ID AS 'EventEntryTypeID'
         ,PS.SEALED_FLAG AS 'SealedFlag'
         ,P.RSTRCD_PARTY_FLAG AS 'RestrictedPartyFlag'
         ,CASE WHEN ISNULL(s.executed,'N')='N' 
                   THEN 'No' 
                   ELSE 'Yes' END AS 'Executed'
         ,ee.EVENT_ENTRY_DESC As 'EventDesc'
         ,CASE WHEN LTRIM(RTRIM(P1.DISPLAYNAME)) = '' THEN '|' ELSE ISNULL(P1.DISPLAYNAME, '|') END AS 'ServiceProvider'    
         ,CASE WHEN LTRIM(RTRIM(HS.HOW_SERVED_DESC)) = '' THEN '|' ELSE ISNULL(HS.HOW_SERVED_DESC, '|') END AS 'ServiceType'
         ,CASE WHEN LTRIM(RTRIM(S.COMMENTS)) = '' THEN '|' ELSE ISNULL(S.COMMENTS, '|') END AS 'ReceivedBy'
         ,HS.HOW_SERVED_ID
,s.SUMMON_NUM as 'SummonNumber'
FROM ACM_SUMMON S 
            LEFT OUTER JOIN ACM_EVENT_ENTRY EE 
                  ON S.EVENT_ENTRY_ID = EE.EVENT_ENTRY_ID AND EE.VOID = 'N'
            LEFT OUTER JOIN (SELECT  EVENT_ENTRY_ID,MIN(TETP.PARTY_ID) AS PARTY_ID,
                              CASE WHEN COUNT(TETP.PARTY_ID) > 1 THEN 'Y' ELSE 'N' END AS MULTI_PARTIES
                              FROM ACM_EVENT_TO_PARTY TETP 
                    GROUP BY EVENT_ENTRY_ID) ETP
                  ON EE.EVENT_ENTRY_ID = ETP.EVENT_ENTRY_ID 
            LEFT OUTER JOIN ACM_PARTY P               ON ETP.PARTY_ID = P.PARTY_ID
            LEFT OUTER JOIN ACM_FACTS_CASE FC               ON FC.CASE_ID = P.CASE_ID AND FC.VOID <> 'Y'
            LEFT OUTER JOIN ACM_PERSON PS             ON P.PERSON_ALIAS_ID = PS.PERSON_ALIAS_ID
            LEFT OUTER JOIN ACM_EVENT_ENT_TYP ET                  ON EE.EVENT_ENTRY_TYP_ID = ET.EVENT_ENTRY_TYP_ID 
            LEFT OUTER JOIN ACM_PERSON P1 ON P1.PERSON_ALIAS_ID = S.SERVICE_PROVIDER_PERSON_ALIAS_ID
        LEFT JOIN ACM_HOW_SERVED HS             ON S.HOW_SERVED_ID = HS.HOW_SERVED_ID
      LEFT OUTER JOIN ACM_EVENT_ENTRY_NOTES EEN             ON EE.EVENT_ENTRY_ID = EEN.EVENT_ENTRY_ID AND EEN.CURRENT_FLG = 'Y'
    ]]></sql>
