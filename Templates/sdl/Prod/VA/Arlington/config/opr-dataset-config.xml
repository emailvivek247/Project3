,LTRIM(RTRIM(a.FNAME + ISNULL(LTRIM(RTRIM(a.MNAME)), ''))) AS 'FirstName'
,LTRIM(RTRIM(a.FNAME + ISNULL(LTRIM(RTRIM(a.MNAME)), ''))) AS 'FirstNameStarts'
,LTRIM(RTRIM(a.FNAME + ISNULL(LTRIM(RTRIM(a.MNAME)), ''))) AS 'FirstNameExact'
,LTRIM(RTRIM(a.FNAME + ISNULL(LTRIM(RTRIM(a.MNAME)), ''))) AS 'FirstNamePhonetic'
,OTHERNAME AS 'OtherName'
,case when a.role is null then '0' when a.role = '' then '0' else LTRIM(RTRIM(a.role)) end as RoleFilter
,UPPER(SUBSTRING(ROLENAME,1,1)) + LOWER(SUBSTRING(ROLENAME,2,50)) AS PartyRole
,case when role=1 then 'Direct Party' when role=2 then 'Reverse Party' when role not in (1,2) then 'No Party' end as Role
,a.Book
,a.Page
,a.doctype as DocCode
,LTRIM(RTRIM(a.code)) AS 'DocType'
,LTRIM(RTRIM(c.descr)) AS 'DocTypeDesc'
,LTRIM(RTRIM(c.descr)) AS 'DocTypeDescDisplay'
,LTRIM(RTRIM(c.descr)) AS 'DocTypeDescFilter'
,b.ST_NUM AS 'StreetNum'
,LTRIM(RTRIM(LTRIM(RTRIM(ISNULL(b.ST_DIR, ''))) + ' ' + LTRIM(RTRIM(ISNULL(b.ST_NAME, ''))))) AS 'StreetName'
,LTRIM(RTRIM(b.ST_TYPE)) AS 'StreetType'
,b.UNIT AS 'Unit'
,b.LEGAL1
,LDESC as Freeform
,LegalDescription
,FirstReference
,LTRIM(RTRIM(LTRIM(RTRIM(ISNULL(b.ST_NUM, ''))) + ' ' + LTRIM(RTRIM(ISNULL(b.ST_DIR, ''))) + ' ' + LTRIM(RTRIM(ISNULL(b.ST_NAME, ''))) + ' ' + LTRIM(RTRIM(ISNULL(b.ST_TYPE, ''))))) AS 'Address'
,case when b.seq is null then 0 else b.seq end as 'seq'
,offlinerecord
,'Arlington' as Location
,a.indextype as IndexCode
,(SELECT LTRIM(RTRIM(descr)) FROM Doctype2lov WHERE type2 = a.indextype) as 'IndexType'
,case when a.seq is null then 0 else a.seq end as PartySeq
,RT.date_time_mod
      from PUBLICSEARCH a with (nolock) left outer join LEGAL b with (nolock) on
      a.SEQ_KEY = b.SEQ_KEY 
      inner join DOCTYPE1LOV c with (nolock)
      on a.CODE = c.CODE
      INNER JOIN ROAM_TRACK RT with (nolock) ON RT.PK_KEY = A.ID
      AND RT.TAB_NAME = 'PUBLICSEARCH' 
      LEFT OUTER JOIN ROAM_TRACK RT1 with (nolock) ON RT1.PK_KEY = B.SEQ_KEY
      AND RT1.TAB_NAME = 'LEGAL' 
      where OFFLINERECORD ='N'  
     AND a.DOCTYPE not in (104,105,107)
     AND a.INDEXTYPE in (1)
     AND RT.DATE_TIME_MOD > ? 
order by RT.date_time_mod desc
    ]]></sql>
  </dataquery>
