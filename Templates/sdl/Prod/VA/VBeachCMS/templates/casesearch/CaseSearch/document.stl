<@display.column media= "html" title="Security Status">
	<#if doc.get("CaseSealed") = "Y">
		<font color = "red"><b>SEALED CASE</b></font>
	</#if>
	<#if doc.get("CaseRestricted") = "Y">
		<font color = "blue"><b>CONFIDENTIAL CASE</b></font>
	</#if>
	<#if doc.get("PersonSealed") = "Y">
		<font color = "red"><b>SEALED PERSON</b></font>
	</#if>
	<#if doc.get("PartyRestricted") = "Y">
		<font color = "blue"><b>CONFIDENTIAL PARTY</b></font>
	</#if>
</@display.column>


<@display.column  media="rtf excel pdf"  title="Security Status">
                                     
	<#if doc.get("CaseSealed") = "Y">
		SEALED CASE
	</#if>
	<#if doc.get("CaseRestricted") = "Y">
		CONFIDENTIAL CASE
	</#if>
	<#if doc.get("PersonSealed") = "Y">
		SEALED PERSON
	</#if>
	<#if doc.get("PartyRestricted") = "Y">
		CONFIDENTIAL PARTY
</#if>
</@display.column>



