<b>General Case Information</b>
<#if (caseCounter > 0)>
<table id="case" class="tablesorter">
 <thead><tr>
  <th>${searchResult.datasetConfiguration.getColumn("CaseNumber").displayName}</th>
  <th>${searchResult.datasetConfiguration.getColumn("CourtType").displayName}</th>
 <th>${searchResult.datasetConfiguration.getColumn("CommencedBy").displayName}</th>
  <th>${searchResult.datasetConfiguration.getColumn("CaseFilingDate").displayName}</th>
 <th>${searchResult.datasetConfiguration.getColumn("CaseStatus").displayName}</th>
  <th>${searchResult.datasetConfiguration.getColumn("CaseJudge").displayName}</th>
<th>${searchResult.datasetConfiguration.getColumn("Sealed").displayName}</th>
<th>${searchResult.datasetConfiguration.getColumn("Confidential").displayName}</th>
<th>${searchResult.datasetConfiguration.getColumn("CaseStyle").displayName}</th>

 </tr></thead>
 <tbody>

<#foreach doc in searchResult.docs>
    
    <#assign CourtType = doc.getValues("CourtType")>
   <#assign CommencedBy = doc.getValues("CommencedBy")> 
<#assign CaseFilingDate = doc.getValues("CaseFilingDate")>    
<#assign CaseStatus = doc.getValues("CaseStatus")>  
<#assign CaseJudge = doc.getValues("CaseJudge")>   
   <#assign Sealed = doc.getValues("Sealed")>   
<#assign Confidential = doc.getValues("Confidential")>   
<#assign CaseStyle = doc.getValues("CaseStyle")>  

    <#assign loopCounter = 0>

    <#foreach CaseNumber in doc.getValues("CaseNumber")>
	<tr>
       
<td>${CaseNumber}</td>
             <td>${CourtType[loopCounter]}</td> 
                           <td>${CommencedBy[loopCounter]}</td> 
             <td>${CaseFilingDate[loopCounter]}</td> 
    <td>${CaseStatus[loopCounter]}</td> 
             <td>${CaseJudge[loopCounter]?replace('.', '&nbsp;')?replace('UAJ', 'UNASSIGNED JUDGE')}</td> 
             <td>${Sealed[loopCounter]}</td>  
 <td>${Confidential[loopCounter]}</td> 
             <td>${CaseStyle[loopCounter]?replace('.', '&nbsp;')}</td>  
             <#assign loopCounter = loopCounter + 1>
	</tr>
     </#foreach>

</#foreach>

 </tbody>
</table>
<#else>
<br><br>
<b><i>No information available.</i></b>
<br><br>
</#if>
</#if>















