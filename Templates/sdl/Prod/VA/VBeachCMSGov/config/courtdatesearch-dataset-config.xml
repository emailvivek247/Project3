  <dataquery name="Deletion" is-delete-only="true">
    <sql><![CDATA[
SELECT RT.TAB_PK_KEY
FROM ROAM_TRACK RT
WHERE RT.DATE_TIME_MOD > DATEADD(mi, -10, GETDATE()) and RT.TAB_NAME = 'ACC_SESN_ITM_ENTITY' AND
RT.DML_TYP IN ('D', 'U') 
UNION ALL
SELECT PK
FROM ACM_V_ESCOURTDATESEARCH ES INNER JOIN ROAM_TRACK RT
ON ES.CASE_ID = RT.PK_KEY
WHERE 
RT.DATE_TIME_MOD > DATEADD(mi, -10, GETDATE()) AND 
RT.DML_TYP IN ('D', 'U') AND 
((ES.CASE_SEALED = 'Y' OR ES.PERSON_SEALED = 'Y' AND COURT_CD IN ('CRIMINAL','CIVIL')) OR CASE_VOID = 'Y' OR PARTY_VOID = 'Y' OR DELETED_FLG = 'Y')
    ]]></sql>
  </dataquery>
