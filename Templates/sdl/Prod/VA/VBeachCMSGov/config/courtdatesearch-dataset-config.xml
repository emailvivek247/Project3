  <dataquery name="WorkingQueue" batch-size="128" is-skipping-null-parameters="false">
    <sql><![CDATA[
SELECT  DISTINCT  PK,
      CONVERT(VARCHAR(20), SESSIONDATE, 112) AS 'SessionDate',
      CONVERT(VARCHAR(20), SESSIONDATE, 100) AS 'DisplaySessionDate',
      CONVERT(VARCHAR(20), SESSIONDATE, 101) AS 'FilterBySessionDate',
      COMMENT_TEXT AS 'SchedulingComment',
      REASON AS 'AppearanceReason',
      HEARING_DESC AS 'HearingResult',
      DISPLAYNAME AS 'ScheduledParty',
      SCHEDULED_CHARGE AS 'ScheduledCharge',
      CASE_NUMBER AS 'CaseNumber',
PARTY_ROLE_DESC AS 'PartyRole',
PARTY_ID AS 'PartyID',
CASE_ID AS 'CaseID',
COURT_CD AS 'CourtType',
CASE_SEALED AS 'CaseSealed',
CASE_RESTRICTED AS 'CaseRestricted',
PERSON_SEALED AS 'PersonSealed',
PARTY_RESTRICTED AS 'PartyRestricted',
DATE_TIME_MOD
  FROM ACM_V_ESCOURTDATESEARCH 
WHERE (CASE_SEALED = 'N' AND PERSON_SEALED = 'N' OR COURT_CD = 'JAPPL')
AND CASE_VOID = 'N'
AND PARTY_VOID = 'N'
AND PARTY_ROLE_DESC IN ('PLAINTIFF', 'DEFENDANT')
AND COURT_CD IN ('JAPPL', 'CRIMINAL', 'CIVIL')
AND (DELETED_FLG IS NULL OR DELETED_FLG <> 'Y')
ORDER BY DISPLAYNAME, DATE_TIME_MOD desc
    ]]></sql>
