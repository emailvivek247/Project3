
<#if (searchResult.docs?has_content && searchResult.docs.size() > 0) >
<#assign ResultId =0>
 <#foreach doc in searchResult.docs>
  <p class="result">
     <br/><b>${searchResult.datasetConfiguration.getColumn("CaseFilingDate").displayName}</b>:
    		<#if doc.get("CaseFilingDate")?has_content>
				${pageStyleUtil.format(doc.get("CaseFilingDate"), "yyyymmdd", "mm/dd/yyyy")}&nbsp;<b>(MM/DD/YYYY)</b>
			</#if>  	
          <br/><b>${searchResult.datasetConfiguration.getColumn("CaseID").displayName}</b>:
${number.format("###0.##",doc.get("CaseID"))}
          <br/><b>${searchResult.datasetConfiguration.getColumn("CaseNumber").displayName}</b>:
${searchResult.highlight(doc, "CaseNumber")}
     <br/><b>${searchResult.datasetConfiguration.getColumn("CaseRestricted").displayName}</b>:
${searchResult.highlight(doc, "CaseRestricted")}
     <br/><b>${searchResult.datasetConfiguration.getColumn("CaseSealed").displayName}</b>:
${searchResult.highlight(doc, "CaseSealed")}
     <br/><b>${searchResult.datasetConfiguration.getColumn("CaseStatus").displayName}</b>:
${searchResult.highlight(doc, "CaseStatus")}
     <br/><b>${searchResult.datasetConfiguration.getColumn("CaseStyle").displayName}</b>:
${searchResult.highlight(doc, "CaseStyle")}
     <br/><b>${searchResult.datasetConfiguration.getColumn("CaseType").displayName}</b>:
${searchResult.highlight(doc, "CaseType")}
 <br/>&nbsp;
 </#foreach>
<#else>
 <#include "noResults.stl"> 
</#if>

