

<#assign display=JspTaglibs["http://displaytag.sf.net"]>


<#if searchResult.docs?has_content>
    <#assign ResultId =0>
	<@display.table name="searchResult.docs" id="doc" export=true requestURI="/search.do" excludedParams="searchType CaseFilingDate CaseFilingDate_select CaseID CaseID_select CaseNumber CaseNumber_select CaseRestricted CaseRestricted_select CaseSealed CaseSealed_select CaseStatus CaseStatus_select CaseStyle CaseStyle_select CaseType CaseType_select CategoryType CategoryType_select CourtType CourtType_select DATE_TIME_MOD DATE_TIME_MOD_select FilterByCaseType FilterByCaseType_select FilterByCourtType FilterByCourtType_select FirstName FirstName_select HearingDate HearingDate_select LastName LastName_select MiddleName MiddleName_select Party Party_select PartyID PartyID_select PartyRestricted PartyRestricted_select PartyType PartyType_select PersonSealed PersonSealed_select SuffixName SuffixName_select ">
	  <#include "document.stl">
	  <@display.setProperty name="export.title" value="Search Criteria :${Request.searchForm.searchQuery}" />  
	</@display.table>
<#else>
 <#include "noResults.stl"> 
</#if>



<script type="text/javascript">
$(document).ready(function() {
	oTable = $('#doc').dataTable({
		"bJQueryUI": true,
		"bPaginate": false,
		"bLengthChange": false,
		"bFilter": false,
		"bSort": true,
		"bInfo": false,
		"bAutoWidth": false,
		"aaSorting": []
	});
} );
</script>
