  <b>Service Information</b>
		 <table id="service" class="tablesorter">
				<thead>
					<tr>
					  <th>${searchResult.datasetConfiguration.getColumn("DocketDate").displayName}</th>
					  <th>${searchResult.datasetConfiguration.getColumn("DocketText").displayName}</th>
					</tr>
				</thead>
			<tbody>
<#if searchResult.docs?has_content>
     <#foreach doc in searchResult.docs>
		<#assign DocketDate= doc.getValues("DocketDate") DocketText= doc.getValues("DocketText") loopCounter = 0>
                          <#foreach DocketDate in doc.getValues("DocketDate")>
					<tr>
                                                <td>${DocketDate}</td>
						<td>${DocketText[loopCounter]}</td>
                                                <#assign loopCounter = loopCounter + 1>
					</tr>
			  </#foreach>
     </#foreach>
			
	<#else>
		
		<tr>
		   <td><b><i>No Service Information Available.</i></b></td>               
               </tr>
 </#if>
 </tbody>
 </table>



<script type="text/javascript">
$(document).ready(function() {
       $("#service").tablesorter({sortList: [[0,0]]}); 
       $("#service").tablesorter({widgets: ['zebra']});
       
});
</script>
        

