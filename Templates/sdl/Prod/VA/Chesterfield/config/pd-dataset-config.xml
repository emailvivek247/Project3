  <dataquery name="WorkingQueue" batch-size="128" is-skipping-null-parameters="false">
    <sql><![CDATA[
SELECT DISTINCT
    ISNULL(RTRIM(FC.CASE_ID), ' ') AS 'CaseID'
   ,ISNULL(PT.PARTY_ID, ' ') AS 'PartyID'
   ,P.PERSON_ALIAS_ID
   ,ISNULL(P.DISPLAYNAME, ' ') AS 'DisplayName'
   ,ISNULL(PRT.PARTY_ROLE_DESC, ' ') AS 'PartyRole'
   ,ISNULL(PA.ADDRESS_LINE1_TXT, ' ') + ' ' + ISNULL(PA.ADDRESS_LINE2_TXT, '') + ' ' + ISNULL(PA.CITY_NAME, '') + ' ' + ISNULL(PA.STATE_NAME, '') + ' ' + ISNULL(PA.POSTAL_CD, '') AS 'Address'
   ,ISNULL(CONVERT(VARCHAR, AC.TOTAL_BALANCE), '|') AS 'AmountOwed'
FROM ACM_FACTS_CASE FC
INNER JOIN ACM_PARTY PT ON PT.CASE_ID = FC.CASE_ID
INNER JOIN ACM_PERSON P ON P.PERSON_ALIAS_ID = PT.PERSON_ALIAS_ID
INNER JOIN ACM_V_PERS_ADDRESS PA ON PA.PERSON_ALIAS_ID = P.PERSON_ALIAS_ID
INNER JOIN ACM_PARTY_ROLE_TYP PRT ON PT.PARTY_ROLE_TYP_ID = PRT.PARTY_ROLE_TYP_ID
INNER JOIN ACM_V_GCI_ACCOUNTING AC ON AC.PARTY_ID = PT.PARTY_ID
WHERE FC.SEALED_CASE_FLG <> 'Y'
AND PT.SEALED_PARTY_FLG <> 'Y'
AND PRT.PARTY_ROLE_TYP_CD NOT IN ('VICT','VICTIM_REP','VICWIT','WIT','W','V')
AND PT.JUVENILE_FLG = 'N'
AND PT.RSTRCD_PARTY_FLAG = 'N'
AND PT.SEALED_PARTY_FLG = 'N'
AND P.SEALED_FLAG = 'N'
    ]]></sql>
