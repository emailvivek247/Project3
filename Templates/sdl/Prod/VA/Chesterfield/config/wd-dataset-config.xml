  <dataquery name="WorkingQueue" batch-size="128" is-skipping-null-parameters="false">
    <sql><![CDATA[
SELECT DISTINCT
     ISNULL(RTRIM(FC.CASE_ID), '|') AS 'CaseID'
    ,CASE WHEN CONVERT(varchar(10), W.ISSUE_DT, 101) = '' THEN '|' ELSE ISNULL(CONVERT(varchar(10), W.ISSUE_DT, 101), '|') END AS 'IssuedDate' 
    ,CASE WHEN P.DISPLAYNAME = '' THEN '|' ELSE ISNULL(P.DISPLAYNAME, '|') END AS 'WarrantName'
    ,PRT.PARTY_ROLE_DESC AS 'RoleType'
    ,CASE WHEN WT.WARRANT_TYP_DESC = '' THEN '|' ELSE ISNULL(WT.WARRANT_TYP_DESC, '|') END AS 'WarrantType'
    ,CASE WHEN CONVERT(varchar(10), W.SERVED_DT, 101) = '' THEN '|' ELSE ISNULL(CONVERT(varchar(10), W.SERVED_DT, 101), '|') END AS 'ServedDate'
    ,CASE WHEN W.SERVED_DT IS NULL THEN 'N' ELSE 'Y' END AS 'WarrantServedFlag' --where can I find these two?
    ,'' AS 'WarrantExecutedFlag'
    ,CASE WHEN W.WARRANT_ID = '' THEN '|' ELSE ISNULL(W.WARRANT_ID, '|') END AS 'WarrantCaseNumber'
FROM ACM_WARRANT W
      LEFT OUTER JOIN ACM_WARRANT_TYP WT ON W.WARRANT_TYP_ID = WT.WARRANT_TYP_ID
      INNER JOIN ACM_EVENT_TO_WARRANT EW ON W.WARRANT_ID = EW.WARRANT_ID
      INNER JOIN ACM_EVENT_ENTRY EE ON EW.EVENT_ENTRY_ID = EE.EVENT_ENTRY_ID AND ISNULL(EE.VOID,'N') = 'N'
      INNER JOIN ACM_EVENT_TO_PARTY EP ON EW.EVENT_ENTRY_ID = EP.EVENT_ENTRY_ID
      INNER JOIN ACM_PARTY PR ON EP.PARTY_ID = PR.PARTY_ID 
      INNER JOIN ACM_PERSON P ON PR.PERSON_ALIAS_ID = P.PERSON_ALIAS_ID
      INNER JOIN ACM_PARTY_ROLE_TYP PRT ON PRT.PARTY_ROLE_TYP_ID = PR.PARTY_ROLE_TYP_ID
      INNER JOIN ACM_EVENT_TO_CASE EC ON PR.CASE_ID = EC.CASE_ID AND EW.EVENT_ENTRY_ID = EC.EVENT_ENTRY_ID
      INNER JOIN ACM_FACTS_CASE FC ON EC.CASE_ID = FC.CASE_ID AND ISNULL(FC.VOID,'N') = 'N'
WHERE FC.SEALED_CASE_FLG <> 'Y'
    ]]></sql>
