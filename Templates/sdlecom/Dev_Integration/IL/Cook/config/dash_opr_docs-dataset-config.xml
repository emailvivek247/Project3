  <dataquery name="WorkingQueue" batch-size="128" is-skipping-null-parameters="false">
    <sql><![CDATA[
SELECT SUBSTRING(DATE_FILED, 1, 4) AS 'YEAR', 'Chancery' AS DIVISION, ISNULL(NULLIF(CASE_TYPE, ''), 'No Case Type Assigned') AS 'CASETYPE', COUNT(CASE_NUMBER) AS DOCUMENTCOUNT
FROM CHANF0
GROUP BY SUBSTRING(DATE_FILED, 1, 4), DIVISION, CASE_TYPE, ISNULL(NULLIF(CASE_TYPE, ''), 'No Case Type Assigned')
UNION
SELECT SUBSTRING(DATE_FILED, 1, 4) AS 'YEAR', 'Civil' AS DIVISION, ISNULL(NULLIF(CASE_TYPE, ''), 'No Case Type Assigned') AS 'CASETYPE', COUNT(CASE_NUMBER) AS DOCUMENTCOUNT
FROM CIVILF0
GROUP BY SUBSTRING(DATE_FILED, 1, 4), DIVISION, CASE_TYPE, ISNULL(NULLIF(CASE_TYPE, ''), 'No Case Type Assigned')
UNION
SELECT SUBSTRING(DATE_FILED, 1, 4) AS 'YEAR', 'Suburban Civil' AS DIVISION, ISNULL(NULLIF(CASE_TYPE, ''), 'No Case Type') AS 'CASETYPE', COUNT(CASE_NUMBER) AS DOCUMENTCOUNT
FROM SUBF0
GROUP BY SUBSTRING(DATE_FILED, 1, 4), DIVISION, CASE_TYPE, ISNULL(NULLIF(CASE_TYPE, ''), 'No Case Type Assigned')
ORDER BY SUBSTRING(DATE_FILED, 1, 4), DIVISION
    ]]></sql>
