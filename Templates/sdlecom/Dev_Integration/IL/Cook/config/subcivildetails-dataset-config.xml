  <dataquery name="WorkingQueue" batch-size="128" is-skipping-null-parameters="false">
    <sql><![CDATA[
select
     RTRIM(LTRIM(CONVERT(VARCHAR(30),a.Case_Number))) + RTRIM(LTRIM(CONVERT(VARCHAR(30),d.ID))) AS PKKEY
	,a.case_number AS 'CaseNumber'
	,CONVERT(VARCHAR(30), a.date_filed , 120) AS 'DateFiled'
	,a.case_type AS 'CaseType'
	,'Suburban Civil' AS 'Division'
	,a.District AS 'District'
	,a.Ad_damnum AS 'AdDamnum'
	,b.Plaintiff_name AS 'PlaintiffName'
	,b.AKA AS 'PlaintiffAKA'
	,c.Defendant_name AS 'DefendantName'
	,c.AKA AS 'DefendantAKA'
	,CONVERT(VARCHAR(30), d.acty_date , 120) AS 'ActivityDate'
	,d.plaintiff_code AS 'PlaintiffCode'
	,d.acty_code AS 'ActyCode'
         ,(Select TOP 1 activity_code_description from [dbo].[CIVIL_ACTIVITY_CODE_FILES] where CONVERT(VARCHAR, d.acty_code)=CONVERT(VARCHAR, activity_code_number)) as 'ActyDesc'	
         ,(Select top 1 judge_name from judge where CONVERT(VARCHAR, judge_code) =CONVERT(VARCHAR, d.judge_code)) AS 'Judge'
	,CONVERT(VARCHAR(30),d.Court_Date , 120) AS 'CourtDate'
	,d.Court_rm AS 'CourtRoom'
	,d.Court_time AS 'CourtTime'
	,d.Court_fee AS 'CourtFee'
	,d.judge_code AS 'JudgeCode'
	,d.Judgement_Amnt AS 'JudgementAmount'
        ,d.ID
from SUBF0 a 
inner join SUBF1 b on a.Case_Number = b.Case_Number
inner join SUBF2 c on a.Case_Number = c.Case_Number
inner join SUBF3 d on a.Case_Number = d.Case_Number
    ]]></sql>
