  <dataquery name="WorkingQueue" batch-size="128" is-skipping-null-parameters="false">
    <sql><![CDATA[
Select top 1
  RTRIM(LTRIM(CONVERT(VARCHAR(30),a.Case_Number))) + RTRIM(LTRIM(CONVERT(VARCHAR(30),d.ID))) AS PKKEY
, 'Federated' as Division
,a.case_number AS 'CaseNumber'
,a.case_type AS 'CaseType'
,b.plaintiff_name AS 'PlaintiffName'
,b.plaintiff_name AS 'PhoneticPlaintiffName'
,b.plaintiff_atty AS 'PlaintiffAttorney'
,c.defendant_name AS 'DefendantName'
,c.defendant_name AS 'PhoneticDefendantName'
,c.defendant_atty_code AS 'DefAttCode'
,CONVERT(VARCHAR(10), a.date_filed, 112) AS DateFiled
,CONVERT(VARCHAR(30), a.date_filed, 120) AS DATEFILEDDISPLAY
,LEFT(a.date_filed, 4) AS 'YEAR'
,a.acty_code AS 'AttorneyCode'
,d.ID AS ID
From chanf0 a 
LEFT JOIN chanf1 b on a.case_number=b.case_number 
LEFT JOIN chanf2 c on a.case_number=c.case_number
INNER JOIN CHANF3 d on a.Case_Number = d.Case_Number
where a.Case_Number = '99999999999'
    ]]></sql>
