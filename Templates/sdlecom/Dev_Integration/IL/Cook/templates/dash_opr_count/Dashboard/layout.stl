<script type="text/javascript">
      function openwindow(yearandmonthanddoctype)
      { 
              yearandmonthanddoctype = yearandmonthanddoctype.replace("=undefined", "");
              year = yearandmonthanddoctype.split("@")[0];
              month = yearandmonthanddoctype.split("@")[1];
              doctype = yearandmonthanddoctype.split("@")[2]; 
              if (month.length < 2){
                    month = "0" + month;
              }
              if (doctype == "No Case Type Assigned") {
                    var url = escape("YEAR:\"" + year + "\"+DIVISION:\"" + month+ "\"+CaseType:\"                                                  \"");
              } else {
                    var url = escape("YEAR:\"" + year + "\"+DIVISION:\"" + month+ "\"+CaseType:\"" + doctype + "\"");
              }

                                                  

              url = "${request.getContextPath()}/search.do?searchType=1&indexName=federated,chancery,civil,subcivil&templateName=&q=" + url;
              window.open(url,null,"height=700,width=900,status=yes,toolbar=no,resizable=yes,menubar=no,location=yes,scrollbars=yes");
      }
      
</script>
