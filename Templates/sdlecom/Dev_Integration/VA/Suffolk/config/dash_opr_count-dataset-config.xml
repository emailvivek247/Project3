  <dataquery name="WorkingQueue" batch-size="128" is-skipping-null-parameters="false">
    <sql><![CDATA[
SELECT DATEPART ( YYYY , RECDATE1 ) as YEAR, DATEPART ( MM , RECDATE1 ) as MONTH, COUNT(SEQ_KEY) AS DOCUMENTCOUNT
FROM DOCUMENT 
WHERE INDEXTYPE not in (13, 14, 6)
and flag4='N' and void='N' and deleterecord='N'
GROUP BY DATEPART ( YYYY , RECDATE1 ), DATEPART ( MM , RECDATE1 )
ORDER BY DATEPART ( YYYY , RECDATE1 ), DATEPART ( MM , RECDATE1 )
    ]]></sql>
