<script type="text/javascript">
      function openwindow(year, month, doctype)
      { 
              if (month.length < 2){
                    month = "0" + month;
              }
              var url = escape("+RecDateFilter:\"" + year + "\"+RecDateFilter:\"" + year + "\"+RecDateFilter:\"" + year + "/" + month + "\"+DocType:\"" + doctype + "\"");
              url = "${request.getContextPath()}/search.do?searchType=1&indexName=opr&templateName=&searchable=Name,Instrument,RecDate,PartyRole,IndexType,DocType,Book,Page,Lot,Unit,SectionNum,Subdivision,Township,Other,DesigStatus,GPIN,ProjectNum,RdNum,Desc2,Desc3,Phase,Acreage,Building&q=" + url;
              window.open(url,null,"height=700,width=900,status=yes,toolbar=no,resizable=yes,menubar=no,location=yes,scrollbars=yes");
      }


      function addToSelect(selectbox, text, value, selected) {
        var selBox = document.getElementById(selectbox);
    var optn = document.createElement("OPTION");
    optn.text = text;
    optn.value = value;
        if (selected == "Y"){
           optn.selected = "selected";
        }
            selBox.options.add(optn);
      }
</script>

</head>
<body link="#0000ff" vlink="#0000ff">
<#nested>
</body></html>
</#macro>