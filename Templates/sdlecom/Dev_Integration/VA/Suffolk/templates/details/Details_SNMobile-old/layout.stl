<script type="text/javascript">
var loadit=function(){
var f=document.getElementById('image'), l=document.getElementById('loading').style;
l.display='block';
if(f.onload==null){
f.onload=function(){l.display='none'};
if(window.attachEvent)
f.attachEvent('onload', f.onload);
}
return true;

}

</script>


