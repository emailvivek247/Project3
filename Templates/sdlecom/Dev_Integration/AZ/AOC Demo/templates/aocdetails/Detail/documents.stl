<#-- warrantdetail -->



<#assign display=JspTaglibs["http://displaytag.sf.net"]>

<A HREF="javascript:history.go(-1)">Â« Back to search results</a>

<br><br>


<#if searchResult.docs?has_content>

<#-- Check Whether Case/Party/Demo/Address/Phone/Warrant/etc. Exists or Not -->
<#assign
    caseCounter = 0
    partiesCounter = 0
    demoCounter = 0
    addressCounter = 0
    phoneCounter = 0
    warrantCounter = 0
    roaCounter = 0
    complaintCounter = 0
    chargeCounter = 0
    totalCounter = 0
  
   
>

<#foreach doc in searchResult.docs>
    <#if doc.getValues("CaseID")?has_content>
        <#assign caseCounter = 1>
    </#if>
    <#if doc.getValues("DisplayName")?has_content>
        <#assign partiesCounter = 1>
    </#if>
    <#if doc.getValues("Current")?has_content>
        <#assign demoCounter = 1>
    </#if>
    <#if doc.getValues("AddressType")?has_content>
        <#assign addressCounter = 1>
     </#if>
    <#if doc.getValues("PhoneType")?has_content>
        <#assign phoneCounter = 1>
    </#if>
    <#if doc.getValues("WarrantType")?has_content>
        <#assign warrantCounter = 1>
    </#if>
    <#if doc.getValues("DateStamped")?has_content>
        <#assign roaCounter = 1>
    </#if>
    <#if doc.getValues("ComplaintNum")?has_content>
        <#assign complaintCounter = 1>
    </#if>
    <#if doc.getValues("ChargeCount")?has_content>
        <#assign chargeCounter = 1>
    </#if> 
    <#if doc.getValues("AccountingName")?has_content>
        <#assign totalCounter = 1>
    </#if>
    
        
</#foreach>

<#foreach doc in searchResult.docs>

    
<#--Start General Case Info-->
<#if (caseCounter > 0)>
  <b>General Case Information</b>
    <table id="case" class="tablesorter">
           <thead>
               <tr>
              <th>${searchResult.datasetConfiguration.getColumn("CaseNumber").displayName}</th>
              <th>${searchResult.datasetConfiguration.getColumn("CaseFilingDate").displayName}</th>
               </tr>
           </thead>
        <tbody>
        <#assign CaseID = doc.getValues("CaseID") CaseFilingDate = doc.getValues("CaseFilingDate") loopCounter = 0>

        <#foreach CaseNumber in doc.getValues("CaseNumber")>
              <tr>
                <#assign caseNumberKey = CaseID[loopCounter]>
                <td>${CaseNumber}</td>
                <td>${CaseFilingDate[loopCounter]}</td>

                <#assign loopCounter = loopCounter + 1>
              </tr>
        </#foreach>
        </tbody>
        </table>
<#else>
<br><br>
<b><i>No Case Information Available.</i></b>
<br><br>
</#if>


<#--Start Party Info-->
<#if (partiesCounter > 0)>
   <b>Parties</b>
     <table id="parties" class="tablesorter">
     <thead>
         <tr>
           <th>${searchResult.datasetConfiguration.getColumn("DISPLAYNAME").displayName}</th>
           <th>${searchResult.datasetConfiguration.getColumn("PartyRole").displayName}</th>
             </tr>
         </thead>
       <tbody>
             <#assign PartyRole = doc.getValues("PartyRole") loopCounter = 0>

       <#foreach DisplayName in doc.getValues("DisplayName")>
         <tr>
               <td>${DisplayName?replace('|', '&nbsp;')}</td>
               <td>${PartyRole[loopCounter]}</td> 
               <#assign loopCounter = loopCounter + 1>
        </tr>
       </#foreach>
       </tbody>
     </table>
<#else>
<br><br>
<b><i>No Party Information Available.</i></b>
<br><br>
</#if>




<#--Start Warrant Info-->
<#if (warrantCounter > 0)>
     <b>Warrant Information</b>
                                                      
       <table id="warrant" class="tablesorter {sortlist: [[0,0]]}">
           <thead>
                <tr>
          <th>${searchResult.datasetConfiguration.getColumn("IssuedDate").displayName}</th>
          <th>${searchResult.datasetConfiguration.getColumn("WarrantName").displayName}</th>
          <th>${searchResult.datasetConfiguration.getColumn("WarrantType").displayName}</th>
          <th>${searchResult.datasetConfiguration.getColumn("WarrantID").displayName}</th>
          <th>${searchResult.datasetConfiguration.getColumn("LocationDesc").displayName}</th>
          <th>${searchResult.datasetConfiguration.getColumn("JudgeName").displayName}</th>
          <th>${searchResult.datasetConfiguration.getColumn("QuashedDate").displayName}</th>
          <th>${searchResult.datasetConfiguration.getColumn("ReturnDate").displayName}</th>
          <th>${searchResult.datasetConfiguration.getColumn("ServedDate").displayName}</th>

               </tr>
            </thead>
         <tbody>
         <#assign WarrantName = doc.getValues("WarrantName") WarrantType = doc.getValues("WarrantType") WarrantID= doc.getValues("WarrantID") LocationDesc = doc.getValues("LocationDesc")
            JudgeName = doc.getValues("JudgeName") IssuedDate = doc.getValues("IssuedDate") QuashedDate = doc.getValues("QuashedDate") ReturnDate = doc.getValues("ReturnDate")
            ServedDate = doc.getValues("ServedDate") loopCounter = 0>

         <#foreach WarrantType in doc.getValues("WarrantType")>
               <tr>
          <td>${IssuedDate[loopCounter]?replace('|', '&nbsp;')}</td> 
          <td>${WarrantName[loopCounter]?replace('|', '&nbsp;')}</td>
          <td>${WarrantType?replace('|', '&nbsp;')}</td>
          <td>${WarrantID[loopCounter]?replace('|', '&nbsp;')}</td>
          <td>${LocationDesc[0]?replace('|', '&nbsp;')}</td>
          <td>${JudgeName[loopCounter]?replace('|', '&nbsp;')}</td>  
          <td>${QuashedDate [loopCounter]?replace('|', '&nbsp;')}</td>
          <td>${ReturnDate[loopCounter]?replace('|', '&nbsp;')}</td>
          <td>${ServedDate[loopCounter]?replace('|', '&nbsp;')}</td>
                  <#assign loopCounter = loopCounter + 1>
               </tr>
         </#foreach>
         </tbody>
      </table>
<#else>
<br><br>
<b><i>No Warrant Information Available.</i></b>
<br><br>
</#if>



<#--Start ROA-->
<#if (roaCounter > 0)>
   <b>Event Information</b>
       <table id="roa" class="tablesorter {sortlist: [[0,0]]}">
          <thead>
              <tr>
          <th>${searchResult.datasetConfiguration.getColumn("DateStamped").displayName}</th>
          <th>${searchResult.datasetConfiguration.getColumn("ROADescription").displayName}</th>
          <th>${searchResult.datasetConfiguration.getColumn("SatisfiedDate").displayName}</th>
          </tr>
          </thead>
        <tbody>
        <#assign ROADescription = doc.getValues("ROADescription") SatisfiedDate = doc.getValues("SatisfiedDate") Precint = doc.getValues("Precint") loopCounter = 0>

        <#foreach DateStamped in doc.getValues("DateStamped")>
              <tr>
          <td>${DateStamped?replace('|', '&nbsp;')}</td>
          <td>${ROADescription[loopCounter]?replace('|', '&nbsp;')}</td> 
          <td>${SatisfiedDate[loopCounter]?replace('|', '&nbsp;')}</td>
                  <#assign loopCounter = loopCounter + 1>
              </tr>
        </#foreach>
        </tbody>
     </table>
<#else>
<br><br>
<b><i>No ROA Information Available.</i></b>
<br><br>
</#if>






<#--Start Citation Info-->
<#if (complaintCounter > 0)>
    <b>Citation Information</b>

        <table id="citation" class="tablesorter">
            <thead>
           <tr>
                   <th>${searchResult.datasetConfiguration.getColumn("ComplaintNum").displayName}</th>
                   <th>${searchResult.datasetConfiguration.getColumn("LocationDesc").displayName}</th>
                   <th>${searchResult.datasetConfiguration.getColumn("CitationDate").displayName}</th>
                   <th>${searchResult.datasetConfiguration.getColumn("VehicleMake").displayName}</th>
                   <th>${searchResult.datasetConfiguration.getColumn("VehicleModel").displayName}</th>
                   <th>${searchResult.datasetConfiguration.getColumn("VehicleColor").displayName}</th>
                   <th>${searchResult.datasetConfiguration.getColumn("LicenseNumber").displayName}</th>
                   <th>${searchResult.datasetConfiguration.getColumn("DLState").displayName}</th>
           </tr>
         </thead>      
                
         <tbody>
         
         <#assign ComplaintNumbers = doc.getValues("ComplaintNum") CitationDate = doc.getValues("CitationDate") LocationDesc = doc.getValues("LocationDesc")
                  VehicleMake = doc.getValues("VehicleMake") VehicleModel = doc.getValues("VehicleModel") VehicleColor = doc.getValues("VehicleColor")
                  LicenseNumber = doc.getValues("LicenseNumber") DLState = doc.getValues("DLState")  loopCounter = 0>       
                
         <#foreach ComplaintNum in ComplaintNumbers>
            <tr>
            <td>${ComplaintNumbers[loopCounter]?replace('|', '&nbsp;')}</td>
            <td>${LocationDesc[0]?replace('|', '&nbsp;')}</td>
            <td>${CitationDate[loopCounter]?replace('|', '&nbsp;')}</td>
            <td>${VehicleMake[loopCounter]?replace('|', '&nbsp;')}</td>
            <td>${VehicleModel[loopCounter]?replace('|', '&nbsp;')}</td>
            <td>${VehicleColor[loopCounter]?replace('|', '&nbsp;')}</td>
            <td>${LicenseNumber[loopCounter]?replace('|', '&nbsp;')}</td>           
            <td>${DLState[loopCounter]?replace('|', '&nbsp;')}</td>

            <#assign loopCounter = loopCounter + 1>
            </tr>
         </#foreach>
         </tbody>
        </table>      
             
<#else>
<br><br>
<b><i>No Citation Information Available.</i></b>
<br><br>
</#if>

                   
<#--Start Charge Info-->
<#if (chargeCounter > 0)>
    <b>Charge Information</b>
       <table id="charges" class="tablesorter">
          <thead>
             <tr>
         <th>${searchResult.datasetConfiguration.getColumn("ChargeName").displayName}</th>
         <th>${searchResult.datasetConfiguration.getColumn("ChargeCount").displayName}</th>
         <th>${searchResult.datasetConfiguration.getColumn("ChargeSequence").displayName}</th>
         <th>${searchResult.datasetConfiguration.getColumn("ChargeCode").displayName}</th>
         <th>${searchResult.datasetConfiguration.getColumn("ChargeDescription").displayName}</th>
         <th>${searchResult.datasetConfiguration.getColumn("ChargeClass").displayName}</th>
         <th>${searchResult.datasetConfiguration.getColumn("ChargeType").displayName}</th>
         <th>${searchResult.datasetConfiguration.getColumn("Jurisdiction").displayName}</th>
         <th>${searchResult.datasetConfiguration.getColumn("OffenseDateFrom").displayName}</th>
         <th>${searchResult.datasetConfiguration.getColumn("OffenseDateTo").displayName}</th>
         <th>${searchResult.datasetConfiguration.getColumn("Disposition").displayName}</th>
         <th>${searchResult.datasetConfiguration.getColumn("DispositionDate").displayName}</th>
             </tr>
          </thead>
         
         <tbody>
        <#assign ChargeName = doc.getValues("ChargeName") ChargeCounts = doc.getValues("ChargeCount") ChargeSequence = doc.getValues("ChargeSequence")
                 ChargeCode = doc.getValues("ChargeCode") ChargeDescription = doc.getValues("ChargeDescription") ChargeClass = doc.getValues("ChargeClass")
                 ChargeType = doc.getValues("ChargeType") Jurisdiction = doc.getValues("Jurisdiction") OffenseDateFrom = doc.getValues("OffenseDateFrom")  
                 OffenseDateTo = doc.getValues("OffenseDateTo") Disposition = doc.getValues("Disposition") DispositionDate = doc.getValues("DispositionDate") loopCounter = 0> 
         
        <#foreach ChargeCount in ChargeCounts>
        
             <tr>
                <td>${ChargeName[loopCounter]?replace('|', '&nbsp;')}</td>
                <td>${ChargeCounts[loopCounter]?replace('|', '&nbsp;')}</td>
                <td>${ChargeSequence[loopCounter]?replace('|', '&nbsp;')}</td>
                <td>${ChargeCode[loopCounter]?replace('|', '&nbsp;')}</td>
                <td>${ChargeDescription[loopCounter]?replace('|', '&nbsp;')}</td>
                <td>${ChargeClass[loopCounter]?replace('|', '&nbsp;')}</td>
                <td>${ChargeType[loopCounter]?replace('|', '&nbsp;')}</td>
                <td>${Jurisdiction[loopCounter]?replace('|', '&nbsp;')}</td>
                <td>${OffenseDateFrom[loopCounter]?replace('|', '&nbsp;')}</td>
                <td>${OffenseDateTo[loopCounter]?replace('|', '&nbsp;')}</td>
                <td>${Disposition[loopCounter]?replace('|', '&nbsp;')}</td>
                <td>${DispositionDate[0]?replace('|', '&nbsp;')}</td>
                
                <#assign loopCounter = loopCounter + 1>
            </tr>
        </#foreach>
        </tbody>
     </table> 
         
<#else>
<br><br>
<b><i>No Charge Information Available.</i></b>
<br><br>
</#if>


<#--Start Accounting Info-->
<#if (totalCounter > 0)>
     <b>Total Balance</b>
        <table id="total" class="tablesorter">
            <thead>
               <tr>
                 <th>${searchResult.datasetConfiguration.getColumn("AccountingName").displayName}</th>
                 <th>${searchResult.datasetConfiguration.getColumn("TotalBalance").displayName}</th>
               </tr> 
            </thead>
          <tbody>
          <#assign TotalBalance = doc.getValues("TotalBalance") loopCounter = 0>
          
          <#foreach AccountingName in doc.getValues("AccountingName")>
               <tr>
                 <td>${AccountingName?replace('|', '&nbsp;')}</td>
                 <td>${TotalBalance[loopCounter]?replace('|', '&nbsp;')}</td>
                 <#assign loopCounter = loopCounter + 1>
               </tr>
          </#foreach>
          </tbody>
     </table>
<#else>
<br><br>
<b><i>No Accounting Information Available.</i></b>
<br><br>
</#if>


  
</#foreach>


</#if>



<script type="text/javascript">
    $(document).ready(function() {  
    $("#case").tablesorter({widgets: ['zebra']});
    $("#parties").tablesorter({widgets: ['zebra']});
    $("#demo").tablesorter({widgets: ['zebra']});
    $("#address").tablesorter({widgets: ['zebra']});
    $("#phone").tablesorter({widgets: ['zebra']});
    $("#warrant").tablesorter({widgets: ['zebra']});
    $("#citation").tablesorter({widgets: ['zebra']});
    $("#charges").tablesorter({widgets: ['zebra']});
    $("#roa").tablesorter({widgets: ['zebra']});
    $("#related").tablesorter({widgets: ['zebra']});
    $("#incarceration").tablesorter({widgets: ['zebra']});
    $("#probation").tablesorter({widgets: ['zebra']});
    $("#scheduling").tablesorter({widgets: ['zebra']});
    $("#address").tablesorter({widgets: ['zebra']});
    $("#demo").tablesorter({widgets: ['zebra']});
    $("#phone").tablesorter({widgets: ['zebra']});
    $("#warrant").tablesorter({widgets: ['zebra']});
    $("#cost").tablesorter({widgets: ['zebra']});
    $("#total").tablesorter({widgets: ['zebra']});
    $("#images").tablesorter({widgets: ['zebra']});

  }); 
</script>

