<#assign display=JspTaglibs["http://displaytag.sf.net"]>

<#assign searchCriteria = "Search Criteria " + Request.searchForm.searchQuery >
<#if filtersApplied?has_content>
    <#assign searchCriteria = searchCriteria + " Filters Applied: " +filtersApplied>
</#if>

<#if searchResult.docs?has_content>
    <#assign ResultId =0>
	<@display.table name="searchResult.docs" id="doc" export=true requestURI="/search.do" excludedParams="searchType FirstName FirstName_select LastName LastName_select MiddleName MiddleName_select Suffix Suffix_select ActAttyList ActAttyList_select BirthDate BirthDate_select CaseNumSearch CaseNumSearch_select CaseNumber CaseNumber_select CourtCode CourtCode_select OrgName OrgName_select RoleCode RoleCode_select RoleDesc RoleDesc_select TTEAPFlag TTEAPFlag_select JudicialOfficer JudicialOfficer_select Casetype Casetype_select CaseCategoryDescription CaseCategoryDescription_select ">
	  <#include "document.stl">
	  <@display.setProperty name="export.title" value="Search Criteria :${Request.searchForm.searchQuery}" />  
	</@display.table>
<#else>
 <#include "noResults.stl"> 
</#if>



<script type="text/javascript">
$(document).ready(function() {
	oTable = $('#doc').dataTable({
		"bJQueryUI": true,
		"bPaginate": false,
		"bLengthChange": false,
		"bFilter": false,
		"bSort": true,
		"bInfo": false,
		"bAutoWidth": false,
		"aaSorting": []
	});
} );
</script>