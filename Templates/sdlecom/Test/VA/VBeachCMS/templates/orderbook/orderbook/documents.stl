<b>Order Book Information</b>
<#if searchResult.docs?has_content>
<table id="orderbook" class="tablesorter">
 <thead><tr>
  <th>${searchResult.datasetConfiguration.getColumn("OrderEntryDate").displayName}</th>
  <#--<th>${searchResult.datasetConfiguration.getColumn("OrderBook").displayName}</th>-->
   <#--<th>${searchResult.datasetConfiguration.getColumn("FilingDate").displayName}</th>-->
  <th>${searchResult.datasetConfiguration.getColumn("OrderType").displayName}</th>
  <th>${searchResult.datasetConfiguration.getColumn("Judge").displayName}</th>
  
  
 </tr></thead>
 <tbody>
<#assign ResultId =0>
 <#foreach doc in searchResult.docs>
  
	<#assign OrderEntryDate = doc.getValues("OrderEntryDate")> 
	 <#--<#assign OrderBook = doc.getValues("OrderBook")> -->
	 <#--<#assign CaseFilingDate = doc.getValues("CaseFilingDate")>-->   
	<#assign FilingDate = doc.getValues("FilingDate")>  
	<#assign OrderType = doc.getValues("OrderType")>   
	<#assign Judge = doc.getValues("Judge")>   
	
	<#assign loopCounter = 0>
	
   <tr>
	<td>${OrderEntryDate[loopCounter]?replace('|', '&nbsp;')}</td>
	 <#--<td>${OrderBook[loopCounter]?replace('|', '&nbsp;')}</td>-->
	 <#--<td>${FilingDate[loopCounter]?replace('|', '&nbsp;')}</td>-->
	<td>${OrderType[loopCounter]?replace('|', '&nbsp;')}</td>
	<td>${Judge[loopCounter]?replace('|', '&nbsp;')}</td>
	
	
	<#assign loopCounter = loopCounter + 1>
   </tr>

 </#foreach>
<#else>
	<table id="orderbook" class="tablesorter">
	 <thead><tr>
	  <th>${searchResult.datasetConfiguration.getColumn("OrderEntryDate").displayName}</th>
	   <#--<th>${searchResult.datasetConfiguration.getColumn("OrderBook").displayName}</th>-->
	   <#--<th>${searchResult.datasetConfiguration.getColumn("FilingDate").displayName}</th>-->
	  <th>${searchResult.datasetConfiguration.getColumn("OrderType").displayName}</th>
	  <th>${searchResult.datasetConfiguration.getColumn("Judge").displayName}</th>


	 </tr></thead>
	 <tbody>
	<#assign ResultId =0>
	 <#foreach doc in searchResult.docs>

	   <tr>
		<td></td>
		 <#--<td></td>-->
		 <#--<td></td>-->
		<td></td>
		<td></td>
	   </tr>

	 </#foreach>
</#if>
 </tbody>
</table>


<script type="text/javascript">
$(document).ready(function() {
	oTable = $('#orderbook').dataTable({
		"bJQueryUI": true,
		"bPaginate": false,
		"bLengthChange": false,
		"bFilter": false,
		"bSort": true,
		"bInfo": false,
		"bAutoWidth": false,
		"aaSorting": []
	});
} );
</script>