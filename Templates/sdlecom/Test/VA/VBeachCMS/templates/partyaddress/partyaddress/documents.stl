<b>Address Information</b>
<#if searchResult.docs?has_content>
	<table id="address" class="tablesorter">
		<thead>
			<tr>
				<th>${searchResult.datasetConfiguration.getColumn("AddressType").displayName}</th>
				<th>${searchResult.datasetConfiguration.getColumn("AddressLine1").displayName}</th>
				<th>${searchResult.datasetConfiguration.getColumn("AddressLine2").displayName}</th>
				<th>${searchResult.datasetConfiguration.getColumn("City").displayName}</th>
				<th>${searchResult.datasetConfiguration.getColumn("State").displayName}</th>
				<th>${searchResult.datasetConfiguration.getColumn("Zip").displayName}</th>
				<th>${searchResult.datasetConfiguration.getColumn("ZipExt").displayName}</th>
				<th>${searchResult.datasetConfiguration.getColumn("County").displayName}</th>
				<th>${searchResult.datasetConfiguration.getColumn("Mailing").displayName}</th>
				<th>${searchResult.datasetConfiguration.getColumn("CurrentAddress").displayName}</th>
			</tr>
		</thead>
		<tbody>
		<#assign ResultId =0>
		<#foreach doc in searchResult.docs>
		<#assign ResultId =0>
		  
					<#assign AddressType = doc.getValues("AddressType")>  
					<#assign AddressLine1 = doc.getValues("AddressLine1")>  
					<#assign AddressLine2 = doc.getValues("AddressLine2")>  
					<#assign City = doc.getValues("City")>  
					<#assign State = doc.getValues("State")>    
					<#assign Zip = doc.getValues("Zip")>  
					<#assign ZipExt = doc.getValues("ZipExt")>  
					<#assign County = doc.getValues("County")>  					
					<#assign Mailing = doc.getValues("Mailing")>
					<#assign CurrentAddress = doc.getValues("CurrentAddress")>  									
					 <#assign loopCounter = 0>
		<#foreach doc in searchResult.docs>
		<tr>
					<td>${AddressType [loopCounter]?replace('|', '&nbsp;')}</td>
					<td>${AddressLine1 [loopCounter]?replace('|', '&nbsp;')}</td>
					<td>${AddressLine2 [loopCounter]?replace('|', '&nbsp;')}</td>
					<td>${City [loopCounter]?replace('|', '&nbsp;')}</td>
					<td>${State [loopCounter]?replace('|', '&nbsp;')}</td>
					<td>${Zip [loopCounter]?replace('|', '&nbsp;')}</td>
					<td>${ZipExt [loopCounter]?replace('|', '&nbsp;')}</td>
					<td>${County [loopCounter]?replace('|', '&nbsp;')}</td>
					<td>${Mailing [loopCounter]?replace('|', '&nbsp;')}</td>
					<td>${CurrentAddress [loopCounter]?replace('|', '&nbsp;')}</td>					
					
					 <#assign loopCounter = loopCounter + 1>
				</tr>
		</#foreach>
		</#foreach>
		</tbody>
	</table>
<#else>
<table id="address" class="tablesorter">
		<thead>
			<tr>
				<th>${searchResult.datasetConfiguration.getColumn("AddressType").displayName}</th>
				<th>${searchResult.datasetConfiguration.getColumn("AddressLine1").displayName}</th>
				<th>${searchResult.datasetConfiguration.getColumn("AddressLine2").displayName}</th>
				<th>${searchResult.datasetConfiguration.getColumn("City").displayName}</th>
				<th>${searchResult.datasetConfiguration.getColumn("State").displayName}</th>
				<th>${searchResult.datasetConfiguration.getColumn("Zip").displayName}</th>
				<th>${searchResult.datasetConfiguration.getColumn("ZipExt").displayName}</th>
				<th>${searchResult.datasetConfiguration.getColumn("County").displayName}</th>
				<th>${searchResult.datasetConfiguration.getColumn("Mailing").displayName}</th>
				<th>${searchResult.datasetConfiguration.getColumn("CurrentAddress").displayName}</th>
			</tr>
		</thead>
		<tbody>
		<#assign ResultId =0>
		<#foreach doc in searchResult.docs>
			<tr>
				<td>No additional information provided.</td>
				<td></td>
				<td></td>
				<td></td>
				<td></td>
				<td></td>
				<td></td>
				<td></td>
				<td></td>
				<td></td>
			</tr>
		</#foreach>
		</tbody>
	</table>
</#if>

<script type="text/javascript">
	$(document).ready(function() {
	oTable = $('#address').dataTable({
		"bJQueryUI": true,
		"bPaginate": false,
		"bLengthChange": false,
		"bFilter": false,
		"bSort": true,
		"bInfo": false,
		"bAutoWidth": false,
		"aaSorting": []
	});
} );
</script>
