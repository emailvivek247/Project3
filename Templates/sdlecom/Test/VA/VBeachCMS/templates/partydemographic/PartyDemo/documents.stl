<b>Demographics</b>
<#if searchResult.docs?has_content>
	<table id="demo" class="tablesorter">
		<thead>
			<tr>
				<th>${searchResult.datasetConfiguration.getColumn("Height").displayName}</th>
				<th>${searchResult.datasetConfiguration.getColumn("Weight").displayName}</th>
				<th>${searchResult.datasetConfiguration.getColumn("Race").displayName}</th>
				<th>${searchResult.datasetConfiguration.getColumn("HairColor").displayName}</th>
				<th>${searchResult.datasetConfiguration.getColumn("EyeColor").displayName}</th>
				<th>${searchResult.datasetConfiguration.getColumn("MilitaryBranch").displayName}</th>
				<th>${searchResult.datasetConfiguration.getColumn("DemographicComment").displayName}</th>
				<th>${searchResult.datasetConfiguration.getColumn("Sex").displayName}</th>
				<th>${searchResult.datasetConfiguration.getColumn("Profession").displayName}</th>
				<th>${searchResult.datasetConfiguration.getColumn("SSN").displayName}</th>
				<th>${searchResult.datasetConfiguration.getColumn("Ethnicity").displayName}</th>
				<th>${searchResult.datasetConfiguration.getColumn("DOB").displayName}</th>
				<th>${searchResult.datasetConfiguration.getColumn("Current").displayName}</th>
			</tr>
		</thead>
		<tbody>
		<#foreach doc in searchResult.docs>
		<#assign ResultId =0>
		 
					<#assign Height = doc.getValues("Height")> 
					<#assign Weight = doc.getValues("Weight")>  
					<#assign Race = doc.getValues("Race")>  
					<#assign HairColor = doc.getValues("HairColor")>  
					<#assign EyeColor = doc.getValues("EyeColor")>    
					<#assign MilitaryBranch = doc.getValues("MilitaryBranch")>  
					<#assign DemographicComment = doc.getValues("DemographicComment")>  
					<#assign Sex = doc.getValues("Sex")>  
					<#assign Profession = doc.getValues("Profession")>  
					<#assign SSN = doc.getValues("SSN")>
					<#assign Ethnicity = doc.getValues("Ethnicity")>  
					<#assign DOB = doc.getValues("DOB")>
					<#assign Current = doc.getValues("Current")>
					
					 <#assign loopCounter = 0>
		<#foreach PartyID in doc.getValues("PartyID")>
			<tr>				
				<td>${Height [loopCounter]?replace('|', '&nbsp;')}</td>
				<td>${Weight [loopCounter]?replace('|', '&nbsp;')}</td>
				<td>${Race [loopCounter]?replace('|', '&nbsp;')}</td>
				<td>${HairColor [loopCounter]?replace('|', '&nbsp;')}</td>
				<td>${EyeColor [loopCounter]?replace('|', '&nbsp;')}</td>
				<td>${MilitaryBranch [loopCounter]?replace('|', '&nbsp;')}</td>
				<td>${DemographicComment [loopCounter]?replace('|', '&nbsp;')}</td>
				<td>${Sex [loopCounter]?replace('|', '&nbsp;')}</td>
				<td>${Profession [loopCounter]?replace('|', '&nbsp;')}</td>
				<td>${SSN [loopCounter]?replace('|', '&nbsp;')}</td>
				<td>${Ethnicity [loopCounter]?replace('|', '&nbsp;')}</td>
				<td>${DOB [loopCounter]?replace('|', '&nbsp;')}</td>
				<td>${Current [loopCounter]?replace('|', '&nbsp;')}</td>
					 <#assign loopCounter = loopCounter + 1>
			</tr>
		</#foreach>
		<#assign caseNumberKey = doc.getValues("PartyID")>
		</#foreach>
		</tbody>
	</table>
		<#else>
			<table id="demo" class="tablesorter">
		<thead>
			<tr>
				<th>${searchResult.datasetConfiguration.getColumn("Height").displayName}</th>
				<th>${searchResult.datasetConfiguration.getColumn("Weight").displayName}</th>
				<th>${searchResult.datasetConfiguration.getColumn("Race").displayName}</th>
				<th>${searchResult.datasetConfiguration.getColumn("HairColor").displayName}</th>
				<th>${searchResult.datasetConfiguration.getColumn("EyeColor").displayName}</th>
				<th>${searchResult.datasetConfiguration.getColumn("MilitaryBranch").displayName}</th>
				<th>${searchResult.datasetConfiguration.getColumn("DemographicComment").displayName}</th>
				<th>${searchResult.datasetConfiguration.getColumn("Sex").displayName}</th>
				<th>${searchResult.datasetConfiguration.getColumn("Profession").displayName}</th>
				<th>${searchResult.datasetConfiguration.getColumn("SSN").displayName}</th>
				<th>${searchResult.datasetConfiguration.getColumn("Ethnicity").displayName}</th>
				<th>${searchResult.datasetConfiguration.getColumn("DOB").displayName}</th>
				<th>${searchResult.datasetConfiguration.getColumn("Current").displayName}</th>
			</tr>
		</thead>
		<tbody>
		<#assign ResultId =0>
		<#foreach doc in searchResult.docs>
			<tr>
				<td>No additional information provided</td>
				<td></td>
				<td></td>
				<td></td>
				<td></td>
				<td></td>
				<td></td>
				<td></td>
				<td></td>
				<td></td>
				<td></td>
				<td></td>
				<td></td>
			</tr>
		</#foreach>
		</tbody>
	</table>  
</#if>
<script type="text/javascript">
	$(document).ready(function() {
		oTable = $('#demo').dataTable({
			"bJQueryUI": true,
			"bPaginate": false,
			"bLengthChange": false,
			"bFilter": false,
			"bSort": true,
			"bInfo": false,
			"bAutoWidth": false,
			"aaSorting": []
		});
	});
</script>




