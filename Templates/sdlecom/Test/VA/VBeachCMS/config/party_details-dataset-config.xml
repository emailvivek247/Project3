  <dataquery name="WorkingQueue" batch-size="128" is-skipping-null-parameters="false">
    <sql><![CDATA[
SELECT DISTINCT 
A.PERSON_ALIAS_ID AS 'PersonSPN',
A.PARTY_ID AS 'PartyID',
CASE WHEN LTRIM(RTRIM(A.RSTRCD_PARTY_FLAG)) = '' THEN 'N' ELSE ISNULL(A.RSTRCD_PARTY_FLAG, 'N') END AS 'PartyConfidential',
A.DATE_TIME_MOD,
B.PERSON_ALIAS_ID AS 'PersonSPN',
CASE WHEN LTRIM(RTRIM(B.FIRST_NAME)) = '' THEN '|' ELSE ISNULL(B.FIRST_NAME, '|') END AS 'FirstName',
CASE WHEN LTRIM(RTRIM(B.MIDDLE_NAME)) = '' THEN '|' ELSE ISNULL(B.MIDDLE_NAME, '|') END AS 'MiddleName',
CASE WHEN LTRIM(RTRIM(B.LAST_NAME)) = '' THEN '|' ELSE ISNULL(B.LAST_NAME, '|') END AS 'LastName',
CASE WHEN LTRIM(RTRIM(B.SUFFIX_NAME)) = '' THEN '|' ELSE ISNULL(B.SUFFIX_NAME, '|') END AS 'SuffixName',
CASE WHEN LTRIM(RTRIM(B.SEALED_FLAG)) = '' THEN 'N' ELSE ISNULL(B.SEALED_FLAG, 'N') END AS 'PersonSealed'
FROM ACM_PARTY A
INNER JOIN ACM_PERSON B
ON A.PERSON_ALIAS_ID = B.PERSON_ALIAS_ID
WHERE VOID <> 'Y'  
ORDER BY DATE_TIME_MOD DESC
    ]]></sql>
