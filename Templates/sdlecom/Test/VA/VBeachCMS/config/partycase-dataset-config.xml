  <dataquery name="WorkingQueue" batch-size="128" is-skipping-null-parameters="false">
    <sql><![CDATA[
SELECT DISTINCT 
 CASE WHEN LTRIM(RTRIM(FC.CASE_NUMBER)) = '' THEN '|' ELSE ISNULL(FC.CASE_NUMBER, '|') END AS 'CaseNumber'
,FC.CASE_ID AS 'CaseID'
,CASE WHEN LTRIM(RTRIM(FC.COURT_CD)) = '' THEN '|' ELSE ISNULL(FC.COURT_CD, '|') END AS 'CourtType'
,P.PERSON_ALIAS_ID AS 'PersonSPN'
,P.PARTY_ID AS 'PartyID'
FROM ACM_FACTS_CASE FC
INNER JOIN ACM_PARTY P ON P.CASE_ID=FC.CASE_ID
    ]]></sql>
