  <dataquery name="WorkingQueue" batch-size="128" is-skipping-null-parameters="false">
    <sql><![CDATA[
SELECT
 DOCUMENT_INFORMATION.ID
,FILE_SERVER
,FILE_PATH
,SPLIT_BATCH_ACCESS.SPLIT_FOLDER
,DOCUMENT_INFORMATION.DOCUMENT_NAME
,DOCUMENT_DESC
FROM DOCUMENT_INFORMATION
INNER JOIN SPLIT_BATCH_ACCESS on DOCUMENT_INFORMATION.SPLIT_BATCH_ACCESS_ID = SPLIT_BATCH_ACCESS.id
INNER JOIN BATCHACCESS ON BATCHACCESS.BATCHACCESS_ID = SPLIT_BATCH_ACCESS.BATCHACCESS_ID
INNER JOIN BATCH ON BATCH.BATCH_ID = BATCHACCESS.BATCH_ID
    ]]></sql>
