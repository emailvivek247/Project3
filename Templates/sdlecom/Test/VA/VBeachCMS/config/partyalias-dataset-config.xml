  <dataquery name="WorkingQueue" batch-size="128" is-skipping-null-parameters="false">
    <sql><![CDATA[
SELECT DISTINCT 
CAST(A.PERSON_ALIAS_ID AS int) AS 'PersonSPN',
A.ALIAS_TYP_DESC AS 'AliasDescription',
A.ALIAS_TYPE AS 'AliasType',
A.FIRST_NAME AS 'AliasFirst',
A.MIDDLE_NAME AS 'AliasMiddle',
A.LAST_NAME AS 'AliasLast',
A.SUFFIX_NAME AS 'AliasSuffix',
A.DATE_TIME_MOD,
P.PARTY_ID AS 'PartyID'
FROM ACM_V_ESALIASDETAIL A
INNER JOIN ACM_PARTY P
ON A.PERSON_ALIAS_ID = P.PERSON_ALIAS_ID
    ]]></sql>
