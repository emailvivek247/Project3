  <dataquery name="WorkingQueue" batch-size="128" is-skipping-null-parameters="false">
    <sql><![CDATA[
SELECT 
	 RECORD_ID AS 'RecordID'
	,CASE WHEN LTRIM(RTRIM(CONVERT(VARCHAR(10), Order_Date, 101))) = '' THEN '|' ELSE ISNULL(CONVERT(VARCHAR(10), Order_Date, 101), '|') END AS 'OrderEntryDate'
	,CASE WHEN LTRIM(RTRIM(CONVERT(VARCHAR(10), Date_Scanned, 101))) = '' THEN '|' ELSE ISNULL(CONVERT(VARCHAR(10), Date_Scanned, 101), '|') END AS 'FilingDate'
	,CASE WHEN LTRIM(RTRIM(Book_Number)) = '' THEN '|' ELSE ISNULL(Book_Number, '|') END AS 'OrderBook'
	,CASE WHEN LTRIM(RTRIM(VA_Case_Number)) = '' THEN '|' ELSE ISNULL(VA_Case_Number, '|') END AS 'CaseNumber'
	,CASE WHEN LTRIM(RTRIM(Judge)) = '' THEN '|' ELSE ISNULL(Judge, '|') END AS 'Judge'
        ,CASE WHEN LTRIM(RTRIM(DLOT.VALUE)) = '' THEN '|' ELSE ISNULL(DLOT.VALUE, '|') END AS 'OrderType'
FROM V_SEARCH_Order_Book VSOB
LEFT JOIN DATA_LOOKUP_Order_Type DLOT ON ISNULL(CONVERT(VARCHAR(10),DLOT.ID),'') = ISNULL(CONVERT(VARCHAR(10),VSOB.ORDER_TYPE),'')
    ]]></sql>
