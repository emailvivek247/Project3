            var searchQuery = '';
            var andOrClause ='';
            var doubleQuotes = '"';
            var counter = 0;
            var textcounter = 0;
            var searchValue = '';
            var searchValueQuery = '';
            for(var i = 0; i < elem.length; i++) {
                if (document.getElementById(elem[i].name + '_select') != null) {
                    counter++;  
                }                   
            }
            for(var i = 0; i < elem.length; i++) {
                if (document.getElementById(elem[i].name + '_select') != null) {
                    setCookie('Mobile' + elem[i].name + '_select', document.getElementById(elem[i].name + '_select').selectedIndex, 0);
