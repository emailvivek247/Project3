    function postAdvancedSearch() {
        try {
            if (!doDateCheck('frmRecDate','toRecDate'))return;
            document.getElementById("q").value = '';
            var inputElemenets =  $('input[type="text"],select#DocTypeDesc,select#combobox');
            var elem = unique(inputElemenets);
