<b>Parties</b>
<#if searchResult.docs?has_content>
<table id="party" class="tablesorter">
 <thead><tr>
  <th>${searchResult.datasetConfiguration.getColumn("PartyName").displayName}</th>
  <th>${searchResult.datasetConfiguration.getColumn("PartyRole").displayName}</th>
  <th>${searchResult.datasetConfiguration.getColumn("PartyIdentifier").displayName}</th>
  <th>${searchResult.datasetConfiguration.getColumn("PartyStatus").displayName}</th>
  <th>${searchResult.datasetConfiguration.getColumn("PartyStatusDate").displayName}</th>
  <th>${searchResult.datasetConfiguration.getColumn("AttorneyName").displayName}</th>
  <th>${searchResult.datasetConfiguration.getColumn("DOD").displayName}</th>
  
  
 </tr></thead>
 <tbody>
<#assign ResultId =0>
 <#foreach doc in searchResult.docs>
  
   <tr>
	<td>${searchResult.highlight(doc, "PartyName")}</td>
	<td>${searchResult.highlight(doc, "PartyRole")}</td>
	<td>${searchResult.highlight(doc, "PartyIdentifier")}</td>
	<td>${searchResult.highlight(doc, "PartyStatus")}</td>
	<td>${pageStyleUtil.format(date.format("yyyy/MM/dd HH:mm:ss z",doc.getDate("PartyStatusDate")), "yyyy/MM/dd HH:mm:ss z", "MM/dd/yyyy HH:mm:ss a")}</td>
	<td>${searchResult.highlight(doc, "AttorneyName")}</td>
	<td>${pageStyleUtil.format(date.format("yyyy/MM/dd HH:mm:ss z",doc.getDate("DOD")), "yyyy/MM/dd HH:mm:ss z", "MM/dd/yyyy")}</td>
 </tr>
 </#foreach>
  </tbody>
</table>
<#else>
 <table id="party" class="tablesorter">
  <thead><tr>
   <th>${searchResult.datasetConfiguration.getColumn("PartyName").displayName}</th>
   <th>${searchResult.datasetConfiguration.getColumn("PartyRole").displayName}</th>
   <th>${searchResult.datasetConfiguration.getColumn("PartyIdentifier").displayName}</th>
   <th>${searchResult.datasetConfiguration.getColumn("PartyStatus").displayName}</th>
   <th>${searchResult.datasetConfiguration.getColumn("PartyStatusDate").displayName}</th>
   <th>${searchResult.datasetConfiguration.getColumn("AttorneyName").displayName}</th>
   <th>${searchResult.datasetConfiguration.getColumn("DOD").displayName}</th>
   
   
  </tr></thead>
  <tbody>
 <#assign ResultId =0>
  <#foreach doc in searchResult.docs>
   
    <tr>
 	<td></td>
 	<td></td>
 	<td></td>
 	<td></td>
 	<td></td>
 	<td></td>
 	<td></td>
  </tr>
  </#foreach>
   </tbody>
</table>
</#if>



<script type="text/javascript">
$(document).ready(function() {
	oTable = $('#party').dataTable({
		"bJQueryUI": true,
		"bPaginate": false,
		"bLengthChange": false,
		"bFilter": false,
		"bSort": true,
		"bInfo": false,
		"bAutoWidth": false,
		"aaSorting": [[ 0, "asc" ]]
	});
} );
</script>
