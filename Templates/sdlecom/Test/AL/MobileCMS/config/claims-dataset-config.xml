  <dataquery name="Incremental">
    <sql><![CDATA[
SELECT 
	 C.CLAIM_NUMBER AS 'ClaimNumber'
	,C.CLAIM_AMOUNT AS 'ClaimAmount'
	,C.CASE_ID AS 'CaseID'
	,CS.CLAIM_STATUS_DESC AS 'ClaimStatus'
	,'FiledBy' AS 'FiledBy'
    ,'FiledAgainst' AS 'FiledAgainst'
FROM ACM_CLAIM C
LEFT JOIN ACM_CLAIM_STATUS CS ON C.CLAIM_STATUS_ID = CS.CLAIM_STATUS_ID 
LEFT JOIN ACM_CLAIM_PARTY CP ON C.CLAIM_ID = CP.CLAIM_ID
LEFT JOIN ACM_PARTY P ON CP.PARTY_ID = P.PARTY_ID
LEFT JOIN ACM_PERSON PER ON P.PERSON_ALIAS_ID = PER.PERSON_ALIAS_ID
WHERE C.DATE_TIME_CREATED >  DATEADD(HH,-2,GETDATE())
OR C.DATE_TIME_MOD >  DATEADD(HH,-2,GETDATE())
    ]]></sql>
  </dataquery>
