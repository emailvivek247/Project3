   ,R.DATE_TIME_MOD
FROM AFM_REGISTRY R
LEFT JOIN ACM_PERSON P ON R.FUNDS_INCUSTODYOF = P.PERSON_ALIAS_ID
WHERE R.DATE_TIME_CREATED >  DATEADD(HH,-2,GETDATE())
OR R.DATE_TIME_MOD >  DATEADD(HH,-2,GETDATE())
