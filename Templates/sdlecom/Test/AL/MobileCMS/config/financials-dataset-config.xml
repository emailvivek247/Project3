  <dataquery name="Incremental">
    <sql><![CDATA[
SELECT 
	R.BANKACCOUNT_NUM AS 'BankAccount'
   ,R.REGISTRY_NUM AS 'TrustFundAcc'
   ,R.REGISTRY_DATE AS 'AccountDate'
   ,R.TAXID_NUM AS 'TaxID'
   ,R.PAYOUT_DATE AS 'PayoutDate'
