<#assign rssUrl = "?indexName=" + indexName + "&templateName=" + templateName + "&q=" + URLEncodedQuery + "&rss=1">
<@com.useHighlightTag '<span style="background-color:#00ff00">', '</span>' />

<#include "layout.stl">

<@layout title="Dashboard">

<#include "/templates/globalNav.stl">

<#if request.getServletContext().getInitParameter("isSecurityEnabled") == "false" || (request.isUserInRole('ROAM_ADMIN') || request.isUserInRole('MOBILE_ADMIN') || request.isUserInRole('MOBILE_FREE_ACCESS'))>

<br />

<@com.errorMarkup/>
<table align="center" border="0" cellpadding="0" cellspacing="0" >
      <tr>
        <td align = "right"><a style="color:#2E7EE4; font-size:90%;" href = "${request.getContextPath()}/search.do?indexName=dash_opr_count&q=&length=10000&sortBy=YEAR&desc=N">Timeline Statistics</a></td>
        <td width="10px" align = "right"></td>
        <td align = "right"><a class="active" style="color:#2E7EE4; font-size:90%;" href = "${request.getContextPath()}/search.do?indexName=dash_opr_count&templateName=dashboarddocdist&q=">Document Trend</a></td>        
</table>

<table width="100%" height="100%" align="center" >
<tr width="100%" valign="top">
<td valign="middle">
<fieldset  border-width="0">
    <legend>Yearly Document list</legend>
        </br><select onchange="getAllDoctypes(this.options[this.selectedIndex].text)" name="selYears" id="selYears"></select>
    <select multiple name="fromBox[]" id="fromBox"></select>
    <select multiple name="toBox[]" id="toBox"></select>         
</fieldset>
</td>
<td valign="middle">
<div id="docTypeChartDiv" class="docTypeChartDiv" align="center"></div>
</td>
</tr>
</table>
<script type="text/javascript">
    createMovableOptions("fromBox","toBox",400,600,'Available Document Types','Selected Document Types');
</script>


<#include "documents.stl">
<#else>
<br /><br /><br />
