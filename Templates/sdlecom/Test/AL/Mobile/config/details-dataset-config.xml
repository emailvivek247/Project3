    <sql><![CDATA[
SELECT 
	A.SEQ_KEY AS 'Instrument'
   ,CASE WHEN LTRIM(RTRIM(B.PROPERTY)) = '' THEN '|' ELSE ISNULL(B.PROPERTY, '|') END AS 'Property'
   ,CASE WHEN LTRIM(RTRIM(A.PROPERTYVALUE)) = '' THEN '|' ELSE ISNULL(A.PROPERTYVALUE, '|') END AS 'PropertyValue'
FROM DOCUMENT_DETAIL A, 
	 PROPERTIES B
WHERE a.propertyid=b.propertyid 
	--AND B.PUBLIC_FLG = 'Y'
	AND A.SEQ_KEY IN (?)
