            CASE WHEN LTRIM(RTRIM(CORP)) = '' THEN '|' ELSE ISNULL(CORP, '|') END AS 'Corp',
            ISNULL(SEQ, '-1') AS 'EntitySeq',
            CASE WHEN LTRIM(RTRIM(ADDRESS1)) = '' THEN '|' ELSE ISNULL(ADDRESS1, '|') END AS 'EntityAddress1',
            CASE WHEN LTRIM(RTRIM(ADDRESS2)) = '' THEN '|' ELSE ISNULL(ADDRESS2, '|') END AS 'EntityAddress2',
            CASE WHEN LTRIM(RTRIM(CITY)) = '' THEN '|' ELSE ISNULL(CITY, '|') END AS 'EntityCity',
            CASE WHEN LTRIM(RTRIM(STATE)) = '' THEN '|' ELSE ISNULL(STATE, '|') END AS 'EntityState',
            CASE WHEN LTRIM(RTRIM(ZIP)) = '' THEN '|' ELSE ISNULL(ZIP, '|') END AS 'EntityZip'
FROM ENTITY E
WHERE SEQ_KEY IN (?)
    ]]></sql>
