    <sql><![CDATA[
SELECT
	 SEQ_KEY AS 'Instrument'
	,CASE WHEN LTRIM(RTRIM(LEGAL_TYPE)) = '' THEN '|' ELSE ISNULL(LEGAL_TYPE, '|') END AS 'LegalType'
	,ISNULL(SEQ, '-1') AS 'LegalSeq'
	,CASE WHEN LTRIM(RTRIM(LEGAL1)) = '' THEN '|' ELSE ISNULL(LEGAL1, '|') END AS 'Con'
	,CASE WHEN LTRIM(RTRIM(LEGAL2)) = '' THEN '|' ELSE ISNULL(LEGAL2, '|') END AS 'TT'
	,CASE WHEN LTRIM(RTRIM(ACREAGE)) = '' THEN '|' ELSE ISNULL(ACREAGE, '|') END AS 'Area'
	,CASE WHEN LTRIM(RTRIM(QUARTER)) = '' THEN '|' ELSE ISNULL(QUARTER, '|') END AS 'Quarter'
	,CASE WHEN LTRIM(RTRIM(BLOCKNUM)) = '' THEN '|' ELSE ISNULL(BLOCKNUM, '|') END AS 'Block'     
	,CASE WHEN LTRIM(RTRIM(PARCEL)) = '' THEN '|' ELSE ISNULL(PARCEL, '|') END AS 'Parcel'
	,CASE WHEN LTRIM(RTRIM(GPIN)) = '' THEN '|' ELSE ISNULL(GPIN, '|') END AS 'PID'
	,CASE WHEN LTRIM(RTRIM(LEGALDESCRIPTION)) = '' THEN '|' ELSE ISNULL(LEGALDESCRIPTION, '|') END AS 'LegalDescription'            
	,CASE WHEN LTRIM(RTRIM(PHASE)) = '' THEN '|' ELSE ISNULL(PHASE, '|') END AS 'Phase'
	,CASE WHEN LTRIM(RTRIM(SECTIONNUM)) = '' THEN '|' ELSE ISNULL(SECTIONNUM, '|') END AS 'Section'
	,CASE WHEN LTRIM(RTRIM(LOTNUM)) = '' THEN '|' ELSE ISNULL(LOTNUM, '|') END AS 'LotNum'
	,CASE WHEN LTRIM(RTRIM(LOTLETTER)) = '' THEN '|' ELSE ISNULL(LOTLETTER, '|') END AS 'Lot'
	,CASE WHEN LTRIM(RTRIM(MAPBOOK)) = '' THEN '|' ELSE ISNULL(MAPBOOK, '|') END AS 'MapBook'
    ,CASE WHEN LTRIM(RTRIM(MAPPAGE)) = '' THEN '|' ELSE ISNULL(MAPPAGE, '|') END AS 'MapPage'
	,CASE WHEN LTRIM(RTRIM(SUBDIVISION)) = '' THEN '|' ELSE ISNULL(SUBDIVISION, '|') END AS 'Subdivision'
