SELECT SEQ_KEY AS 'Instrument',
            (SELECT ROLE FROM DOCROLES WHERE ROLEID = E.ROLE) AS 'EntityRole',
            CASE WHEN LTRIM(RTRIM(LNAME+' '+ISNULL(FNAME,'')+' '+ ISNULL(MNAME,'') )) = '' THEN '|' ELSE ISNULL(LNAME+' '+ISNULL(FNAME,'')+' '+ ISNULL(MNAME,'') , '|') END AS 'EntityName',
