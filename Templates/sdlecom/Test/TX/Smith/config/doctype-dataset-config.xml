  <dataquery name="WorkingQueue" batch-size="128" is-skipping-null-parameters="false">
    <sql><![CDATA[
select distinct
	  a.code AS "DocTypeCode"
	 ,LTRIM(RTRIM(a.descr)) AS "DocTypeDesc"
	 ,LTRIM(RTRIM(a.descr)) AS "DocTypeDisplay"
from DOCTYPE1LOV a 
inner join PUBLICSEARCH b on a.CODE = b.CODE
AND b.indextype not in ('6','12','13')
and b.doctype not in (4,14,37,54,71,79, 80, 81,84,85,163,164,165,166,169,183)
and a.code not in ('VOIDLR','VOIDML','VOIDNO','VOIDMD','VOIDDE','VOIDBR','VOID')
order by LTRIM(RTRIM(a.descr)) asc
    ]]></sql>
