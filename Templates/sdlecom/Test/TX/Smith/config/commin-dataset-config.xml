  <dataquery name="WorkingQueue" batch-size="128" is-skipping-null-parameters="false">
    <sql><![CDATA[
SELECT 
  DE.DOCUMENT_EXTRACT_ID
 ,DE.DOCUMENT_INFORMATION_ID AS 'Instrument'
 ,DE.PAGE_NUMBER AS 'Page'
 ,DE.TEXT AS 'FullText'
 ,P.RECDATE1 AS 'RecDate'
 ,CONVERT(VARCHAR(30), P.RECDATE1, 120) AS 'RecDateDisplay'
 ,CONVERT(VARCHAR(10), P.RECDATE1, 112) AS 'RecDateSearch'
 ,LTRIM(RTRIM(P.CODE)) AS 'DocType'
 ,LTRIM(RTRIM(P.CODE)) AS 'DocTypeFilter'
 ,LTRIM(RTRIM(C.descr)) AS 'DocTypeDesc'
 ,LTRIM(RTRIM(C.descr)) AS 'DocTypeDescDisplay'
 ,P.FIRSTLEGAL AS 'Remarks'
 ,DE.DATE_TIME_MOD
FROM AILIS_SMITH_TEST_OCR.dbo.DOCUMENT_EXTRACT DE
LEFT JOIN SMITH_73_TEST.DBO.PUBLICSEARCH P ON DE.DOCUMENT_INFORMATION_ID = P.SEQ_KEY
LEFT JOIN SMITH_73_TEST.DBO.DOCTYPE1LOV c on P.CODE = c.CODE 
WHERE P.OFFLINERECORD <> 'Y'
    ]]></sql>
