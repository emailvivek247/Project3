,LTRIM(RTRIM(a.code)) as DocType
,(isnull (a.bookshort,'')+ ' ' + isnull(a.BOOK,'') + ' '+ isnull(a.PAGE,'')) AS 'BookPage'
,Book
,Page
,LOTNUM as 'Lot'
,UNIT AS 'Unit'
,SECTIONNUM as 'SectionNum'
,PHASE AS 'Phase'
,BUILDING AS 'Building'
,ACREAGE AS 'Acreage'
,BLOCKNUM as 'Block'
,SUBDIVISION as 'Subdivision'
,TOWNSHIP AS 'Township'
,CITY as CityTown
,LDESC as 'Other'
,LEGAL1 as 'ProjectNum'
,LEGAL2 as 'RdNum'
,LDESC2 as 'Desc2'
,LDESC3 as 'Desc3'
,GPIN
,'' AS 'Status'
,LegalDescription
,MULTIPLE_LEGAL
,FirstReference
,MULTIPLE_REFERENCE
,a.date_time_mod
,b.seq
,offlinerecord
,'Orange' as Location
,a.indextype as IndexCode
,a.seq as PartySeq
,a.doctype as DocCode
      from PUBLICSEARCH a with (nolock) left outer join LEGAL b with (nolock) on
      a.SEQ_KEY = b.SEQ_KEY 
      INNER JOIN ROAM_TRACK RT with (nolock) ON RT.PK_KEY = A.ID
      AND RT.TAB_NAME = 'PUBLICSEARCH' 
      LEFT OUTER JOIN ROAM_TRACK RT1 with (nolock) ON RT1.PK_KEY = B.SEQ_KEY
      AND RT1.TAB_NAME = 'LEGAL' 
      where OFFLINERECORD ='N' 
      AND a.indextype not in (6,8,12,13,14)
      AND a.doctype not in (168,169,170,171,172,173,174)
	  AND (RT.DATE_TIME_MOD > ? OR RT1.DATE_TIME_MOD > ?)
order by RT.date_time_mod desc
    ]]></sql>
