  <dataquery name="WorkingQueue" batch-size="128" is-skipping-null-parameters="false">
    <sql><![CDATA[
SELECT 
    a.SEQ_KEY AS SEQ_KEY, 
    CAST(a.SEQ_KEY AS varchar(30)) AS SEQKEY, 
    a.INSTNUM AS REFNUM,
    a.FILENUM_OLD AS LEGACYREFNNUM,
    b.DOC_TYPE,
    a.date_time_mod
FROM DOCREF a INNER JOIN DOCUMENT b 
  ON b.SEQ_KEY = a.INSTNUM
WHERE ACTIVE = 'Y' ORDER BY SEQ_KEY DESC
    ]]></sql>
