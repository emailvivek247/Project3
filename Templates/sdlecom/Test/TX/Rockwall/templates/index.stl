                        </br>
                        </br>

                <#elseif (pageStyleUtil.isUserInRoleGroup('UNOFFICIAL_ROLE', request) || pageStyleUtil.isUserInRoleGroup('UNOFFICIAL_ROLE', request)) && (dc.name == "historical" || dc.name == "histbook" || dc.name == "histindex")>
