  <dataquery name="WorkingQueue" batch-size="128" is-skipping-null-parameters="false">
    <sql><![CDATA[
SELECT
	 CLAIMS.DISPLAYNAME AS 'Claimant'
	,CLAIMS.CLAIM_AMOUNT AS 'Amount'
	,CLAIMS.CLAIM_DATE AS 'ClaimDate'
	,CLAIMS.CLAIM_STATUS_DESC AS 'Status'
	,CLAIMS.FILLED_DATE AS 'DateFiled'
	,CT.CASE_TYP_DESC AS 'CaseType'
        ,CLAIMS.CASE_ID AS 'CaseID'
FROM MCPC_ACM_V_GCI_CLAIMS CLAIMS
INNER JOIN ACM_FACTS_CASE FC ON FC.CASE_ID = CLAIMS.CASE_ID
LEFT JOIN ACM_CASE_TYP CT ON FC.CASE_TYP_ID = CT.CASE_TYP_ID
WHERE CT.CASE_TYP_CD NOT IN ('ADP','COM')
AND FC.SEALED_CASE_FLG <> 'Y'
AND FC.IS_RESTRICTED_FLG <> 'Y'
AND FC.REMOVE_PUBLICACCESS_FLG <> 'Y'
    ]]></sql>
