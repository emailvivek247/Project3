  <dataquery name="WorkingQueue" batch-size="128" is-skipping-null-parameters="false">
    <sql><![CDATA[
SELECT DISTINCT
	FC.CASE_NUMBER AS 'CaseNumber'
   ,FC.CASE_ID AS 'CaseID'
   ,PA.DOD AS 'DOD'
   ,AB.BALANCE_AMT AS 'BondAmount'
   ,PERS.DISPLAYNAME AS 'PartyName'
   ,PRT.PARTY_ROLE_DESC AS 'PartyRole'
   ,CT.CASE_TYP_DESC AS 'CaseType'
   ,CASE WHEN AB.VOID_FLG IS NULL THEN 'N'
   WHEN AB.VOID_FLG = 'N' THEN 'N'
   WHEN AB.VOID_FLG = 'Y' THEN 'Y' END AS 'VoidFlag'
   ,FC.DATE_TIME_MOD
FROM ACM_FACTS_CASE FC
INNER JOIN ACM_PARTY P ON P.CASE_ID = FC.CASE_ID 
INNER JOIN ACM_PERSON PERS ON PERS.PERSON_ALIAS_ID = P.PERSON_ALIAS_ID
LEFT JOIN ACM_PERSON_AGE PA ON PA.PERSON_ALIAS_ID = P.PERSON_ALIAS_ID
LEFT JOIN ACM_PARTY_ROLE_TYP PRT ON PRT.PARTY_ROLE_TYP_ID = P.PARTY_ROLE_TYP_ID
LEFT JOIN ACM_BOND_PARTY ABP ON ABP.PARTY_ID = P.PARTY_ID
LEFT JOIN ACM_BOND AB ON AB.BOND_ID = ABP.BOND_ID
LEFT JOIN ACM_CASE_TYP CT ON FC.CASE_TYP_ID = CT.CASE_TYP_ID
LEFT JOIN ACM_ATTORNEY_PARTY AP ON P.PARTY_ID = AP.PARTY_ID
WHERE CT.CASE_TYP_CD NOT IN ('ADP','COM')
AND FC.SEALED_CASE_FLG <> 'Y'
AND FC.IS_RESTRICTED_FLG <> 'Y'
AND FC.REMOVE_PUBLICACCESS_FLG <> 'Y'
AND P.VOID <> 'Y'
AND ISNULL(AP.INACTIVE_FLG,'') <> 'Y'
    ]]></sql>
