   ,FC.CASE_FILING_DT AS 'CaseFilingDateFilter'
   ,CONVERT(VARCHAR(10), FC.CASE_FILING_DT, 112) AS 'CaseFilingDate' 
   ,CONVERT(VARCHAR(30), FC.CASE_FILING_DT, 120) AS 'CaseFilingDateDisplay'
   ,CFT.CASE_FILING_TYP_DESC AS 'CaseFilingType'
   ,CST.CASE_STATUS_DESCR AS 'CaseStatus'
   ,MAVCJ.DISPLAYNAME AS 'Judge'
   ,FC.CASE_SHORT_TITLE AS 'CaseStyle'
   ,ACT.CASE_TYP_DESC AS 'CaseType'
   ,FC.DATE_TIME_MOD
FROM ACM_FACTS_CASE FC
LEFT JOIN ACM_CASE_STATUS CS ON CS.CASE_ID = FC.CASE_ID
LEFT JOIN ACM_CASE_STATUS_TYP CST ON CST.CASE_STATUS_TYP_ID = CS.CASE_STATUS_TYP_ID
LEFT JOIN ACM_CASE_TYP CT ON FC.CASE_TYP_ID = CT.CASE_TYP_ID
LEFT JOIN ACM_COURT_TYP COURT ON FC.COURT_TYP_ID = COURT.COURT_TYP_ID
LEFT JOIN ACM_CASE_CATEGORY CASECAT ON FC.CASE_CATEGORY_ID = CASECAT.CASE_CATEGORY_ID
LEFT JOIN ACM_CASE_SUB_TYP CSUBTYP ON FC.CASE_SUB_TYP_ID = CSUBTYP.CASE_SUB_TYP_ID
LEFT JOIN ACM_CASE_FILING_TYP CFT ON FC.CASE_FILING_TYP_ID = CFT.CASE_FILING_TYP_ID
LEFT JOIN MCPC_ACM_V_CASE_JUDGE MAVCJ ON FC.CASE_ID = MAVCJ.CASE_ID
LEFT JOIN ACM_CASE_TYP ACT ON FC.CASE_TYP_ID = ACT.CASE_TYP_ID
WHERE ACT.CASE_TYP_CD NOT IN ('ADP','COM')
AND (FC.DATE_TIME_CREATED >  DATEADD(HH,-2,GETDATE())
