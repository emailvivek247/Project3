    <sql><![CDATA[
SELECT
    FC.CASE_NUMBER AS 'CaseNumber'
   ,FC.CASE_ID AS 'CaseID'
   ,COURT.COURT_NAME AS 'CourtType'
   ,COURT.COURT_NAME AS 'CourtTypeFilter'
   ,CASECAT.CATEGORY_DESC AS 'CaseCategory'
