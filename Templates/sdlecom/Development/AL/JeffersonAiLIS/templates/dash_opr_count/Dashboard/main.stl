<#assign rssUrl = "?indexName=" + indexName + "&templateName=" + templateName + "&q=" + URLEncodedQuery + "&rss=1">
<@com.useHighlightTag '<span style="background-color:#00ff00">', '</span>' />
<style type="text/css" media=all>
.blueTr
{
 FONT-SIZE: 10px;
 background-color: #8AB2D7;
 background-image: url(Images/TableHeader.gif);
 background-repeat:repeat-x;
    
}
.textBoldLight
{
 COLOR: #FFFFFF;
 FONT-SIZE: 8pt;
 FONT-WEIGHT: bold;
}
</style>
<#include "layout.stl">
<@layout title="Dashboard">
<#include "/templates/globalNav.stl">
<#if request.getServletContext().getInitParameter("isSecurityEnabled") == "false" || (request.isUserInRole('ROAM_ADMIN') || request.isUserInRole('JEFFERSON_ADMIN') || request.isUserInRole('JEFFERSON_PROBATE_ACCESS'))>

 <@com.errorMarkup/>
<br />
<table align="center" border="0" cellpadding="0" cellspacing="0" >
      <tr align="center">
        <td align="center"><a class="active" style="color:#2E7EE4; font-size:90%;" href = "${request.getContextPath()}/search.do?indexName=dash_opr_count&q=&length=10000&sortBy=YEAR&desc=N">Timeline Statistics</a></td>
        <td width="10px" align = "right"></td>
        <td align="center"><a style="color:#2E7EE4; font-size:90%;" href = "${request.getContextPath()}/search.do?indexName=dash_opr_count&templateName=dashboarddocdist&q=">Document Trend</a></td>        
</table>
<table height="100%" width="100%" valign="middle" align="right">
<tr height="50%" width="100%" align="center">

<td width="60%" height="100%" align="center">
    <div id="yearDistChartDiv" class="yearDistChartDiv" align="center"></div>
    <div id="yearDistGridDiv" class="yearDistGridDiv" align="center"></div>
<input type="image" src="smartcharts/images/viewChart.png" name="image" onClick="toggleChartandGrid('.yearDistChartDiv', '.yearDistGridDiv')"/><input type="image" src="smartcharts/images/viewGrid.png" name="image" onClick="toggleChartandGrid('.yearDistGridDiv', '.yearDistChartDiv')"/>
</td>
<td width="40%" height="100%" align="center">
    <div id="monthDistChartDiv" class="monthDistChartDiv" align="center"></div>
    <div id="monthDistGridDiv" class="monthDistGridDiv" align="center"></div>
<input type="image" src="smartcharts/images/viewChart.png" name="image" onClick="toggleChartandGrid('.monthDistChartDiv', '.monthDistGridDiv')"/><input type="image" src="smartcharts/images/viewGrid.png" name="image" onClick="toggleChartandGrid('.monthDistGridDiv', '.monthDistChartDiv')"/>
</td>
</tr>
<tr height="50%" width="100%" align="center">
<td width="80%" align="center">
    <div id="docTypeDistChartDiv" class="docTypeDistChartDiv" align="center"></div>
    <div id="docTypeDistGridDiv" class="docTypeDistGridDiv" align="center"></div>
<input type="image" src="smartcharts/images/viewChart.png" name="image" onClick="toggleChartandGrid('.docTypeDistChartDiv', '.docTypeDistGridDiv')"/><input type="image" src="smartcharts/images/viewGrid.png" name="image" onClick="toggleChartandGrid('.docTypeDistGridDiv', '.docTypeDistChartDiv')"/>
</td>
<td width="20%">
<div id="fcexpDiv" align="center">SmartCharts Export Handler Component</div>
<script type="text/javascript">
    var myExportComponent = new SmartChartsExportObject("fcExporter1", "smartcharts/charts/FCExporter.swf");
    myExportComponent.componentAttributes.fullMode = '1';
    myExportComponent.exportAttributes.showExportDialog = '0';
        myExportComponent.componentAttributes.defaultExportFileName = "ROAM-Dashboard"
    myExportComponent.componentAttributes.saveMode = 'both';
        myExportComponent.componentAttributes.defaultExportFormat='PDF';
        //myExportComponent.componentAttributes.saveMode = 'batch';
    myExportComponent.componentAttributes.showAllowedTypes = '1';
    myExportComponent.componentAttributes.width = '300';
        myExportComponent.componentAttributes.height = '220';
    myExportComponent.componentAttributes.showMessage = '1';
        myExportComponent.componentAttributes.message = 'Export any chart from context menu and then use this to save';
    myExportComponent.Render("fcexpDiv");
</script>
</td>
</tr>
</table>
<#include "documents.stl">
<#else>
<br /><br /><br />
<table align="center" border="0">
   <tr>
    <td align="center">
       This is premium content. To view dashboards please subscribe to our premium subscription which will provide you access to dashboards and other premium features. <br /><br />You can add the premium access using this link -- <a href="viewAvailableSubscriptions.admin">Add Subscriptions</a><br /><br />
    </td>
   </tr>
</table> 
</#if>
</@layout>