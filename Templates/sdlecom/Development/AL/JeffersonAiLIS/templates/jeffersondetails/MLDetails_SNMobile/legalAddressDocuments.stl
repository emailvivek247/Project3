<b>ADDRESS</b>
<table id="legal" class="tablesorter">
	<thead>
		<tr>
			<th>${searchResult.datasetConfiguration.getColumn("LegalSeq").displayName}</th>
	  		<th>${searchResult.datasetConfiguration.getColumn("Address").displayName}</th>
	  		<th>${searchResult.datasetConfiguration.getColumn("Unit").displayName}</th>
	  		<th>${searchResult.datasetConfiguration.getColumn("CityTown").displayName}</th>
	  		<th>${searchResult.datasetConfiguration.getColumn("State").displayName}</th>
	  		<th>${searchResult.datasetConfiguration.getColumn("Zip").displayName}</th>
	   	</tr>
	</thead>
	<tbody>
	<#assign legalSeq = doc.getValues("LegalSeq") address = doc.getValues("Address") unit = doc.getValues("Unit") 
	      cityTown = doc.getValues("CityTown") state = doc.getValues("State") zip = doc.getValues("Zip") loopCounter = 0>
			<tr>
				<td>${legalSeq[loopCounter]?replace('|', '&nbsp;')}</td>
			    <td>${address[loopCounter]?replace('|', '&nbsp;')}</td>
		  		<td>${unit[loopCounter]?replace('|', '&nbsp;')}</td>
		  		<td>${cityTown[loopCounter]?replace('|', '&nbsp;')}</td>
		  		<td>${state[loopCounter]?replace('|', '&nbsp;')}</td>
		  		<td>${zip[loopCounter]?replace('|', '&nbsp;')}</td>
				<#assign loopCounter = loopCounter + 1>
		    </tr>
	</tbody>
</table>