<b>FREEFORM</b>
<table id="legal" class="tablesorter">
	<thead>
		<tr>
			<th>${searchResult.datasetConfiguration.getColumn("LegalSeq").displayName}</th>
	  		<th>${searchResult.datasetConfiguration.getColumn("Freeform1Other").displayName}</th>
	  		<th>${searchResult.datasetConfiguration.getColumn("Freeform2").displayName}</th>
	  		<th>${searchResult.datasetConfiguration.getColumn("Freeform3").displayName}</th>
	   	</tr>
	</thead>
	<tbody>
		<#assign legalSeq = doc.getValues("LegalSeq")>
		<#assign freeFormOther1 = doc.getValues("Freeform1Other")>
		<#assign freeForm2 = doc.getValues("Freeform2")>
		<#assign freeForm3 = doc.getValues("Freeform3")>
		<#assign loopCounter = 0>
		<tr>
			<td>${legalSeq[loopCounter]?replace('|', '&nbsp;')}</td>
		    <td>${freeFormOther1[loopCounter]?replace('|', '&nbsp;')}</td>
			<td>${freeForm2[loopCounter]?replace('|', '&nbsp;')}</td>
			<td>${freeForm3[loopCounter]?replace('|', '&nbsp;')}</td>
			<#assign loopCounter = loopCounter + 1>
		</tr>
	</tbody>
</table>