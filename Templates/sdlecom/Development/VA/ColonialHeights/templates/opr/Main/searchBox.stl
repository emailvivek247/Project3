                                            <input type="text" id="frmRecDate" name="RecDate" title="${searchResult.datasetConfiguration.getColumn("RecDate").displayName}" size="10" maxlength="10" >
                                            <script language="javascript">
                                                $(function() {
                                                            var dates = $( "#frmRecDate, #toRecDate" ).datepicker({                 
                                                                changeYear: true, 
                                                                changeMonth: true,
                                                                defaultDate: "",
                                                                maxDate: "+0",
                                                                changeMonth: true,
                                                                numberOfMonths: 1,
                                                                yearRange:"1974:+2", 
                                                                onSelect: function( selectedDate ) {
                                                                    var option = this.id == "frmRecDate" ? "minDate" : "maxDate",
                                                                        instance = $( this ).data( "datepicker" );
                                                                        date = $.datepicker.parseDate(
                                                                            instance.settings.dateFormat ||
                                                                            $.datepicker._defaults.dateFormat,
                                                                            selectedDate, instance.settings );
                                                                    dates.not( this ).datepicker( "option", option, date );
                                    },
                                    onClose: function(dateText, inst) {
                                                                                                      $(this).datepicker('option', 'dateFormat', 'mm/dd/yy');                
                                                                }
                                                            });
                                                        });  
                                                if (getCookie('MainfrmRecDate') != null) {
                                                    document.getElementById("frmRecDate").value = getCookie('MainfrmRecDate');
                                                }
                                            </script>
