            CASE WHEN LTRIM(RTRIM(CONVERT(VARCHAR(10), RECDATE1, 101))) = '' THEN '|' ELSE ISNULL(CONVERT(VARCHAR(10), RECDATE1, 101), '|') END AS 'RecDate1',
            CASE WHEN LTRIM(RTRIM(CONVERT(VARCHAR(10), RECDATE2, 101))) = '' THEN '|' ELSE ISNULL(CONVERT(VARCHAR(10), RECDATE2, 101), '|') END AS 'RecDate2',
            PAGECOUNT AS 'PageCount',
            CASE WHEN LTRIM(RTRIM(TEXT1)) = '' THEN '|' ELSE ISNULL(TEXT1, '|') END AS 'Remarks1',
            CASE WHEN LTRIM(RTRIM(TEXT2)) = '' THEN '|' ELSE ISNULL(TEXT2, '|') END AS 'Remarks2',
            CASE WHEN LTRIM(RTRIM(CONVERT(VARCHAR(20), AMOUNT1))) = '' THEN '|' ELSE ISNULL(CONVERT(VARCHAR(20), AMOUNT1), '|') END AS 'Amount1',
            CASE WHEN LTRIM(RTRIM(CONVERT(VARCHAR(20), AMOUNT2))) = '' THEN '|' ELSE ISNULL(CONVERT(VARCHAR(20), AMOUNT2), '|') END AS 'Amount2',
            CASE WHEN LTRIM(RTRIM(CONVERT(VARCHAR(20), AMOUNT6))) = '' THEN '|' ELSE ISNULL(CONVERT(VARCHAR(20), AMOUNT6), '|') END AS 'Amount6',
            CASE WHEN LTRIM(RTRIM(CONVERT(VARCHAR(20), TAXAMOUNT))) = '' THEN '|' ELSE ISNULL(CONVERT(VARCHAR(20), TAXAMOUNT), '|') END AS 'TaxAmount',
            CASE WHEN LTRIM(RTRIM(BOOK)) = '' THEN '|' ELSE ISNULL(BOOK, '|') END AS 'Book',
            CASE WHEN LTRIM(RTRIM(PAGE)) = '' THEN '|' ELSE ISNULL(PAGE, '|') END AS 'Page',
            CASE WHEN LTRIM(RTRIM(USERID3)) = '' THEN '|' ELSE ISNULL(USERID3, '|') END AS 'BookType',
            CASE WHEN LTRIM(RTRIM(USERID2)) = '' THEN '|' ELSE ISNULL(USERID2, '|') END AS 'Legacy',
            CASE WHEN LTRIM(RTRIM(PARTY1)) = '' THEN '|' ELSE ISNULL(PARTY1, '|') END AS 'Party1',
            CASE WHEN LTRIM(RTRIM(PARTY1_ROLE)) = '' THEN '|' ELSE ISNULL(PARTY1_ROLE, '|') END AS 'Party1Role',
            CASE WHEN LTRIM(RTRIM(PARTY2)) = '' THEN '|' ELSE ISNULL(PARTY2, '|') END AS 'Party2',
            CASE WHEN LTRIM(RTRIM(PARTY2_ROLE)) = '' THEN '|' ELSE ISNULL(PARTY2_ROLE, '|') END AS 'Party2Role',
            QUEUESTATUS AS 'QueueStatus',
            IMAGE_STATUS AS 'ImageStatus',
            TIMESTAMP AS 'DATE_TIME_MOD'
FROM DOCUMENT
WHERE INDEXTYPE NOT IN (12)
AND DOCTYPE not in (291,607)
AND FLAG4 = 'N'
AND VOID = 'N'
AND DELETERECORD = 'N'
--and  SEQ_KEY = '20041012001622560'
ORDER BY TIMESTAMP DESC
    ]]></sql>
