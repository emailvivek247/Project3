<#if (searchResult.docs?has_content && searchResult.docs.size() > 0) >
<#assign ResultId =0>
 <#foreach doc in searchResult.docs>
  <p class="result">
  
     <br/><b>${searchResult.datasetConfiguration.getColumn("Instrument").displayName}</b>:<a href="search.do?indexName=details&templateName=MLDetails_SNMobile&lq=${pageStyleUtil.encodeURL("Instrument:\"" + doc.get("Instrument") + "\"")}"> ${doc.get("Instrument")}</a></b>
    <#if isPhoneticSearch == "1">
                <br/><b>${searchResult.datasetConfiguration.getColumn("PhoneticName").displayName}</b>:${searchResult.highlight(doc, "PhoneticName")}      
      <#else>
          <br/><b>${searchResult.datasetConfiguration.getColumn("Name").displayName}</b>:${searchResult.highlight(doc, "Name")}
      </#if>
     <br/><b>${searchResult.datasetConfiguration.getColumn("PartyRole").displayName}</b>:${searchResult.highlight(doc, "PartyRole")}
     <br/><b>${searchResult.datasetConfiguration.getColumn("OtherName").displayName}</b>:${searchResult.highlight(doc, "OtherName")}
     <br/><b>${searchResult.datasetConfiguration.getColumn("OtherRoleName").displayName}</b>:${searchResult.highlight(doc, "OtherRoleName")}
     <br/><b>Date Married</b>:
            <#if doc.get("MarriageDate")?has_content>
              ${pageStyleUtil.format(doc.get("MarriageDate"), "yyyymmdd", "mm/dd/yyyy")}&nbsp;<b>(MM/DD/YYYY)</b>
        </#if>
     <br/><b>${searchResult.datasetConfiguration.getColumn("DocType").displayName}</b>:${searchResult.highlight(doc, "DocType")}
     <br/><b>${searchResult.datasetConfiguration.getColumn("LicenseNum").displayName}</b>:${searchResult.highlight(doc, "LicenseNum")}
     <br/><b>${searchResult.datasetConfiguration.getColumn("AppDateDisplay").displayName}</b>:&nbsp;${pageStyleUtil.format(doc.get("AppDateDisplay"), "yyyy-MM-dd HH:mm:ss", "MM/dd/yyyy HH:mm:ss a")}     
     <br /><b>View Image</b>: 
         <a class="nocolor" href="search.do?indexName=images&lq=${pageStyleUtil.encodeURL("Instrument:" + doc.get("Instrument"))}" target="_blank">Click to View</a>   
<#--          <br/><b>DocType</b>:
${searchResult.highlight(doc, "DocType")}
     <br/><b>LicenseNum</b>:
${searchResult.highlight(doc, "LicenseNum")}
     <br/><b>Application Date</b>:
${searchResult.highlight(doc, "AppDateDisplay")}-->


 <br/>&nbsp;
 </#foreach>
<#else>
<center>
 <#include "noResults.stl"> 
 </center>
</#if>

