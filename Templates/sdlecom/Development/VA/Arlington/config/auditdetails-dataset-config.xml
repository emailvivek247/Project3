  <dataquery name="WorkingQueue" batch-size="128" is-skipping-null-parameters="false">
    <sql><![CDATA[
SELECT DISTINCT

 CONVERT(VARCHAR(50),   A.SEQ_KEY)  AS 'Instrument'
,CASE WHEN LTRIM(RTRIM(A.REMARKS)) = '' THEN '|' ELSE ISNULL(A.REMARKS, '|') END AS 'AuditRemarks'
,CASE WHEN LTRIM(RTRIM(CONVERT(VARCHAR(10), A.AUDTIME, 101))) = '' THEN '|' ELSE ISNULL(CONVERT(VARCHAR(10), A.AUDTIME, 101), '|') END AS 'AuditDate'
,CASE WHEN LTRIM(RTRIM(B.ATTRIBUTENAME)) = '' THEN '|' ELSE ISNULL(B.ATTRIBUTENAME, '|') END AS 'Attribute'
,CASE WHEN LTRIM(RTRIM(B.ATTRIBUTEOLDVALUE)) = '' THEN '|' ELSE ISNULL(B.ATTRIBUTEOLDVALUE, '|') END AS 'AttributeOldValue'
,CASE WHEN LTRIM(RTRIM(B.ATTRIBUTENEWVALUE)) = '' THEN '|' ELSE ISNULL(B.ATTRIBUTENEWVALUE, '|') END AS 'AttributeNewValue'
FROM DOCUMENT_REMARKS A
LEFT JOIN AUDITDETAIL B ON A.SEQ_KEY = B.SEQKEY
    ]]></sql>
