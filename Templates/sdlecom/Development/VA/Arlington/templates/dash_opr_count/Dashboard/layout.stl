<script type="text/javascript">
      function openwindow(yearandmonthanddoctype)
      { 
              yearandmonthanddoctype = yearandmonthanddoctype.replace("=undefined", "");
              year = yearandmonthanddoctype.split("@")[0];
              month = yearandmonthanddoctype.split("@")[1];
              doctype = yearandmonthanddoctype.split("@")[2];
              if (month.length < 2){
                    month = "0" + month;
              }
              var url = escape("+RecDateFilter:\"" + year + "\"+RecDateFilter:\"" + year + "/" + month + "\"+DocType:\"" + doctype + "\"");
              url = "${request.getContextPath()}/search.do?searchType=1&indexName=opr&templateName=&searchable=Name,Instrument,RecDate,PartyRole,IndexType,DocType,Book,Page,Lot,Unit,SectionNum,Subdivision,Township,Other,DesigStatus,GPIN,ProjectNum,RdNum,Desc2,Desc3,Phase,Acreage,Building&q=" + url;
              window.open(url,null,"height=700,width=900,status=yes,toolbar=no,resizable=yes,menubar=no,location=yes,scrollbars=yes");
      }
      
</script>

