<#if searchResult.docs?has_content>
	<table id="prop" class="tablesorter">
		<thead>
			<tr>
			  <th>${searchResult.datasetConfiguration.getColumn("Property").displayName}</th>
			  <th>${searchResult.datasetConfiguration.getColumn("Values").displayName}</th>
			</tr>
		</thead>
		<tbody>
<#assign ResultId =0>
 <#foreach doc in searchResult.docs>
			<tr>
				<td>${searchResult.highlight(doc, "Property")}</td>
				<td>${searchResult.highlight(doc, "Values")}</td>
			</tr>
 </#foreach>
		</tbody>
	</table>
</#if>

<script type="text/javascript">
$(document).ready(function() {
	oTable = $('#prop').dataTable({
		"bJQueryUI": true,
		"bPaginate": false,
		"bLengthChange": false,
		"bFilter": false,
		"bSort": true,
		"bInfo": false,
		"bAutoWidth": false,
		"aaSorting": []
	});
} );
</script>
