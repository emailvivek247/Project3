  <dataquery name="WorkingQueue" batch-size="128" is-skipping-null-parameters="false">
    <sql><![CDATA[
SELECT
		B.INSTNUM AS 'RefInstrument2'
       ,CASE WHEN A.USERID3 = '' THEN '|' ELSE ISNULL (A.USERID3,'|') END AS 'DocRefBookType2'
       ,CASE WHEN A.BOOK = '' THEN '|' ELSE  ISNULL(A.BOOK,'|') END AS 'DocRefBook2'
       ,CASE WHEN A.PAGE = '' THEN '|' ELSE  ISNULL(A.PAGE,'|') END AS 'DocRefPage2'
       ,CASE WHEN A.USERID2 = '' THEN '|' ELSE  ISNULL(A.USERID2,'|') END AS 'DocRefLegacy2'
       ,A.SEQ_KEY AS 'Instrument'  
       ,B.DOCREFTYPE AS 'DocRefType'
       ,B.ACTIVE AS 'Active'
    FROM DOCUMENT A, DOCREF B 
    WHERE A.SEQ_KEY = B.SEQ_KEY AND B.ACTIVE = 'Y'
    ]]></sql>
