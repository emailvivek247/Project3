

<@display.column media="html" indexColumnName="CaseNumber" title="${searchResult.datasetConfiguration.getColumn(\"CaseNumber\").displayName}">
    <#if doc.get("CourtType") = "CIVIL">
        <a href="${request.getContextPath()}/search.do?indexName=detail&templateName=CivilDetail&lq=${pageStyleUtil.encodeURL("CaseID:\"" + doc.get("CaseID") + "\"")}">${searchResult.highlight(doc, "CaseNumber")}</a>             
    <#else>
        <a href="${request.getContextPath()}/search.do?indexName=detail&templateName=CriminalDetail&lq=${pageStyleUtil.encodeURL("CaseID:\"" + doc.get("CaseID") + "\"")}">${searchResult.highlight(doc, "CaseNumber")}</a> 
    </#if>
</@display.column>
<@display.column media="rtf excel pdf" indexColumnName="CaseNumber" title="${searchResult.datasetConfiguration.getColumn(\"CaseNumber\").displayName}">
    ${doc.get("CaseNumber")}
</@display.column>

<@display.column media="html" indexColumnName="DisplayCaseFilingDate" title="${searchResult.datasetConfiguration.getColumn(\"DisplayCaseFilingDate\").displayName}">
<#-- *Format Recorded Date -->           
