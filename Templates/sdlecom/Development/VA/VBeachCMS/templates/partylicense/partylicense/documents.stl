<b>Drivers License Information</b>
<#if searchResult.docs?has_content>
	<table id="license" class="tablesorter">
		<thead>
			<tr>
				<th>${searchResult.datasetConfiguration.getColumn("LicenseNumber").displayName}</th>
				<th>${searchResult.datasetConfiguration.getColumn("StateName").displayName}</th>				
				<th>${searchResult.datasetConfiguration.getColumn("CountryName").displayName}</th>
				<th>${searchResult.datasetConfiguration.getColumn("LicenseEndorsement").displayName}</th>
				<th>${searchResult.datasetConfiguration.getColumn("LicenseClass").displayName}</th>
				<th>${searchResult.datasetConfiguration.getColumn("LicenseStatus").displayName}</th>
				<th>${searchResult.datasetConfiguration.getColumn("IssueDate").displayName}</th>
				<th>${searchResult.datasetConfiguration.getColumn("ExpirationDate").displayName}</th>
			</tr>
		</thead>
		<tbody>
		<#assign ResultId =0>
			<#foreach doc in searchResult.docs>
					<#assign LicenseNumber = doc.getValues("LicenseNumber")>  
					<#assign StateName = doc.getValues("StateName")>  
					<#assign CountryName = doc.getValues("CountryName")>  
					<#assign LicenseEndorsement = doc.getValues("LicenseEndorsement")> 
					<#assign LicenseClass = doc.getValues("LicenseClass")>  
					<#assign LicenseStatus = doc.getValues("LicenseStatus")> 
					<#assign IssueDate = doc.getValues("IssueDate")>  
					<#assign ExpirationDate = doc.getValues("ExpirationDate")> 
					
					<#assign loopCounter = 0>
		<#foreach doc in searchResult.docs>
			<tr>
					<td>${LicenseNumber [loopCounter]?replace('|', '&nbsp;')}</td>
					<td>${StateName [loopCounter]?replace('|', '&nbsp;')}</td>
					<td>${CountryName [loopCounter]?replace('|', '&nbsp;')}</td>
					<td>${LicenseEndorsement [loopCounter]?replace('|', '&nbsp;')}</td>
					<td>${LicenseClass [loopCounter]?replace('|', '&nbsp;')}</td>
					<td>${LicenseStatus [loopCounter]?replace('|', '&nbsp;')}</td>
					<td>${IssueDate [loopCounter]?replace('|', '&nbsp;')}</td>
					<td>${ExpirationDate [loopCounter]?replace('|', '&nbsp;')}</td>
					
					 <#assign loopCounter = loopCounter + 1>
			</tr>
		</#foreach>
		</#foreach>
		</tbody>
	</table>
<#else>
<table id="license" class="tablesorter">
		<thead>
			<tr>
				<th>${searchResult.datasetConfiguration.getColumn("LicenseNumber").displayName}</th>
				<th>${searchResult.datasetConfiguration.getColumn("StateName").displayName}</th>				
				<th>${searchResult.datasetConfiguration.getColumn("CountryName").displayName}</th>
				<th>${searchResult.datasetConfiguration.getColumn("LicenseEndorsement").displayName}</th>
				<th>${searchResult.datasetConfiguration.getColumn("LicenseClass").displayName}</th>
				<th>${searchResult.datasetConfiguration.getColumn("LicenseStatus").displayName}</th>
				<th>${searchResult.datasetConfiguration.getColumn("IssueDate").displayName}</th>
				<th>${searchResult.datasetConfiguration.getColumn("ExpirationDate").displayName}</th>
			</tr>
		</thead>
		<tbody>
		<#assign ResultId =0>
		<#foreach doc in searchResult.docs>
			<tr>
				<td>No additional information provided</td>
				<td></td>
				<td></td>
				<td></td>
				<td></td>
				<td></td>
				<td></td>
				<td></td>
			</tr>
		</#foreach>
		</tbody>
	</table>  
</#if>
<script type="text/javascript">
	$(document).ready(function() {
	oTable = $('#license').dataTable({
		"bJQueryUI": true,
		"bPaginate": false,
		"bLengthChange": false,
		"bFilter": false,
		"bSort": true,
		"bInfo": false,
		"bAutoWidth": false,
		"aaSorting": []
	});
} );
</script>



