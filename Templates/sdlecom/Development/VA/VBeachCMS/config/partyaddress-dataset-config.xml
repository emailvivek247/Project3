    <sql><![CDATA[
SELECT DISTINCT 
A.PERSON_ALIAS_ID AS 'PersonSPN',
A.ADDRESS_LINE1_TXT AS 'AddressLine1',
A.ADDRESS_LINE2_TXT AS 'AddressLine2',
A.MAILING_FLAG AS 'Mailing',
A.COMMENT_TEXT AS 'AddressComment',
A.POSTAL_CD AS 'Zip',
A.STATE_NAME AS 'State',
