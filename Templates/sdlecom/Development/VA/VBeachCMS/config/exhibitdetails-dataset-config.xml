  <dataquery name="WorkingQueue" batch-size="128" is-skipping-null-parameters="false">
    <sql><![CDATA[
SELECT DISTINCT
	 COD.CASE_ID AS 'CaseID'
        ,CASE WHEN LTRIM(RTRIM(CONVERT(VARCHAR(10), COD.ID_DATE, 101))) = '' THEN '|' ELSE ISNULL(CONVERT(VARCHAR(10), COD.ID_DATE, 101), '|') END AS 'IDDate'
        ,CASE WHEN LTRIM(RTRIM(CONVERT(VARCHAR(10), COD.EVIDENCE_DATE, 101))) = '' THEN '|' ELSE ISNULL(CONVERT(VARCHAR(10), COD.EVIDENCE_DATE, 101), '|') END AS 'EvidenceDate'
        ,COD.ID
        ,CASE WHEN LTRIM(RTRIM(COT.ID_NUM)) = '' THEN '|' ELSE ISNULL(COT.ID_NUM, '|') END AS 'IDNumber'
	,CASE WHEN LTRIM(RTRIM(COD.OBJ_DESC)) = '' THEN '|' ELSE ISNULL(COD.OBJ_DESC, '|') END AS 'ObjectDesc'
	,CASE_NUMBER AS 'CaseNumber'
	,COD.DATE_TIME_MOD
FROM ACM_CASE_OBJ_DESC COD 
INNER JOIN ACM_CASE_OBJ_TRKNG COT ON COD.ID=COT.CASE_OBJ_DESC_ID
INNER JOIN ACM_V_ESCASEDETAIL E ON COD.CASE_ID = E.CASE_ID
    ]]></sql>
