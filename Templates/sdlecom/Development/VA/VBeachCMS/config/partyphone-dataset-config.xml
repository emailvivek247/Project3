  <dataquery name="WorkingQueue" batch-size="128" is-skipping-null-parameters="false">
    <sql><![CDATA[
SELECT DISTINCT 
A.PERSON_ALIAS_ID AS 'PersonSPN',
A.PHONETYPE AS 'PhoneType',
A.PREFIX AS 'Prefix',
A.PHONENUMBER AS 'PhoneNumber',
A.EXTENSION AS 'Extension',
