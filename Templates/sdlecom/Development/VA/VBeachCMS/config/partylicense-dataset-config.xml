  <dataquery name="WorkingQueue" batch-size="128" is-skipping-null-parameters="false">
    <sql><![CDATA[
SELECT DISTINCT 
A.PERSON_ALIAS_ID AS 'PersonSPN',
A.LICENSE_NUM AS 'LicenseNumber',
A.STATE_NAME AS 'StateName',
A.COUNTRY_NAME AS 'CountryName',
A.LICENSE_ENDORSEMENT AS 'LicenseEndorsement',
A.LICENSE_CLS_DESC AS 'LicenseClass',
A.LICENSE_STS_DESC AS 'LicenseStatus',
CONVERT(varchar(10),A. EFFECTIVE_DATE, 101) AS 'IssueDate',
CONVERT(varchar(10), A.END_DATE, 101) AS 'ExpirationDate',
P.PARTY_ID AS 'PartyID'
FROM ACM_V_ESDLDETAIL A
INNER JOIN ACM_PARTY P
ON A.PERSON_ALIAS_ID = P.PERSON_ALIAS_ID
    ]]></sql>
