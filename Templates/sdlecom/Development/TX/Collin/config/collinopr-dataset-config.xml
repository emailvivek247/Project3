  <dataquery name="WorkingQueue" batch-size="128" is-skipping-null-parameters="false">
    <sql><![CDATA[
Select top 1000000
cast(a.id as varchar)+ '-' + cast(isnull(b.seq, 0) as varchar) as pk_key
,a.SEQ_KEY AS 'Instrument'
,CONVERT(VARCHAR(10), RECDATE1, 112) AS RecDate
,RECDATE1 AS RecDateFilter
,CONVERT(VARCHAR(30), RECDATE1, 120) AS RecDateDisplay
,LNAME+' '+ISNULL(FNAME,'')+' '+ ISNULL(MNAME,'') as Name
,LNAME+' '+ISNULL(FNAME,'')+' '+ ISNULL(MNAME,'') as NameSort
,LNAME+' '+ISNULL(FNAME,'')+' '+ ISNULL(MNAME,'') as PhoneticName
,DESIGSTATUS AS DesigStatus
,LTRIM(RTRIM(UPPER(SUBSTRING(ROLENAME,1,1)))) + LTRIM(RTRIM(LOWER(SUBSTRING(ROLENAME,2,50)))) AS PartyRole
,case when role=1 then 'Direct Party' when role=2 then 'Reverse Party' when role not in (1,2) then 'No Party' end as Role
,case when a.role is null then '0' when a.role = '' then '0' else LTRIM(RTRIM(a.role)) end as RoleFilter
,OTHERNAME AS 'OtherName'
,OTHERDESIGSTATUS AS 'OtherDesigStatus'
,OTHERROLE AS 'OtherRole'
,OTHERROLENAME as 'OtherRoleName'
,LTRIM(RTRIM((SELECT descr FROM Doctype2lov WHERE type2 = a.indextype))) as 'IndexType'
,LTRIM(RTRIM(a.code)) as DocType
,c.DESCR AS 'DocTypeDesc'
,(isnull (a.bookshort,'')+ ' ' + isnull(a.BOOK,'') + ' '+ isnull(a.PAGE,'')) AS 'BookPage'
,Book
,Page
,LOTNUM as 'Lot'
,UNIT AS 'Unit'
,SECTIONNUM as 'SectionNum'
,PHASE AS 'Phase'
,BUILDING AS 'Building'
,ACREAGE AS 'Acreage'
,BLOCKNUM as 'Block'
,SUBDIVISION as 'Subdivision'
,TOWNSHIP AS 'Township'
,MAPBOOK as MapBook
,MAPPAGE as MapPage
,CITY as CityTown
,LDESC as 'Other'
,LEGAL1 as 'ProjectNum'
,LEGAL2 as 'RdNum'
,LDESC2 as 'Desc2'
,LDESC3 as 'Desc3'
,GPIN
,'' AS 'Status'
,LegalDescription
,MULTIPLE_LEGAL
,FirstReference
,MULTIPLE_REFERENCE
,a.date_time_mod
,case when b.seq is null then '0' else b.seq end as 'seq'
,offlinerecord
,'Collin' as Location
,a.indextype as IndexCode
,a.seq as PartySeq
,a.doctype as DocCode
,a.VERIFIED as Verified
    from PUBLICSEARCH a left outer join LEGAL b on
    a.SEQ_KEY = b.SEQ_KEY
   inner join DOCTYPE1LOV c
    on a.CODE = c.CODE 
    where OFFLINERECORD ='N' 
    AND a.indextype not in (6,12,23,24)
    AND a.doctype not in (667,668,689,716,717,722,737,751,752,780,787)
and a.date_time_mod > '01-01-2010'
order by a.date_time_mod desc
    ]]></sql>
