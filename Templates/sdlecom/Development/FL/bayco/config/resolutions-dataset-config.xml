  <dataquery name="WorkingQueue" batch-size="128" is-skipping-null-parameters="false">
    <sql><![CDATA[
select
r.DOC_TYP_NAME
,r.DOC_DATE
,r.RESOLUTION_NUM
,r.DATE
,r.[DESC]
,r.DOC_HANDLE
,'5' + r.DOC_HANDLE AS 'PKHandle'
,r.FILE_TYP_NUM
,r.FILE_NAME
,de.DOCUMENT_EXTRACT_ID
,de.PAGE_NUMBER
,de.TEXT
from resolutions r
left join DOCUMENT_EXTRACT de on ('5'+r.DOC_HANDLE) = de.DOCUMENT_INFORMATION_ID
    ]]></sql>
