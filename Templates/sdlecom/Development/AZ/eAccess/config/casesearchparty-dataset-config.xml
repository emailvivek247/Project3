  <dataquery name="WorkingQueue" batch-size="128" is-skipping-null-parameters="false">
    <sql><![CDATA[
select
	 a.UniqueKeyID AS 'PKKEY'
	,a.CaseID
	,a.CountyName
	,a.CountyName AS 'CountyNameFilter'
	,a.CaseTitle
	,a.SearchCaseNumber AS 'CaseNumber'
	,a.SearchCaseNumber as 'CaseNumberSort'
	, CASE WHEN FirstName IS null or RTRIM(LTRIM(FirstName)) = '' then A.LastName else 
	LTRIM(RTRIM(LTRIM(RTRIM(a.LastName))+', '+ISNULL(LTRIM(RTRIM(a.FirstName)),'')+' '+ ISNULL(LTRIM(RTRIM(a.MiddleName)),'')))
	END AS 'Name'
	,a.FirstName AS 'FirstName'
	,a.LastName AS 'LastName'
	,a.LastName AS 'LastNameSort'
	,a.DateOfBirth AS 'BirthDate'
	,a.DateOfBirth AS 'BirthDateFilter'
    ,a.DOBSearch
	,a.ParticipantRoleDescription AS 'RoleDesc'
	,a.ParticipantRoleDescription AS 'RoleDescFilter'
	,a.IsTTEAP AS 'TTEAPFlag'
	,a.IsTTEAP AS 'TTEAPFlagFilter'
	,a.court as 'Court'
	,a.court as 'CourtFilter'
	,a.CCILastLookupDateTime AS 'DateTimeMod'
	,b.JudicialOfficer 
	,b.CaseGeneralCategoryDescription 
	,b.CaseCategoryDescription 
FROM caseparty a
inner join cases b
on a.CaseID = b.CaseID
    ]]></sql>
