<#if searchResult.docs?has_content>
<table id="party" class="tablesorter">
	 <thead>
	     <tr>
	 	<th>${searchResult.datasetConfiguration.getColumn("PersonGivenName").displayName}</th>
		<th>${searchResult.datasetConfiguration.getColumn("PersonMiddleName").displayName}</th>
		<th>${searchResult.datasetConfiguration.getColumn("PersonSurName").displayName}</th>
		<th>${searchResult.datasetConfiguration.getColumn("PersonSuffixName").displayName}</th>
		<th>${searchResult.datasetConfiguration.getColumn("BirthDate").displayName}</th>
		<th>${searchResult.datasetConfiguration.getColumn("CaseParticipantRoleDescription").displayName}</th>
		<th>${searchResult.datasetConfiguration.getColumn("City").displayName}</th>
		<th>${searchResult.datasetConfiguration.getColumn("StateCode").displayName}</th>
		<th>${searchResult.datasetConfiguration.getColumn("TTEAPFlag").displayName}</th>
	    </tr>
	 </thead>
	 <tbody>
	<#assign ResultId =0>
	 
	 
	     <#foreach doc in searchResult.docs>
		<#assign PersonGivenName = doc.getValues("PersonGivenName")>
		<#assign PersonMiddleName = doc.getValues("PersonMiddleName")>
		<#assign PersonSurName = doc.getValues("PersonSurName")>
		<#assign PersonSuffixName = doc.getValues("PersonSuffixName")>
		<#assign BirthDate = doc.getValues("BirthDate")>
		<#assign CaseParticipantRoleDescription = doc.getValues("CaseParticipantRoleDescription")>
		<#assign City = doc.getValues("City")>
		<#assign StateCode = doc.getValues("StateCode")>
		<#assign TTEAPFlag = doc.getValues("TTEAPFlag")>
	   <#assign loopCounter = 0>
	   
	   	<tr>
			<td>${doc.get("PersonGivenName")}</td>
			<td>${doc.get("PersonMiddleName")}</td>
			<td>${doc.get("PersonSurName")}</td>
			<td>${doc.get("PersonSuffixName")}</td>
			<td>${doc.get("BirthDate")}</td>
			<td>${doc.get("CaseParticipantRoleDescription")}</td>
			<td>${doc.get("City")}</td>
			<td>${doc.get("StateCode")}</td>
			<td>${doc.get("TTEAPFlag")}</td>

			<#assign loopCounter = loopCounter + 1>
		</tr>
	 </#foreach>
	
	 </tbody>
	</table>
