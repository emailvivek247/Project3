

<#assign display=JspTaglibs["http://displaytag.sf.net"]>


<#if searchResult.docs?has_content>
    <#assign ResultId =0>
	<@display.table name="searchResult.docs" id="doc" export=true requestURI="/search.do" excludedParams="searchType ActiveAttorneyList ActiveAttorneyList_select BirthDate BirthDate_select CCILastLookupDateTime CCILastLookupDateTime_select CaseID CaseID_select CaseNumberDisplay CaseNumberDisplay_select CaseNumberSearch CaseNumberSearch_select CaseParticipantRoleCode CaseParticipantRoleCode_select CaseParticipantRoleDescription CaseParticipantRoleDescription_select CourtCode CourtCode_select OrganizationName OrganizationName_select PersonGivenName PersonGivenName_select PersonSurName PersonSurName_select RoamDefaultTemplate RoamDefaultTemplate_select TTEAPFlag TTEAPFlag_select ">
	  <#include "document.stl">
	  <@display.setProperty name="export.title" value="Search Criteria :${Request.searchForm.searchQuery}" />  
	</@display.table>
<#else>
 <#include "noResults.stl"> 
</#if>



<script type="text/javascript">
$(document).ready(function() {
	oTable = $('#doc').dataTable({
		"bJQueryUI": true,
		"bPaginate": false,
		"bLengthChange": false,
		"bFilter": false,
		"bSort": true,
		"bInfo": false,
		"bAutoWidth": false,
		"aaSorting": []
	});
} );
</script>
