<#if searchResult.docs?has_content>
<table id="charge" class="tablesorter">
	<thead>
		<tr>
			<th>${searchResult.datasetConfiguration.getColumn("OffenseARSCode").displayName}</th>
			<th>${searchResult.datasetConfiguration.getColumn("OffenseARSDescription").displayName}</th>
			<th>${searchResult.datasetConfiguration.getColumn("OffenseClassShortDescription").displayName}</th>
			<th>${searchResult.datasetConfiguration.getColumn("DispositionDescription").displayName}</th>
			<th>${searchResult.datasetConfiguration.getColumn("DispositionDate").displayName}</th>
		</tr>
	</thead>
 	<tbody>
	<#assign ResultId =0>
	 <#foreach doc in searchResult.docs>
		  <tr>
			<td>${searchResult.highlight(doc, "OffenseARSCode")}</td>
			<td>${searchResult.highlight(doc, "OffenseARSDescription")}</td>
			<td>${searchResult.highlight(doc, "OffenseClassShortDescription")}</td>
			<td>${searchResult.highlight(doc, "DispositionDescription")}</td>
			<td>${searchResult.highlight(doc, "DispositionDate")}</td>
		</tr>
 	</#foreach>
	</tbody>
</table>
