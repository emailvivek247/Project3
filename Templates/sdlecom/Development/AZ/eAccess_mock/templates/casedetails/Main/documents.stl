<script type="text/javascript">
$(document).ready(function() {

	var url1 = "search.do?indexName=partydetails&templateName=Main&lq=CaseID%3A%22<#list caseNumberKey as CaseIDs>${CaseIDs}</#list>%22";
	$('#partycontainer').load(url1);
	var url2 = "search.do?indexName=chargedetails&templateName=Main&lq=CaseID%3A%22<#list caseNumberKey as CaseIDs>${CaseIDs}</#list>%22";
