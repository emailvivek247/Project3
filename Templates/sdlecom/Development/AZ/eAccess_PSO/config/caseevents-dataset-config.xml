  <dataquery name="WorkingQueue" batch-size="128" is-skipping-null-parameters="false">
    <sql><![CDATA[
SELECT
	 es.UniqueKeyID as 'PKKEY'
        ,es.CaseID
        ,es.CaseTitle
        ,es.Court
        ,es.CountyName
        ,es.CountyName AS 'CountyNameFilter'
	,es.DisplayCaseNumber AS 'CaseNumber'
        ,es.EventDate
        ,es.EventDate AS 'EventDateFilter'
	,D.DocumentTitle AS 'EventDesc'
	,es.AttorneyBarList AS 'ActAttyList'
	,es.CCILastLookupDateTime AS 'DateTimeMod'
	,ai.DocumentID
FROM eventsearch es
INNER JOIN AocImages ai ON es.CaseID = ai.CaseID
INNER JOIN document D on D.CaseID = ai.CaseID
    ]]></sql>
