  <dataquery name="Incremental">
    <sql><![CDATA[
Select DISTINCT
p.id
,SEQ_KEY "Instrument"
,BACKFILENUM as LicenseNum
,CONVERT(VARCHAR(30), RECDATE1, 120) AS RECDATE1DISPLAY
,CONVERT(VARCHAR(10), RECDATE1, 112) AS RECDATE1SEARCH
,CONVERT(VARCHAR(10), RECDATE2, 112) AS RECDATE2
,RECDATE2 AS RECDATE2FILTER
,LNAME+' '+ISNULL(FNAME,'')+' '+ ISNULL(MNAME,'') as Name
,LNAME+' '+ISNULL(FNAME,'')+' '+ ISNULL(MNAME,'') as NameSort
,LNAME+' '+ISNULL(FNAME,'')+' '+ ISNULL(MNAME,'') as PhoneticName
,ROLENAME PartyRole
,OTHERNAME "OtherName"
,OTHERROLE "OtherRole"
,OTHERROLENAME
,code as DocType
,r.date_time_mod
,seq
,offlinerecord
,offlineimage
,'SMITH' as Location
      from PUBLICSEARCH P with (nolock) INNER JOIN ROAM_TRACK R with (nolock)
      ON P.SEQ_KEY = R.PK_KEY
      where P.OFFLINERECORD ='N' 
      AND P.indextype in (6)
      AND R.TAB_NAME = 'MARRIAGE' AND R.DML_TYP IN ('I', 'U')
      AND R.DATE_TIME_MOD > ? 
order by r.date_time_mod desc
    ]]></sql>
  </dataquery>
