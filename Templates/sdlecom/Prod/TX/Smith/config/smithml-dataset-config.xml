    <sql><![CDATA[
Select 
id
,SEQ_KEY "Instrument"
,BACKFILENUM as LicenseNum
,CONVERT(VARCHAR(30), RECDATE1, 120) AS RECDATE1DISPLAY
,CONVERT(VARCHAR(10), RECDATE1, 112) AS RECDATE1SEARCH
,CONVERT(VARCHAR(10), RECDATE2, 112) AS RECDATE2
,RECDATE2 AS RECDATE2FILTER
,LNAME+' '+ISNULL(FNAME,'')+' '+ ISNULL(MNAME,'') as Name
,LNAME+' '+ISNULL(FNAME,'')+' '+ ISNULL(MNAME,'') as NameSort
,LNAME+' '+ISNULL(FNAME,'')+' '+ ISNULL(MNAME,'') as PhoneticName
,ROLENAME PartyRole
,OTHERNAME "OtherName"
,OTHERROLE "OtherRole"
,OTHERROLENAME
,code as DocType
,date_time_mod
,seq
,offlinerecord
,offlineimage
,'Smith' as Location
    from PUBLICSEARCH  
    where OFFLINERECORD <> 'Y' 
    AND indextype in (6)
AND doctype not in (4,14,37,54,71,79, 80, 81,84,85,163,164,165,166,169,183)
order by date_time_mod desc
    ]]></sql>
