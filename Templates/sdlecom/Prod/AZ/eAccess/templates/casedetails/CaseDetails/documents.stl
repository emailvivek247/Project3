<b>General Case Info</b>
<#if searchResult.docs?has_content>
	<table id="case" class="tablesorter">
	 <thead>
		<tr>
			<th>${searchResult.datasetConfiguration.getColumn("DisplayCaseNumber").displayName}</th>
			<th>${searchResult.datasetConfiguration.getColumn("FilingDate").displayName}</th>
			<th>${searchResult.datasetConfiguration.getColumn("CaseTitle").displayName}</th>
			<th>${searchResult.datasetConfiguration.getColumn("CaseStatus").displayName}</th>
			<th>${searchResult.datasetConfiguration.getColumn("CountyName").displayName}</th>
			<th>${searchResult.datasetConfiguration.getColumn("JudicialOfficer").displayName}</th>
			<th>${searchResult.datasetConfiguration.getColumn("CaseGeneralCategoryDescription").displayName}</th>
			<th>${searchResult.datasetConfiguration.getColumn("CaseCategoryDescription").displayName}</th>
		</tr>
	 </thead>
	 <tbody>
   <#assign ResultId =0>
   <#foreach doc in searchResult.docs>
   
      <#assign CaseID = doc.getValues("CaseID")
      	       DisplayCaseNumber = doc.getValues("DisplayCaseNumber")
      	       CaseStatus = doc.getValues("CaseStatus")
      	       CaseTitle = doc.getValues("CaseTitle")
      	       CountyName = doc.getValues("CountyName")
      	       FilingDate = doc.getValues("FilingDate")
      	       JudicialOfficer = doc.getValues("JudicialOfficer")
      	       FilingDate = doc.getValues("CaseGeneralCategoryDescription")
      	       JudicialOfficer = doc.getValues("CaseCategoryDescription")>
      	       
      	      

		<tr>
			<td>${doc.get("DisplayCaseNumber")}</td>
			<td>${date.format("MM/dd/yyyy HH:mm:ss z",doc.getDate("FilingDate"))}</td>
			<td>${doc.get("CaseTitle")}</td>
			<td>${doc.get("CaseStatus")}</td>
			<td>${doc.get("CountyName")}</td>
			<td>${doc.get("JudicialOfficer")}</td>
			<td>${doc.get("CaseGeneralCategoryDescription")}</td>
			<td>${doc.get("CaseCategoryDescription")}</td>
		</tr>
	<#assign caseNumberKey = doc.getValues("CaseID")>
    </#foreach>
   </tbody>
</table>
	



<#else>
 <table id="case" class="tablesorter">
 	 <thead>
 		<tr>
 			<th>${searchResult.datasetConfiguration.getColumn("DisplayCaseNumber").displayName}</th>
