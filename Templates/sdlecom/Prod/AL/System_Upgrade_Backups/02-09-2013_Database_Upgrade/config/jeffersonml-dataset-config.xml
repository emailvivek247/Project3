  <dataquery name="Incremental">
    <sql><![CDATA[
SELECT  DISTINCT
          P.id
	 , SEQ_KEY AS "Instrument"
	, TO_CHAR(RECDATE1 , 'YYYY-MM-DD HH:MM:SS') AS "RecDate1Display"
	, TO_CHAR(RECDATE1 , 'YYYYMMDD')  AS "RecDate1Search"
	, TO_CHAR(RECDATE2 , 'YYYYMMDD')  AS "RecDate2"
	, RECDATE2 AS "RecDate2Filter"
	, LNAME||' '||NVL(FNAME,'') as "Name"
	  , LNAME||' '||nvl(FNAME,'') AS "NameSort"
	, LNAME||' '||NVL(FNAME,'') AS "PhoneticName"
	, ROLENAME AS "PartyRole"
	, OTHERNAME AS "OtherName" 
	, OTHERROLENAME AS "OtherRoleName" 
        ,code as "DocType"
	, R.DATE_TIME_MOD AS DATE_TIME_MOD
FROM PUBLICSEARCH P INNER JOIN ROAM_TRACK R 
      ON P.SEQ_KEY = R.PK_KEY
WHERE P.OFFLINERECORD ='N' 
      AND P.indextype in ('6')
      AND R.TAB_NAME = 'MARRIAGE' AND R.DML_TYP IN ('I', 'U')
      AND R.DATE_TIME_MOD > ?   
ORDER BY TO_CHAR(R.DATE_TIME_MOD,'YYYY-MM-DD HH:MI:SS')||'.000'  DESC
    ]]></sql>
  </dataquery>
