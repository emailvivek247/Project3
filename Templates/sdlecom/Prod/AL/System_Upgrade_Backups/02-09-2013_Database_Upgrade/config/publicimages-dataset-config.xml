  <dataquery name="Incremental">
    <sql><![CDATA[
SELECT
    A.seq_key  AS  "Instrument"
  ,R.DATE_TIME_MOD AS "DATE_TIME_MOD"
  ,A.pagecount
  ,D.DESCR AS "DocTypeDesc"
FROM DOCUMENT A 
      INNER JOIN DOCTYPE1LOV D on A.doc_type = D.CODE
      INNER JOIN ROAM_TRACK R ON A.seq_key = r.pk_key
WHERE     
    A.indextype not in ('12','6') 
    AND A.doctype not in ('325') 
    AND r.DATE_TIME_MOD >  ?
    AND R.TAB_NAME = 'DOCUMENT' AND R.DML_TYP IN ('I', 'U')
ORDER BY TO_CHAR(R.DATE_TIME_MOD,'YYYY-MM-DD HH:MI:SS')||'.000' DESC
    ]]></sql>
  </dataquery>
