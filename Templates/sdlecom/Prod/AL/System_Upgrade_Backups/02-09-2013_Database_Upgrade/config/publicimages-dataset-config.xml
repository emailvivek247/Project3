  <dataquery name="WorkingQueue" batch-size="128" is-skipping-null-parameters="false">
    <sql><![CDATA[
SELECT 
	a.seq_key  AS  "Instrument"
  ,sysdate AS "DATE_TIME_MOD"
  ,a.pagecount
  ,d.DESCR AS "DocTypeDesc"
FROM DOCUMENT A 
      INNER JOIN DOCTYPE1LOV D on a.doc_type = D.CODE 
WHERE indextype not in ('12','6') 
	AND doctype not in ('325')  
ORDER BY DATE_TIME_MOD DESC
    ]]></sql>
