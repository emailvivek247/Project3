    <sql><![CDATA[
SELECT  SEQ_KEY AS "Instrument" 
	,CASE WHEN TRIM(DOC_TYPE) = '' THEN '|' ELSE NVL(DOC_TYPE, '|') END AS "DocType"
	,CASE WHEN TRIM(TO_CHAR(RECDATE1,'MM/DD/YYYY')) = '' THEN '|' ELSE NVL(TO_CHAR(RECDATE1,'MM/DD/YYYY'), '|')  END AS "RecDate1"
	,CASE WHEN TRIM(TO_CHAR(RECDATE2,'MM/DD/YYYY')) = '' THEN '|' ELSE NVL(TO_CHAR(RECDATE2,'MM/DD/YYYY'), '|')  END AS "RecDate2"
	,CASE WHEN TRIM(CAST(PAGECOUNT AS VARCHAR2(20))) = '' THEN '|' ELSE NVL(CAST(PAGECOUNT AS VARCHAR2(20)), '|') END AS "PageCount"
	,CASE WHEN TRIM(TEXT1) = '' THEN '|' ELSE NVL(TEXT1, '|') END AS "Remarks1"
	,CASE WHEN TRIM(TEXT2) = '' THEN '|' ELSE NVL(TEXT2, '|') END AS "Remarks2"
	,CASE WHEN TRIM(CAST(AMOUNT1 AS VARCHAR2(20))) = '' THEN '|' ELSE NVL(CAST(AMOUNT1 AS VARCHAR2(20)), '|') END AS "Amount1"
	,CASE WHEN TRIM(CAST(AMOUNT6 AS VARCHAR2(20))) = '' THEN '|' ELSE NVL(CAST(AMOUNT6 AS VARCHAR2(20)), '|') END AS "Amount6"
	,CASE WHEN TRIM(CAST(TAXAMOUNT AS VARCHAR2(20))) = '' THEN '|' ELSE NVL(CAST(TAXAMOUNT AS VARCHAR2(20)), '|') END AS "TaxAmount"
	,CASE WHEN TRIM(BOOK) = '' THEN '|' ELSE NVL(BOOK, '|') END AS "Book"
	,CASE WHEN TRIM(PAGE) = '' THEN '|' ELSE NVL(PAGE, '|') END AS "Page"
	,CASE WHEN TRIM(USERID3) = '' THEN '|' ELSE NVL(USERID3, '|') END AS "BookType"
	,CASE WHEN TRIM(USERID2) = '' THEN '|' ELSE NVL(USERID2, '|') END AS "Legacy"
	,CASE WHEN TRIM(PARTY1) = '' THEN '|' ELSE NVL(PARTY1, '|') END AS "Party1"
	,CASE WHEN TRIM(PARTY1_ROLE) = '' THEN '|' ELSE NVL(PARTY1_ROLE, '|') END AS "Party1Role"
	,CASE WHEN TRIM(PARTY2) = '' THEN '|' ELSE NVL(PARTY2, '|') END AS "Party2"
	,CASE WHEN TRIM(PARTY2_ROLE) = '' THEN '|' ELSE NVL(PARTY2_ROLE, '|') END AS "Party2Role"
	,QUEUESTATUS AS "QueueStatus"
	,IMAGE_STATUS AS "ImageStatus"
	,TIMESTAMP AS "DATE_TIME_MOD"
FROM DOCUMENT
WHERE INDEXTYPE NOT IN ('12')
        AND DOCTYPE NOT IN ('325')
        AND FLAG4 = 'N'
	AND VOID = 'N'
	AND DELETERECORD = 'N'
ORDER BY TIMESTAMP DESC
    ]]></sql>
