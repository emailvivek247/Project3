    <sql><![CDATA[
SELECT 
	SEQ_KEY AS "Instrument"
	,CASE WHEN TRIM(LEGAL_TYPE) = '' THEN '|' ELSE NVL(LEGAL_TYPE, '|') END AS "LegalType"
	,CASE WHEN TRIM(SEQ) = '' THEN '|' ELSE NVL(to_char(SEQ), '|') END AS "LegalSeq"
	,CASE WHEN TRIM(LEGAL1) = '' THEN '|' ELSE NVL(LEGAL1, '|') END AS "Con"
	,CASE WHEN TRIM(LEGAL2) = '' THEN '|' ELSE NVL(LEGAL2, '|') END AS "TT"
	,CASE WHEN TRIM(ACREAGE) = '' THEN '|' ELSE NVL(ACREAGE, '|') END AS "Area"
	,CASE WHEN TRIM(QUARTER) = '' THEN '|' ELSE NVL(QUARTER, '|') END AS "Quarter"
	,CASE WHEN TRIM(BLOCKNUM) = '' THEN '|' ELSE NVL(BLOCKNUM, '|') END AS "Block"     
	,CASE WHEN TRIM(PARCEL) = '' THEN '|' ELSE NVL(PARCEL, '|') END AS "Parcel"
	,CASE WHEN TRIM(LEGALDESCRIPTION) = '' THEN '|' ELSE NVL(LEGALDESCRIPTION, '|') END AS "PID"
	,CASE WHEN TRIM(LEGALDESCRIPTION) = '' THEN '|' ELSE NVL(LEGALDESCRIPTION, '|') END AS "LegalDescription"            
	,CASE WHEN TRIM(PHASE) = '' THEN '|' ELSE NVL(PHASE, '|') END AS "Phase"
	,CASE WHEN TRIM(SECTIONNUM) = '' THEN '|' ELSE NVL(SECTIONNUM, '|') END AS "Section"
	,CASE WHEN TRIM(LOTNUM) = '' THEN '|' ELSE NVL(LOTNUM, '|') END AS "LotNum"
	,CASE WHEN TRIM(LOTLETTER) = '' THEN '|' ELSE NVL(LOTLETTER, '|') END AS "Lot"
	,CASE WHEN TRIM(MAPBOOK) = '' THEN '|' ELSE NVL(MAPBOOK, '|') END AS "MapBook"
	,CASE WHEN TRIM(MAPPAGE) = '' THEN '|' ELSE NVL(MAPPAGE, '|') END AS "MapPage"
	,CASE WHEN TRIM(SUBDIVISION) = '' THEN '|' ELSE NVL(SUBDIVISION, '|') END AS "Subdivision"
	,CASE WHEN TRIM(LDESC) = '' THEN '|' ELSE NVL(LDESC, '|') END AS "Freeform1Other"
	,CASE WHEN TRIM(LDESC2) = '' THEN '|' ELSE NVL(LDESC2, '|') END AS "Freeform2"
	,CASE WHEN TRIM(LDESC3) = '' THEN '|' ELSE NVL(LDESC3, '|') END AS "Freeform3"
	,CASE WHEN TRIM((st_num || ' ' || st_dir || ' ' ||  st_name || ' ' || st_suf)) = '' THEN '|' ELSE NVL((st_num || ' ' || st_dir || ' ' ||  st_name || ' ' || st_suf), '|') END AS "Address"
	,CASE WHEN TRIM(UNIT) = '' THEN '|' ELSE NVL(UNIT, '|') END AS "Unit"
	,CASE WHEN TRIM(CITY) = '' THEN '|' ELSE NVL(CITY, '|') END AS "CityTown"
	,CASE WHEN TRIM(STATE) = '' THEN '|' ELSE NVL(STATE, '|') END AS "State"
	,CASE WHEN TRIM(ZIP) = '' THEN '|' ELSE NVL(ZIP, '|') END AS "Zip"
FROM LEGAL
WHERE SEQ_KEY IN (?)
    ]]></sql>
