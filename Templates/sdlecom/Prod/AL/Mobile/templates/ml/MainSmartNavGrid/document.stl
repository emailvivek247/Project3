<#-- *Modify columns -->
<#assign urlEncoder = new("net.javacoding.xsearch.utility.EscapeChars")>
 <tr>
    <td>
      <a target="_parent" onClick='rowSelected(this)' href="${request.getContextPath()}/search.do?indexName=details&templateName=defaultml&lq=${urlEncoder.forURL("Instrument:\"" + doc.get("Instrument") + "\"")}">${number.format("###0.##",doc.get("Instrument"))}</a>
   </td>    
   <td>
    <#if isPhoneticSearch == "1">
      ${searchResult.highlight(doc, "DisplayNamePhonetic")}      
     <#else>
      ${searchResult.highlight(doc, "DisplayName")}
     </#if>  
   </td>
   <td>${searchResult.highlight(doc, "PartyRole")}</td>
   <td>${searchResult.highlight(doc, "DocType")}</td>
</tr>
