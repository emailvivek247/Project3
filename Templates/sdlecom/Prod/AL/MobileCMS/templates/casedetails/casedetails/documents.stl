		"bLengthChange": false,
		"bFilter": false,
		"bSort": true,
		"bInfo": false,
		"bAutoWidth": false,
		"aaSorting": []
	});

 $("#printRecepit").click(function() {   
alert($("#printArea").html());          
             $('#printArea').focus();
             printElem({pageTitle:'Case Details' }); 
         }); 
	
} );

 function printElem(options){
	     $("#printArea").printElement(options);             
	 }



</script>