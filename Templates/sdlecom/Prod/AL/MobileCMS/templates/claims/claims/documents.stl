<b>Claims</b>
<#if searchResult.docs?has_content>
<table id="claims" class="tablesorter">
 <thead><tr>
  <th>${searchResult.datasetConfiguration.getColumn("ClaimNumber").displayName}</th>
  <th>${searchResult.datasetConfiguration.getColumn("FiledBy").displayName}</th>
  <th>${searchResult.datasetConfiguration.getColumn("ClaimAmount").displayName}</th>
  <th>${searchResult.datasetConfiguration.getColumn("ClaimStatus").displayName}</th>
  
  
 </tr></thead>
 <tbody>
<#assign ResultId =0>
 <#foreach doc in searchResult.docs>
   <tr>
            <td>${searchResult.highlight(doc, "ClaimNumber")}</td>
            <td>${searchResult.highlight(doc, "FiledBy")}</td>
            <td>${pageStyleUtil.formatCurrency(doc.get("ClaimAmount"))}</td>
            <td>${searchResult.highlight(doc, "ClaimStatus")}</td>
 </tr>

 </#foreach>
 </tbody>
</table>
<#else>
 <table id="claims" class="tablesorter">
 <thead><tr>
  <th>${searchResult.datasetConfiguration.getColumn("ClaimNumber").displayName}</th>
  <th>${searchResult.datasetConfiguration.getColumn("FiledBy").displayName}</th>
  <th>${searchResult.datasetConfiguration.getColumn("ClaimAmount").displayName}</th>
  <th>${searchResult.datasetConfiguration.getColumn("ClaimStatus").displayName}</th>
  
  
 </tr></thead>
 <tbody>
<#assign ResultId =0>
 <#foreach doc in searchResult.docs>
   <tr>

            <td></td>
            <td></td>
	    <td></td>
            <td></td>
 </tr>

 </#foreach>
 </tbody>
</table>
</#if>



<script type="text/javascript">
$(document).ready(function() {
	oTable = $('#claims').dataTable({
		"bJQueryUI": true,
		"bPaginate": false,
		"bLengthChange": false,
		"bFilter": false,
		"bSort": true,
		"bInfo": false,
		"bAutoWidth": false,
		"aaSorting": []
	});
} );
</script>