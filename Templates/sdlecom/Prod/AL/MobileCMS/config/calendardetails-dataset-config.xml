  <dataquery name="WorkingQueue" batch-size="128" is-skipping-null-parameters="false">
    <sql><![CDATA[
SELECT distinct
	CONVERT(VARCHAR(10), SESSION_START_DT, 112) AS 'SessionDateSearch' 
        ,SESSION_START_DT AS 'SessionDateDisplay'
	,CASE_ID AS 'CaseID'
	,CASE_NUMBER AS 'CaseNumber'
	,COURT_ROOM_DESC AS 'CourtRoom'
	,CONVERT(VARCHAR(5), SESSION_START_DT, 108)
	+ '    '+  RIGHT (CONVERT(VARCHAR , START_TIME, 102),2)AS 'StartTime'
	,JUDGE AS 'Judge'
	,CASE_SHORT_TITLE AS 'CaseStyle'
	,COURT_ROOM_ID AS 'RoomNumber'
	,APPR_DESCRIPTION AS 'AppearanceReason'
	,COURT_ROOM_ID AS 'CourtRoomID'
	,SIAR.SESSION_ITEM_ID
	,SIAR.HEARING_RESULTS_ID
FROM MCPC_ACC_V_ATTENDANCE_SEARCH VAS
LEFT OUTER JOIN ACC_SESN_ITM_APPR_RESN SIAR ON SIAR.SESSION_ITEM_ID = VAS.SESSION_ITEM_ID 
WHERE VAS.SEALED_CASE_FLAG <> 'Y'
AND SEALED_FLAG <> 'Y'
AND IS_RESTRICTED_CASE_FLG <> 'Y'
AND IS_RESTRICTED_FLG <> 'Y'
AND EXPUNGE_FLG <> 'Y'
AND SEALED_PARTY_FLG <> 'Y'
AND SIAR.HEARING_RESULTS_ID IS NULL
    ]]></sql>
