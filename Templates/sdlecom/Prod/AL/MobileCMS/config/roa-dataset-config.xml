  <dataquery name="Incremental">
    <sql><![CDATA[
SELECT DISTINCT
	CASE WHEN LTRIM(RTRIM(CONVERT(VARCHAR(10), FILING_DT, 101))) = '' THEN '|' ELSE ISNULL(CONVERT(VARCHAR(10), FILING_DT, 101), '|') END AS 'FilingDate'
	,CASE WHEN LTRIM(RTRIM(PARTY)) = '' THEN '|' ELSE ISNULL(PARTY, '|') END AS 'Party'
