  <dataquery name="WorkingQueue" batch-size="128" is-skipping-null-parameters="false">
    <sql><![CDATA[
SELECT DISTINCT
    PS.CASE_NUMBER AS 'CaseNumber'
   ,PS.CASE_ID AS 'CaseID'
   ,EBP.STARTING_SEQUENCE AS 'Book'
   ,EBP.ENDING_SEQUENCE AS 'Page'
   ,PS.LAST_NAME AS 'LastName'
   ,PS.FIRST_NAME AS 'FirstName'
   ,PS.DISPLAYNAME AS 'DisplayName'
   ,DS.EVENT_ENTRY_ID AS 'EventEntryID'
   ,case when DS.EVENT_ENTRY_ID IS null then 'N' ELSE 'Y' END AS 'IDFlag'
   ,EET.EVENT_ENTRY_TYP_CD AS 'EventEntryTypeCode'
   ,EN.ADDITIONAL_COMMENTS  AS  'Comments'
   ,PS.DATE_TIME_MOD
FROM PARTYSEARCH PS
INNER JOIN ACM_EVENT_TO_CASE ETC ON ETC.CASE_ID = PS.CASE_ID
INNER JOIN ACM_EVENT_ENTRY EE ON ETC.EVENT_ENTRY_ID = EE.EVENT_ENTRY_ID
LEFT JOIN DOCUMENT_STORAGE DS ON EE.EVENT_ENTRY_ID = DS.EVENT_ENTRY_ID
LEFT JOIN ACM_EVENT_BOOK_PAGE EBP ON EBP.EVENT_ENTRY_ID = ETC.EVENT_ENTRY_ID
LEFT JOIN ACM_EVENT_ENTRY_NOTES EN ON EN.EVENT_ENTRY_ID = EE.EVENT_ENTRY_ID
LEFT JOIN ACM_EVENT_ENT_TYP EET ON EET.EVENT_ENTRY_TYP_ID = EE.EVENT_ENTRY_TYP_ID 
WHERE PS.CASE_TYP_CD IN ('WIT','FNG','CVA','ADM','TES')
AND PS.PARTY_ROLE_TYP_CD IN ('DEC')
AND PS.SEALED_FLAG <> 'Y'
AND PS.SEALED_CASE_FLAG <> 'Y'
AND PS.IS_RESTRICTED_FLG <> 'Y'
AND EBP.STARTING_SEQUENCE IS NOT NULL
OR PS.CASE_NUMBER IN ('20080140','20080141')
    ]]></sql>
