  <dataquery name="Incremental">
    <sql><![CDATA[
SELECT DISTINCT
	 VB.BOND_SERIAL_NUM AS 'BondNumber'
	,VB.BOND_ID AS 'BondID'
	,VB.SET_AMOUNT AS 'BondAmount'
	,VB.PARTY_NAME AS 'PartyName'
	,VB.CASE_ID AS 'CaseID'
	,VB.CASE_NUMBER AS 'CaseNumber'
	,VB.EVENT_ENT_TYP_DESC AS 'BondEvent'
	,VB.FILING_DT AS 'FilingDate'
	,VB.BONDSMAN AS 'Bondsman'
	,VB.UNDERWRITER_NAME AS 'Underwriter'
	,VB.BOND_TYP_DESCR AS 'BondType'
	,EB.REMARKS AS 'Comments'
FROM SW_ACM_V_GCI_BONDS VB
LEFT JOIN ACM_EVENT_TO_BOND EB ON VB.BOND_ID = EB.BOND_ID
WHERE VB.FILING_DT > (GETDATE() - 30)
    ]]></sql>
  </dataquery>
