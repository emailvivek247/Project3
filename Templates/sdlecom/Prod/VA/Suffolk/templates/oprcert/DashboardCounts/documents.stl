
<#if (searchResult.docs?has_content && searchResult.docs.size() > 0) >

 <#foreach doc in searchResult.docs>
   <p style="border-bottom:1px solid #DDDDDD;">
   <a target="_blank" href="http://www.sample.com/id=">
    </a><br />
   <font size="-1"></font>
     <br/><b>DOCUMENTCOUNT</b>:
${number.format("###0.##",doc.get("DOCUMENTCOUNT"))}
          <br/><b>Year</b>:
${number.format("###0.##",doc.get("Year"))}
          <br/><b>Month</b>:
${number.format("###0.##",doc.get("Month"))}
          <br/><b>DOC_TYPE</b>:
${searchResult.highlight(doc, "DOC_TYPE")}
 <br>&nbsp;
 </#foreach>

<#else>
 <#include "noResults.stl"> 
</#if>

