    <style type="text/css">
      /* HOVER STYLES */
        div#pop-up {
        display: none;
        position: absolute;
        width: 280px;
        padding: 10px;
        background: #fff;
        color: #000000;
        border: 3px solid #7EB544;
        font-size: 90%;
        z-index: 1;
        float: right;
      }
    </style>
    <script type="text/javascript">
      $(function() {
        var moveLeft = -10;
        var moveDown = -125;
        
        $('a#trigger').hover(function(e) {
          $('div#pop-up').show();
          //.css('top', e.pageY + moveDown)
          //.css('left', e.pageX + moveLeft)
          //.appendTo('body');
        }, function() {
          $('div#pop-up').hide();
        });
        
        $('a#trigger').mousemove(function(e) {
          $("div#pop-up").css('top', e.pageY + moveDown).css('left', e.pageX + moveLeft);
        });
        
      });
    </script>




