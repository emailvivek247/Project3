    <sql><![CDATA[
SELECT DISTINCT
         SERVER AS 'Server'
        ,PATH AS 'Path'
        ,BOOKTYPE AS 'Booktype'
        ,BOOKNAME AS 'BookName'
        ,INDEXNAME AS 'IndexName'
        ,PAGE AS 'Book'
	,LEFT(Image,LEN(Image) -4) AS 'Page'       
        ,LTRIM(RTRIM(IMAGE)) as 'Image'
        ,ID
        ,'\\' + SERVER + '\' + PATH + '\' + BOOKTYPE + '\' + BOOKNAME + '\' + INDEXNAME + '\' + PAGE + '\' + IMAGE  AS 'ImagePath' 
FROM IMAGE_LOC_HIST
