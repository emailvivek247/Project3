<#if (searchResult.docs?has_content && searchResult.docs.size() > 0) >
<#assign ResultId =0>
 <#foreach doc in searchResult.docs>
     <p class="result" style="border-bottom:1px solid #efefef;">
      <br/><b>${searchResult.datasetConfiguration.getColumn("RPCNum").displayName}</b>:${searchResult.highlight(doc, "RPCNum")}
      <br/><b>${searchResult.datasetConfiguration.getColumn("Address").displayName}</b>:${searchResult.highlight(doc, "Address")}
      <br/><b>${searchResult.datasetConfiguration.getColumn("LegalDesc").displayName}</b>:${searchResult.highlight(doc, "LegalDesc")}
      <br/><b>${searchResult.datasetConfiguration.getColumn("Status").displayName}</b>:${searchResult.highlight(doc, "Status")}

<br/><a class="nocolor" href="search.do?indexName=arlimages&lq=${pageStyleUtil.encodeURL("PKKEY:" + doc.get("PKKEY"))}" target="_blank">View Image</a>

<br/>&nbsp;
 </#foreach>


<#else>
 <#include "noResults.stl"> 
</#if>



<script type="text/javascript">
$(document).ready(function() {
	oTable = $('#doc').dataTable({
		"bJQueryUI": true,
		"bPaginate": false,
		"bLengthChange": false,
		"bFilter": false,
		"bSort": true,
		"bInfo": false,
		"bAutoWidth": false,
		"aaSorting": []
	});
} );
</script>