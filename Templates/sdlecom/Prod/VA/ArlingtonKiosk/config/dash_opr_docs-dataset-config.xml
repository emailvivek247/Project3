  <dataquery name="WorkingQueue" batch-size="128" is-skipping-null-parameters="false">
    <sql><![CDATA[
SELECT 
	  DATEPART ( YYYY , RECDATE1 ) as Year, 
	  DATEPART ( MM , RECDATE1 ) as Month, 
	  LTRIM(RTRIM(DOC_TYPE)) AS DOC_TYPE, 
	  COUNT(SEQ_KEY) AS DOCUMENTCOUNT
FROM DOCUMENT
WHERE DOCTYPE not in (104,105,107)
and flag4='N' and void='N' and deleterecord='N'
GROUP BY DATEPART ( YYYY , RECDATE1 ), DATEPART ( MM , RECDATE1 ), DOC_TYPE
ORDER BY DATEPART ( YYYY , RECDATE1 ), DATEPART ( MM , RECDATE1 ), DOC_TYPE
    ]]></sql>
