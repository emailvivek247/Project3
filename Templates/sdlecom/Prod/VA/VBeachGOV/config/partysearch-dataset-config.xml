  <dataquery name="Deletion" is-delete-only="true">
    <sql><![CDATA[
SELECT RT.TAB_PK_KEY
FROM ROAM_TRACK RT
WHERE 
RT.DATE_TIME_MOD > DATEADD(mi, -10, GETDATE()) AND RT.TAB_NAME = 'ACM_PARTY' AND
RT.DML_TYP IN ('D', 'U') 
UNION ALL
SELECT PARTY_ID
FROM ACM_V_ESPARTYSEARCH ES INNER JOIN ROAM_TRACK RT
ON ES.CASE_ID = RT.PK_KEY
WHERE 
RT.DATE_TIME_MOD > DATEADD(mi, -10, GETDATE()) AND 
RT.DML_TYP IN ('D', 'U') AND 
((ES.CASE_SEALED = 'Y' OR ES.PERSON_SEALED = 'Y' AND COURT_CD IN ('CRIMINAL','CIVIL')) OR CASE_VOID = 'Y' OR PARTY_VOID = 'Y')
    ]]></sql>
  </dataquery>
  <dataquery name="Incremental">
    <sql><![CDATA[
SELECT DISTINCT
CASE_NUMBER AS 'CaseNumber',
PARTY_ID AS 'PartyID',
CASE_ID AS 'CaseID',
LAST_NAME AS 'LastName',
FIRST_NAME AS 'FirstName',
MIDDLE_NAME AS 'MiddleName',
SUFFIX_NAME AS 'SuffixName',
DISPLAYNAME AS 'DisplayName',
DISPLAYNAME AS 'DisplayNameSort',
CONVERT(varchar(10), DOB, 101) AS 'DateofBirth',
HAIR_COLOR_DESC AS 'HairColor',
EYE_COLOR_DESC AS 'EyeColor',
HEIGHT AS 'Height',
WEIGHT AS 'Weight',
SSN AS 'SSN',
SPN AS 'SPN',
SEX AS 'Sex',
LICENSE_NUM AS 'DriverLicense',
ADDRESS_LINE1_TXT AS 'AddressLine1',
ADDRESS_LINE2_TXT AS 'AddressLine2',
CITY_NAME AS 'City',
STATE_NAME AS 'State',
POSTAL_CD AS 'Zip',
STREET_ADDRESS AS 'DisplayAddress',
PERSON_SEALED AS 'PersonSealed',
PARTY_RESTRICTED AS 'PartyRestricted',
RT.DATE_TIME_MOD
FROM ACM_V_ESPARTYSEARCH PS INNER JOIN ROAM_TRACK RT ON PS.CASE_ID = RT.PK_KEY
WHERE (CASE_SEALED = 'N' AND PERSON_SEALED = 'N' AND PARTY_RESTRICTED = 'N')
AND CASE_VOID = 'N'
AND PARTY_VOID = 'N'
AND PARTY_ROLE_DESC IN ('PLAINTIFF', 'DEFENDANT')
AND COURT_CD IN ('CRIMINAL', 'CIVIL')
AND RT.DML_TYP IN ('I', 'U')
AND RT.DATE_TIME_MOD > ? 
ORDER BY RT.DATE_TIME_MOD DESC
    ]]></sql>
  </dataquery>
