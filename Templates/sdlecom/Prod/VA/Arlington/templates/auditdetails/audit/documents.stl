<#assign auditCounter = 0>

<#if searchResult.docs?has_content>
    <#foreach doc in searchResult.docs> 
    <#foreach Instrument in doc.getValues("Instrument")>
       <#assign auditCounter = 1>    
       <#break>
    </#foreach>
    </#foreach>
</#if>

<#if searchResult.docs?has_content>
	<#if (auditCounter > 0)>

	<b><font size="3px" color="#000000" >Document Notations</font></b>
	<table id="audit" class="tablesorter">
			   <thead>
					<tr>
					  <th class="ui-state-default">${searchResult.datasetConfiguration.getColumn("AuditDate").displayName}</th>
					  <th class="ui-state-default">${searchResult.datasetConfiguration.getColumn("AuditRemarks").displayName}</th>
					</tr>
				</thead>
			 <tbody>

	<#foreach doc in searchResult.docs>
			 <#assign Instrument = doc.getValues("Instrument") 
					  AuditDate = doc.getValues("AuditDate") 
					  AuditRemarks = doc.getValues("AuditRemarks") 
					  Attribute = doc.getValues("Attribute") 
					  AttributeOldValue = doc.getValues("AttributeOldValue")
					  AttributeNewValue = doc.getValues("AttributeNewValue") 
					  loopCounter = 0>
					  
			 <#foreach Instrument in doc.getValues("Instrument")>
				   <tr>
					  <td>${AuditDate[loopCounter]?replace('|', '&nbsp;')}</td>
					  <td>${AuditRemarks[loopCounter]?replace('|', '&nbsp;')}</td>
					  <#assign loopCounter = loopCounter + 1>
				  </tr>
			 </#foreach>
	 </#foreach>
	 
     </#if>
</#if>



</tbody>
      </table>

<script type="text/javascript">
$(document).ready(function() {

    oTable = $('#audit').dataTable({
        "bJQueryUI": true,
        "bPaginate": false,
        "bLengthChange": false,
        "bFilter": false,
        "bSort": true,
        "bInfo": false,
        "bAutoWidth": false,
        "aaSorting": []
    });
    

} );
</script>