<body class="page">

	<!-- Begin template wrapper -->
	<div id="wrapper">
	<#include "nav.stl" />
<!-- Begin content -->
<div id="content_wrapper">
	<br class="clear" id="logo-break"/>
	<div class="inner">
		<div class="inner_wrapper">

<div class="content">
		<div style="width: 500px; margin-left: auto;margin-right:auto;margin-top:50px;">
			<div id="ADCSI" class="ADCSI" style="margin-top: 0px; margin-bottom: 12px;">
			<div class="ADCSI_title" id="ADCSI_title">
			 <span style="font-size: 11px;"><a href="publicLogin.admin"><@spring.message code="security.authentication.loginHeader" /></a>&nbsp;&nbsp; or &nbsp;&nbsp; <a href="publicSignUp.admin"><@spring.message code="security.authentication.createHeader" /></a></span>
			</div>				
			    <br />
			    <div style="text-align: center;padding-top: 10px;padding-bottom: 20px;">
					<span style="display:none;"></span>
					<h2><@spring.message code="security.authentication.resendActivationText" /></h2><@spring.message code="security.authentication.resendActivationHeader" />
				</div>		    			    
				<@form.form id="resendUserActivation-form" name="resendUserActivation" method="post" action="publicResendUserActivationEmail.admin" modelAttribute="UserForm" autocomplete="on">
				<div class="ADCSI_row" id="emailInput">
				<span class="ADCSI_leftcol" style="width:110px;"><@spring.message code="security.authentication.username" /></span>
				<span class="ADCSI_rightcol" style="width: 330px; display: inline-block; ">
					<span>
						<@form.input path="username" class="validate[required,custom[email],minSize[6],maxSize[50]] ADCSI_Input" spellcheck="false" tabindex="1" maxlength="50" />
						<@form.errors path="username" cssClass="sverror hidden" />
					</span>
				</span>
				</div>
				<div style="text-align: center;padding-top: 30px;padding-bottom: 10px;">
					<span style="display:none;"></span>
						<input type="submit" name="resendUserActivation" id="resendUserActivation" value="<@spring.message code="security.authentication.resendActivationText" />" tabindex="2" />						
				</div>		    		
		   		</@form.form>			
		</div>
	</div>


<script type="text/javascript">
		$(document).ready(function(){
			$("input:submit").button(); 
			//$('#resendUserActivation-form').validationEngine();	
			jQuery('.sverror').each(function() {
			    var currentElement = '#' + $(this).attr('id').split('.')[0];	     
		        $(currentElement).validationEngine('showPrompt', $(this).html(), 'fail', 'topRight', true);
			});	
		});
</script>

		</div>
	</div>
</div>
