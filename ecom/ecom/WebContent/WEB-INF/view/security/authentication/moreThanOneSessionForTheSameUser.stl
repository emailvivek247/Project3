<#include "/common/include.stl" />
<@loginLayout encoding="utf-8">
<link rel="stylesheet" href="${request.getContextPath()}/resources/styles/login.css" />
<style>
div.ADCSI {
    width: 550px;
}
span.ADCSI_leftcol {
    width: 150px;
    text-align:right;
    display: inline-block;
}
</style>
<div class="content">
	<div style="width: 500px; margin-left: auto;margin-right:auto;margin-top:100px;">
		<div id="ADCSI" class="ADCSI" style="margin-top: 0px; margin-bottom: 12px;">
		<div class="ADCSI_title" id="ADCSI_title">
			 <span style="font-size: 11px;"><a href="publicLogin.admin"><@spring.message code="security.authentication.loginHeader" /></a>&nbsp;&nbsp; or &nbsp;&nbsp; <a href="publicSignUp.admin"><@spring.message code="security.authentication.createHeader" /></a></span>
		</div>	
		<div style="text-align: center;padding-top: 10px;padding-bottom: 20px;">
			<span style="display:none;"></span>
				<@spring.message code="security.authentication.moreThanOneSessionForTheSameUser" />
		</div>		    			
	</div>
</div>
</@loginLayout>
